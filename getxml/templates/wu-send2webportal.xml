<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <FileTransfer.send2webportal
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/filetransfer/send2webportal.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Send to Webportal"
        NODENAMES=""
        PROJECT="FileTransfer"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-send2webportal.xml,v 1.2 2009/05/20 15:28:10 sek Exp $"
        >
        <DEFAULTS
          KEEP_FILES="N"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT>
          <BASE_URL
            MANDATORY="true"
            TYPE="TEXT"
            />
          <FILE_LIST
            MANDATORY="true"
            TYPE="TEXT"
            />
          <KEEP_FILES
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </KEEP_FILES>
          <MAIL_FROM
            MANDATORY="true"
            TYPE="TEXT"
            />
          <MAIL_SMTP
            MANDATORY="true"
            TYPE="TEXT"
            />
          <MAIL_SUBJECT
            TYPE="TEXT"
            />
          <MAIL_TEXT
            TYPE="TEXT"
            />
          <MAIL_TO
            MANDATORY="true"
            TYPE="TEXT"
            />
          <PORTAL_DIR
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WU_ENABLE_DEBUG_LOOP>
        </INPUT>
        <OUTPUT/>
      </FileTransfer.send2webportal>
    </workingunits>
  </config>
</DPF>
