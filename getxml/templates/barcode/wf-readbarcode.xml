          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </USER_EXIST1>
        <USER_EXIST2
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1300"
          POSY="200"
          TYPE="USER_EXIST"
          USE="%Barcode.UserExitAfterReadBarcode%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
