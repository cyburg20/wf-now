<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Barcode.readbarcode
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Readbarcode"
        PROJECT="Barcode"
        VERSION="$Id: wf-readbarcode.xml,v 1.6 2012/05/24 14:10:29 dei Exp $"
        > 
        <Barcode.readbarcode1
          COMMENT=""
          ERROR="END"
          NAME="Barcode.readbarcode"
          NEXT="IF-THEN-ELSE1"
          POSX="700"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Barcode.readbarcode1>
        <DEFAULTS
          Barcode.UserExitAfterReadBarcode="Barcode.UserExitAfterReadBarcode"
          Barcode.UserExitBeforeReadBarcode="Barcode.UserExitBeforeReadBarcode"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <FileSystem.readIniFile1
          COMMENT=""
          ERROR="END"
          NAME="FileSystem.readIniFile"
          NEXT="USER_EXIST1"
          POSX="300"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <FORCE_INI_VALUES
              val="N"
              />
            <ReturnQuotes
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FORCE_INI_VALUES="Y"
            INIFILENAME="%SEAL_CUSTOMDIR%/server/dpf/conf/barcode/readbarcode.ini"
            />
        </FileSystem.readIniFile1>
        <FileTransfer.sendback1
          COMMENT=""
          ERROR="END"
          NAME="sendback"
          NEXT="USER_EXIST2"
          POSX="1100"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            SEND_FILE_PROPERTIES=""
            SOURCE="%BC_RESULT%"
            TARGETFILE="%BC_RESULT_FILE%"
            >
            <IOR
              val="%ORIGFILESERVERIOR%"
              />
            <OVERWRITE
              val="Y"
              />
            <SOURCE
              val="%CURRFILENAME%"
              />
            <TARGETFILE
              val="%TARGETFILENAME%"
              />
          </DEFAULTS>
          <MAPPING
            SEND_FILE_PROPERTIES=""
            SOURCE="%BC_RESULT%"
            TARGETFILE="%BC_RESULT_FILE%"
            />
        </FileTransfer.sendback1>
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="USER_EXIST2"
          OPERATOR="exists"
          POSX="900"
          POSY="200"
          PROPERTY="BC_RESULT_FILE"
          THEN="FileTransfer.sendback1"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <IF-THEN-ELSE2
          COMMENT=""
          ELSE="USER_EXIST1"
          OPERATOR="is file"
          POSX="100"
          POSY="200"
          PROPERTY="%SEAL_CUSTOMDIR%/server/dpf/conf/barcode/readbarcode.ini"
          THEN="FileSystem.readIniFile1"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE2>
        <MAIN
          BEGIN="IF-THEN-ELSE2"
          NAME=""
          />
        <MAPPING/>
        <USER_EXIST1
          COMMENT=""
          ERROR="END"
          NEXT="Barcode.readbarcode1"
          POSX="500"
          POSY="200"
          TYPE="USER_EXIST"
          USE="%Barcode.UserExitBeforeReadBarcode%"
