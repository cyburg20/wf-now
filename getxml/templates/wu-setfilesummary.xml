<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <setfilesummary
        VERSION="$Id: wu-setfilesummary.xml,v 1.2 2003/03/13 11:50:43 wolfi Exp $"
        PROJECT="DPF"
        TYPE="WUSystemCall"
        LOGNAME="dpfsetfilesummary"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="N"
        THREADS="1"
        IGNOREEXITCODE="N"
        COMMAND="%DPFROOT%/tools/bin_winnt4/readprop -w %SUMMARY_OPTIONS% -f &quot;%CURRFILENAME%&quot;">
        <DEFAULTS
          SUMMARY_OPTIONS=""
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <ENVIRONMENTS
          />
        <INPUT
          CURRFILENAME="*"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="PDF"
          />
      </setfilesummary>
    </workingunits>
  </config>
</DPF>
