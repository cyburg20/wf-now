<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Rli.convertFiles
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/rli/convertfiles.pl -parfile %PARFILENAME%"
        DPFINT-FileLinks="doc://:/perl"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Convert files of RLI file with dpf4convert"
        NODENAMES=""
        PROJECT="Rli"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-convertfiles.xml,v 1.11 2009/05/13 15:05:32 sek Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          ExitOnMissingSubJobs="Y"
          WuEnableDebugLoop="N"
          />
        <INPUT>
          <CurrFileName
            TYPE="TEXT"
            />
          <DpfDebugSubJobs
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </DpfDebugSubJobs>
          <ExitOnMissingSubJobs
            DEFAULT="Y"
            DESCRIPTION="error if there are no jobs to convert"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </ExitOnMissingSubJobs>
          <RlicBaseName
            TYPE="TEXT"
            />
          <RlicCurrFileName
            TYPE="TEXT"
            />
          <RlicJobName
            TYPE="TEXT"
            />
          <RlicTargetFileName
            TYPE="TEXT"
            />
          <WuEnableDebugLoop
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WuEnableDebugLoop>
        </INPUT>
        <OUTPUT/>
      </Rli.convertFiles>
    </workingunits>
  </config>
</DPF>
