<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Rli.setPdfBookmarks
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/rli/setpdfbookmarks.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Set PDF Bookmarks"
        NODENAMES=""
        PROJECT="Rli"
        THREADS="2"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-setpdfbookmarks.xml,v 1.2 2008/12/12 15:09:54 stefan Exp $"
        >
        <DEFAULTS
          CreateClosedBookmarks="N"
          DeleteOldBookmarks="Y"
          WuEnableDebugLoop="N"
          />
        <INPUT>
          <CreateClosedBookmarks
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </CreateClosedBookmarks>
          <CurrRliFileName
            TYPE="TEXT"
            />
          <DeleteOldBookmarks
            DEFAULT="Y"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </DeleteOldBookmarks>
          <FilterOperator
            TYPE="TEXT"
            />
          <FilterValue
            TYPE="TEXT"
            />
          <RlicBookMarkFileName
            DESCRIPTION="RLI column with given bookmark files"
            TYPE="TEXT"
            />
          <RlicFilter
            TYPE="TEXT"
            />
          <WuEnableDebugLoop
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WuEnableDebugLoop>
        </INPUT>
        <OUTPUT/>
      </Rli.setPdfBookmarks>
    </workingunits>
  </config>
</DPF>
