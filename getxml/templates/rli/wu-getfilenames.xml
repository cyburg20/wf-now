<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Rli.getFileNames
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/rli/getfilenames.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Get all file names from RLI file"
        NODENAMES=""
        PROJECT="Rli"
        THREADS="5"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-getfilenames.xml,v 1.12 2009/08/27 16:39:30 sek Exp $"
        >
        <DEFAULTS/>
        <INPUT
          DPFINT-Icon="doc://:/wu-input.gif"
          >
          <CurrRliFileName
            TYPE="TEXT"
            />
          <FilterOperator
            DESCRIPTION="optional operator to compare value(s), default: eq"
            TYPE="TEXT"
            />
          <FilterOperator2
            DESCRIPTION="second optional operator to compare value(s), default: eq"
            TYPE="TEXT"
            />
          <FilterValue
            DESCRIPTION="value to search for in filter"
            TYPE="TEXT"
            />
          <FilterValue2
            DESCRIPTION="second set of colon separated values to work on these specific RLI lines only"
            TYPE="TEXT"
            />
          <RlicFileName
            TYPE="TEXT"
            />
          <RlicFilter
            DESCRIPTION="name of RLI column for filter"
            TYPE="TEXT"
            />
          <RlicFilter2
            DESCRIPTION="name of second RLI column to use to filter specific RLI lines only"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </Rli.getFileNames>
    </workingunits>
  </config>
</DPF>
