<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Rli.setRliSections
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/rli/setrlisections.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Set Rli Sections"
        NODENAMES=""
        PROJECT="Rli"
        THREADS="5"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-setrlisections.xml,v 1.3 2010/10/28 15:57:08 stefan Exp $"
        >
        <INPUT>
          <CurrRliFileName
            MANDATORY="true"
            TYPE="TEXT"
            />
          <Parameters
            MANDATORY="true"
            TYPE="TEXT"
            />
          <SectionName
            DEFAULT="default"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </Rli.setRliSections>
    </workingunits>
  </config>
</DPF>
