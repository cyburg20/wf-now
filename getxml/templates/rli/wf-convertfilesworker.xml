<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Rli.convertFilesWorker
        COMMENT="Customizalbe sub workflow per file in RLI, default: dpf4convert is used for conversion"
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Type="WorkflowDescription"
        INTERNALNAME="Rli.convertFilesWorker"
        NAME="convertFilesWorker"
        PROJECT="Rli"
        VERSION="$Id: wf-convertfilesworker.xml,v 1.1 2006/07/10 12:41:32 stefan Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <MAIN
          BEGIN="initDpf4Convert"
          NAME=""
          />
        <MAPPING/>
        <callDpf4convert
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="200"
          POSY="300"
          TYPE="SWF"
          USE="dpf4convert"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </callDpf4convert>
        <initDpf4Convert
          COMMENT=""
          NEXT="callDpf4convert"
          POSX="200"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_PLM_SYSTEM_NAME="no_plm_system"
            DPF4C_SWITCH_CHECKIN="N"
            DPF4C_SWITCH_CREATE_DESTFORMATS="Y"
            DPF4C_SWITCH_GET_FILETYPE="N"
            />
        </initDpf4Convert>
      </Rli.convertFilesWorker>
    </workflows>
  </config>
</DPF>
