<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Rli.writeBookmarkFile
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/rli/writebookmarkfile.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Write Bookmark File"
        NODENAMES=""
        PROJECT="Rli"
        THREADS="3"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-writebookmarkfile.xml,v 1.2 2010/10/28 15:57:09 stefan Exp $"
        >
        <INPUT>
          <BOOKMARK_DEPTH_OFFSET
            TYPE="TEXT"
            />
          <BookmarkDepthOffset
            TYPE="TEXT"
            />
          <CurrRliFileName
            DESCRIPTION="input RLI"
            TYPE="TEXT"
            />
          <DestFileName
            DESCRIPTION="name of bookmark file"
            TYPE="TEXT"
            />
          <FilterOperator
            TYPE="TEXT"
            />
          <FilterValue
            TYPE="TEXT"
            />
          <RlicBookmarkColor
            DESCRIPTION="RLI column with color information, #ff00ff"
            TYPE="TEXT"
            />
          <RlicBookmarkLevel
            DESCRIPTION="name of RLI column with bookmark level in it. default ChapterDepth"
            TYPE="TEXT"
            />
          <RlicBookmarkOpen
            DESCRIPTION="RLI column with flag if bookmark should be open or closed"
            TYPE="TEXT"
            />
          <RlicBookmarkStyle
            DESCRIPTION="RLI column name with style, eg. normal, italic-bold, italic, bold"
            TYPE="TEXT"
            />
          <RlicBookmarkText
            DESCRIPTION="name of RLI column with bookmark text"
            TYPE="TEXT"
            />
          <RlicFileName
            TYPE="TEXT"
            />
          <RlicFilter
            TYPE="TEXT"
            />
          <WuEnableDebugLoop
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WuEnableDebugLoop>
        </INPUT>
        <OUTPUT/>
      </Rli.writeBookmarkFile>
    </workingunits>
  </config>
</DPF>
