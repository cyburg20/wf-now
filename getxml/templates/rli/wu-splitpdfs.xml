<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Rli.splitPdfs
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/rli/splitpdfs.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Split PDF files into smaller parts"
        NODENAMES=""
        PROJECT="Rli"
        THREADS="2"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-splitpdfs.xml,v 1.6 2010/11/08 08:30:49 stefan Exp $"
        >
        <DEFAULTS
          HideSplittedPdfsInToc="N"
          IgnoreErrors="N"
          />
        <INPUT>
          <FilterOperator
            TYPE="TEXT"
            />
          <FilterOperator2
            TYPE="TEXT"
            />
          <FilterPrefix
            TYPE="TEXT"
            />
          <FilterValue
            TYPE="TEXT"
            />
          <FilterValue2
            TYPE="TEXT"
            />
          <FormatThicknessFactor_ANSI_A
            TYPE="TEXT"
            />
          <FormatThicknessFactor_DIN_A4
            TYPE="TEXT"
            />
          <HideSplittedPdfsInToc
            DEFAULT="N"
            DESCRIPTION="hide following splitted PDF&apos;s in TOC, show only first part"
            TYPE="TEXT"
            />
          <IgnoreErrors
            DEFAULT="N"
            DESCRIPTION="ignore errors, just write RLI column ERR_REASON"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IgnoreErrors>
          <PdfPraeTolerance
            TYPE="TEXT"
            />
          <RlicFilter
            TYPE="TEXT"
            />
          <RlicFilter2
            TYPE="TEXT"
            />
          <RlicSplitOptions
            DESCRIPTION="optional column with split options eg. page ranges"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </Rli.splitPdfs>
    </workingunits>
  </config>
</DPF>
