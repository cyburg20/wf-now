<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4VIEW.StampAndConvert
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Stamp And Convert"
        PROJECT="DPF4VIEW"
        VERSION="$Id: wf-stampandconvert.xml,v 1.2 2012/03/16 13:26:07 sek Exp $"
        >
        <Convert2Pdf
          COMMENT=""
          ERROR="END"
          NEXT="IfWantSameStampOnAllFiles"
          POSX="600"
          POSY="200"
          TYPE="SWF"
          USE="DPF4CONVERT.DPF4Convert_Convert2PDF"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Convert2Pdf>
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <IfNonPdfDetected
          COMMENT=""
          ELSE="IfWantSameStampOnAllFiles"
          OPERATOR="is true"
          POSX="400"
          POSY="200"
          PROPERTY="NonPdfDetected"
          THEN="Convert2Pdf"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IfNonPdfDetected>
        <IfWantSameStampOnAllFiles
          COMMENT=""
          ELSE="StampPdf"
          OPERATOR="is true"
          POSX="400"
          POSY="400"
          PROPERTY="WantSameStampOnAllFiles"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IfWantSameStampOnAllFiles>
        <MAIN
          BEGIN="IfNonPdfDetected"
          NAME=""
          />
        <MAPPING/>
        <StampPdf
          COMMENT=""
          ERROR="END"
          NAME="Pdf.stamp"
          NEXT="END"
          POSX="600"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            PDFSTAMP_DB_FILENAME="%DIFFERENT_PDFSTAMP_DB_FILENAME%"
            PDFSTAMP_STP_FILENAME="%DIFFERENT_PDFSTAMP_STP_FILENAME%"
            />
        </StampPdf>
      </DPF4VIEW.StampAndConvert>
    </workflows>
  </config>
</DPF>
