          NEXT="set_custom_config"
          POSX="300"
          POSY="100"
          TYPE="SWF"
          USE="DPF4VIEW.version_dpf4v"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4VIEW.version_dpf4v1>
