          POSX="700"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4VIEW.dpf4v_readcustomconfig1>
        <DPF4VIEW.dpf4view_user_pre_conv1
          COMMENT=""
          ERROR="END"
