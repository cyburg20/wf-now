<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Pdf.DecompressXrefTable
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Decompress Xref Table"
        PROJECT="Pdf"
        VERSION="$Id: wf-decompressxreftable.xml,v 1.1 2012/12/06 13:05:01 sek Exp $"
        >
        <BackupSourceFileName
          COMMENT=""
          ERROR="END"
          NAME="FileTransfer.copyFiles"
          NEXT="DecompressXrefTable"
          POSX="400"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <COPY_DEBUG
              val="N"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            COPY_TARGET_PROPERTIES="BackupFileName"
            />
        </BackupSourceFileName>
        <DEFAULTS/>
        <DecompressXrefTable
          COMMENT=""
          ERROR="END"
          NAME="Pdf.DecompressXrefTable"
          NEXT="OverWriteSourceWithDecompressedFile"
          POSX="600"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DecompressXrefTable>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="SetBackupFileName"
          NAME=""
          />
        <MAPPING/>
        <OverWriteSourceWithDecompressedFile
          COMMENT=""
          ERROR="END"
          NAME="FileTransfer.copyFiles"
          NEXT="END"
          POSX="800"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <COPY_DEBUG
              val="N"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            COPY_TARGET_PROPERTIES="SourceFileName"
            />
        </OverWriteSourceWithDecompressedFile>
        <SetBackupFileName
          COMMENT=""
          NEXT="BackupSourceFileName"
          POSX="200"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            BackupFileName="%SourceFileName%.BAK"
            />
        </SetBackupFileName>
      </Pdf.DecompressXrefTable>
    </workflows>
  </config>
</DPF>
