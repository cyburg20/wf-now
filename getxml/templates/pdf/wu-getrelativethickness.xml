<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Pdf.getRelativeThickness
        COMMAND="%DPFSRV%/scripts/pdf/getrelativethickness.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Get relative thickness"
        PROJECT="Pdf"
        Status="Running"
        THREADS="4"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-getrelativethickness.xml,v 1.2 2007/08/07 13:33:03 stefan Exp $"
        >
        <DEFAULTS/>
        <INPUT>
          <PLS_DUPLEX
            DESCRIPTION="flag if PDF document should be interpreted with duplex on"
            TYPE="TEXT"
            />
          <PageInfoFileName
            DESCRIPTION="name of page info file, created by Pdf.pdfprae"
            TYPE="TEXT"
            />
        </INPUT>
      </Pdf.getRelativeThickness>
    </workingunits>
  </config>
</DPF>
