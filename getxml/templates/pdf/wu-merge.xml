<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Pdf.merge
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/pdf/merge.pl -parfile %PARFILENAME%"
        DPFINT-FileLinks="doc://:/%DPFSRV%/scripts/pdf/merge.pl"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Merge PDF files"
        LICENSE="CMULTI"
        NODENAMES=""
        PROJECT="Pdf"
        THREADS="5"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-merge.xml,v 1.7 2017/02/08 12:47:16 thomas Exp $"
        >
        <DEFAULTS
          CreateErrorSheet="N"
          DPFINT-Icon="doc://:/defaults.gif"
          PDFMERGE_MAKE_TAR="N"
          PdfMergeBookmarkFiles="N"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          CURRFILENAME000001="*"
          CURRFILENAME00000N="*"
          DPFINT-Icon="doc://:/wu-input.gif"
          WU_ENABLE_DEBUG_LOOP="*"
          >
          <CURRFILENAME000001
            TYPE="TEXT"
            />
          <CURRFILENAME00000N
            TYPE="TEXT"
            />
          <CreateErrorSheet
            DEFAULT="N"
            DESCRIPTION="create a sheet with all merge error and continue the wf on merge errors"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </CreateErrorSheet>
          <JOBDIR
            TYPE="TEXT"
            />
          <PDFMERGE_MAKE_TAR
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </PDFMERGE_MAKE_TAR>
          <PDFMERGE_TARGETFILE
            TYPE="TEXT"
            />
          <PdfMergeBookmarkFileName
            DESCRIPTION="filename of a cfg file with bookmark information per file to modify bookmark text and color"
            TYPE="TEXT"
            />
          <PdfMergeBookmarkFiles
            DEFAULT="N"
            DESCRIPTION="if Y, add bookmarks for each file (filename), default: N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </PdfMergeBookmarkFiles>
          <PdfMergeListFileName
            DESCRIPTION="optional file with filenames for merge, no CURRFILENAME000001 parameters needed then"
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WU_ENABLE_DEBUG_LOOP>
        </INPUT>
        <OUTPUT/>
      </Pdf.merge>
    </workingunits>
  </config>
</DPF>
