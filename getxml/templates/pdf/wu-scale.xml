<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Pdf.scale
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/pdf/scale.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Scale a PDF to a certain page size"
        NODENAMES=""
        PROJECT="Pdf"
        THREADS="4"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-scale.xml,v 1.1 2016/03/03 15:10:22 kig Exp $"
        >
        <DEFAULTS/>
        <INPUT>
          <PDFSCALE_CONFIG
            TYPE="TEXT"
            />
          <PDFSCALE_ORIENTATION
            TYPE="TEXT"
            />
          <PDFSCALE_PAGESIZE
            TYPE="TEXT"
            />
          <PDFSCALE_ROTATE
            TYPE="TEXT"
            />
          <PDFSCALE_SCALETYPE
            TYPE="TEXT"
            />
          <PDFSCALE_TOLERANCE
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </Pdf.scale>
    </workingunits>
  </config>
</DPF>
