<DPF>
  <Scenarios>
    <SCENARIO_JOBS_DETAILED_ID
      name="SCENARIO_JOBS_DETAILED_ID"
      >
      <Window1
        height="300"
        module="JOBS_MOD_ID"
        width="1000"
        workingset="WS_MY_JOBS"
        x="0"
        y="0"
        />
      <Window2
        height="300"
        module="JOBS_MOD_ID"
        width="1000"
        workingset="WS_JOBS_IN_PROCESS"
        x="0"
        y="300"
        />
      <Window3
        height="300"
        module="JOBS_MOD_ID"
        width="1000"
        workingset="WS_DEFECTIVE_JOBS"
        x="0"
        y="600"
        />
    </SCENARIO_JOBS_DETAILED_ID>
    <SCENARIO_WUS_DETAILED_ID
      name="SCENARIO_WUS_DETAILED_ID"
      >
      <Window1
        height="300"
        module="SYSTEM_MOD_ID"
        width="1000"
        workingset="WS_DEFECTIVE_WUS"
        x="0"
        y="0"
        />
      <Window2
        height="300"
        module="SYSTEM_MOD_ID"
        width="1000"
        workingset="WS_RUNNING_WUS"
        x="0"
        y="300"
        />
      <Window3
        height="300"
        module="SYSTEM_MOD_ID"
        width="1000"
        workingset="WS_STOPPED_WUS"
        x="0"
        y="600"
        />
    </SCENARIO_WUS_DETAILED_ID>
    <STARTSCENARIO_ID
      name="STARTSCENARIO_ID"
      >
      <Window1
        height="500"
        module="JOBS_MOD_ID"
        width="800"
        workingset="WS_ALL_JOBS"
        x="0"
        y="0"
        />
      <Window2
        height="500"
        module="WORKFLOWS_MOD_ID"
        width="800"
        workingset="WS_ALL_WORKFLOWS"
        x="50"
        y="50"
        />
      <Window3
        height="500"
        module="SYSTEM_MOD_ID"
        width="800"
        workingset="WS_ALL_WUS"
        x="100"
        y="100"
        />
    </STARTSCENARIO_ID>
  </Scenarios>
  <WorkingSets>
    <LOGS_MOD_ID>
      <__________
        name="__________"
        />
      <dpfclient.log
        name="dpfclient.log"
        />
      <dpfserver.log
        name="dpfserver.log"
        />
    </LOGS_MOD_ID>
    <SESSIONS_MOD_ID>
      <WS_ALL_SESSIONS
        name="WS_ALL_SESSIONS"
        />
    </SESSIONS_MOD_ID>
    <SYSTEM_MOD_ID>
      <WS_ALL_WUS
        changeable="N"
        name="WS_ALL_WUS"
        >
        <Columns>
          <Column0
            name="DUMMYCOLUMN"
            />
        </Columns>
      </WS_ALL_WUS>
      <WS_DEFECTIVE_WUS
        changeable="N"
        name="WS_DEFECTIVE_WUS"
        >
        <Columns>
          <Column1
            name="Status"
            >
            <Filter
              name="val"
              val="Error"
              />
          </Column1>
        </Columns>
      </WS_DEFECTIVE_WUS>
      <WS_RUNNING_WUS
        changeable="N"
        name="WS_RUNNING_WUS"
        >
        <Columns>
          <Column1
            name="Status"
            >
            <Filter
              name="val"
              val="Running"
              />
          </Column1>
        </Columns>
      </WS_RUNNING_WUS>
      <WS_STOPPED_WUS
        changeable="N"
        name="WS_STOPPED_WUS"
        >
        <Columns>
          <Column1
            name="Status"
            >
            <Filter
              name="val"
              val="Stopped"
              />
          </Column1>
        </Columns>
      </WS_STOPPED_WUS>
    </SYSTEM_MOD_ID>
    <WORKFLOWS_MOD_ID>
      <WS_ALL_WORKFLOWS
        changeable="N"
        name="WS_ALL_WORKFLOWS"
        >
        <Columns>
          <Column0
            name="DUMMYCOLUMN"
            />
        </Columns>
      </WS_ALL_WORKFLOWS>
    </WORKFLOWS_MOD_ID>
  </WorkingSets>
</DPF>
