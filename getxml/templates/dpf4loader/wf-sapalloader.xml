<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4LOADER.sapalloader
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Document Load - Subprocess SAP Archive Link Loader"
        PROJECT="DPF4LOADER"
        VERSION="$Id: wf-sapalloader.xml,v 1.7 2013/12/03 13:06:05 tobias Exp $"
        >
        <CheckinALFile
          COMMENT="execute 1 AL @%SAPBO_OBJECTTYPE%@%SAPBO_OBJECTKEY%@%SAPAL_ARCHIVEID%@BACK@%CurrFileName%@%SAPAL_DOCTYPE%@%SAPAL_DOCCLASS%@%SAPAL_PIECE_8%@"
