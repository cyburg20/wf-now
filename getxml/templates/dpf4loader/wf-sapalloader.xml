          NAME="Sap.executeSapFunction"
          NEXT="END"
          POSX="700"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            ExecuteSapFunctionErrorStatus="0"
            FunctionCall="%SAPFUNC%"
            SAPLANGU="%SAPCHECKIN_LANGUAGE%"
            SAPMANDANT="%SAPCHECKIN_MANDANT%"
            SAPSYSTEM="%SAPCHECKIN_SYSTEM%"
            />
        </CheckinALFile>
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="2800"
          POSY="300"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="CheckinALFile"
          NAME=""
          />
        <MAPPING/>
      </DPF4LOADER.sapalloader>
    </workflows>
  </config>
</DPF>
