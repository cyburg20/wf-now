<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4LOADER.opentextloader
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Document Load - Subprocess OpenText Loader"
        PROJECT="DPF4LOADER"
        VERSION="$Id: wf-opentextloader.xml,v 1.7 2013/12/03 13:18:05 tobias Exp $"
        >
        <CheckinWithOTName
          COMMENT=""
          ERROR="END"
          NAME="opentext.executeopentextfunction"
          NEXT="END"
          POSX="700"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <OPENTEXT_INI
              val="%PLSSV%/sapserv/conf/opentext.ini"
              />
          </DEFAULTS>
          <MAPPING
            FunctionCall="put -objid &quot;%OT_DATAID%&quot; -filename &quot;%CurrFileName%&quot;"
            />
        </CheckinWithOTName>
        <CheckinWithoutOTName
          COMMENT=""
          ERROR="END"
          NAME="opentext.executeopentextfunction"
          NEXT="END"
          POSX="700"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <OPENTEXT_INI
              val="%PLSSV%/sapserv/conf/opentext.ini"
              />
          </DEFAULTS>
          <MAPPING
            FunctionCall="put -objid &quot;%OT_DATAID%&quot; -filename &quot;%CurrFileName%&quot; -objname &quot;%OT_NAME%&quot;"
            />
        </CheckinWithoutOTName>
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="1400"
          POSY="200"
          TYPE="STOP"
          />
        <IF-OTName
          COMMENT=""
          ELSE="CheckinWithOTName"
          OPERATOR="!="
          POSX="500"
          POSY="300"
          PROPERTY="OT_NAME"
          THEN="CheckinWithoutOTName"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="HORIZONTAL"
            THEN="DIRECT"
            />
        </IF-OTName>
        <MAIN
          BEGIN="IF-OTName"
          NAME=""
          />
        <MAPPING/>
      </DPF4LOADER.opentextloader>
    </workflows>
  </config>
</DPF>
