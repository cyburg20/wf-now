<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <FileCompress.unixCreateTar
        COMMAND="rsh %RSH_HOSTNAME% -l %RSH_USERNAME% tar -c -C \&quot;%TARDIR%\&quot; -f \&quot;%TARFILENAME%\&quot; ."
        DPFINT-FileLinks="doc://:/rsh"
        DPFINT-Icon="doc://:/wu.gif"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Unix Tar"
        PROJECT="FileCompress"
        THREADS="3"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-unixcreatetar.xml,v 1.2 2007/02/13 23:58:29 stefan Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          TARDIR=""
          TARFILENAME=""
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          DPFINT-Icon="doc://:/wu-input.gif"
          TARDIR="*"
          TARFILENAME="*"
          WU_ENABLE_DEBUG_LOOP="*"
          />
        <OUTPUT
          DPFINT-Icon="doc://:/wu-output.gif"
          />
      </FileCompress.unixCreateTar>
    </workingunits>
  </config>
</DPF>
