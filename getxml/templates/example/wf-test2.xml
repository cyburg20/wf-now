<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Thesis.Test2
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Test2"
        PROJECT="Thesis"
        VERSION="$Id$"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="1000"
          POSY="100"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="MODIFY2"
          OPERATOR="eq"
          POSX="500"
          POSY="100"
          PROPERTY="foo"
          THEN="MODIFY3"
          TYPE="IF-THEN-ELSE"
          VALUE="bar"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <MAIN
          BEGIN="example1"
          NAME=""
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="IF-THEN-ELSE1"
          POSX="300"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            test="mist"
            />
        </MODIFY1>
        <MODIFY2
          COMMENT=""
          NEXT="END"
          POSX="700"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            path="no"
            />
        </MODIFY2>
        <MODIFY3
          COMMENT=""
          NEXT="END"
          POSX="600"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            path="yes"
            />
        </MODIFY3>
        <example1
          COMMENT=""
          ERROR="END"
          NAME="example"
          NEXT="MODIFY1"
          POSX="100"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            DPFINT-Icon="doc://:/defaults.gif"
            >
            <CURRFILENAME
              val=""
              />
            <TargetFileName
              val="%JobDir%/%WFPOSITION%.pdf"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </example1>
      </Thesis.Test2>
    </workflows>
  </config>
</DPF>
