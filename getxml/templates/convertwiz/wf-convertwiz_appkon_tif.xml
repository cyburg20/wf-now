        VERSION="$Id: wf-convertwiz_appkon_tif.xml,v 1.5 2011/10/27 14:48:32 dh Exp $"
        >
        <CONVERTWIZ.Convertwiz_User_Pre_Convert1COPY
          COMMENT=""
          ERROR="END"
          NEXT="check_how_to_convertCOPY"
          POSX="100"
          POSY="200"
          TYPE="SWF"
          USE="CONVERTWIZ.Convertwiz_User_Pre_Convert"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </CONVERTWIZ.Convertwiz_User_Pre_Convert1COPY>
        <CONVERTWIZ.check_office_versionCOPY7
          COMMENT=""
          ERROR="END"
          NAME="CONVERTWIZ.check_office_version"
          NEXT="SWITCH_CW_OFFICE_MAJOR_VERSIONCOPY6"
          POSX="700"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            CW_OFFICE_MAJOR_VERSION=""
            />
        </CONVERTWIZ.check_office_versionCOPY7>
        <CONVERTWIZ.conversion_nativeCOPY3
          COMMENT=""
          ERROR="END"
          NAME="CONVERTWIZ.conversion_native"
          NEXT="check_for_user_exit"
          POSX="900"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            APPKONV_CONFIG_DIR_DEFAULT="%DPFSRV%/conf/dpf4convertwiz/appkonv"
            />
        </CONVERTWIZ.conversion_nativeCOPY3>
        <CONVERTWIZ.conversion_office_10COPY9
          COMMENT=""
          ERROR="END"
          NAME="CONVERTWIZ.conversion_office_10"
          NEXT="check_for_user_exit"
          POSX="1100"
          POSY="600"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            APPKONV_CONFIG_DIR_DEFAULT="%DPFSRV%/conf/dpf4convertwiz/appkonv"
            />
        </CONVERTWIZ.conversion_office_10COPY9>
        <CONVERTWIZ.conversion_office_11COPY8
          COMMENT=""
          ERROR="END"
          NAME="CONVERTWIZ.conversion_office_11"
          NEXT="check_for_user_exit"
          POSX="1100"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            APPKONV_CONFIG_DIR_DEFAULT="%DPFSRV%/conf/dpf4convertwiz/appkonv"
            />
        </CONVERTWIZ.conversion_office_11COPY8>
        <CONVERTWIZ.conversion_office_12COPY5
          COMMENT=""
          ERROR="END"
          NAME="CONVERTWIZ.conversion_office_12"
          NEXT="check_for_user_exit"
          POSX="1100"
          POSY="800"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            APPKONV_CONFIG_DIR_DEFAULT="%DPFSRV%/conf/dpf4convertwiz/appkonv"
            />
        </CONVERTWIZ.conversion_office_12COPY5>
        <DEFAULTS/>
        <DPF4CONVERT.tiffstamp1
          COMMENT=""
          ERROR="END"
          NAME="tiffstamp"
          NEXT="END"
          POSX="2200"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            DPF4C_MODIFY_STAMP_N_TEXT0=""
            DPF4C_MODIFY_STAMP_N_TEXT1=""
            DPF4C_MODIFY_STAMP_N_TEXT2=""
            DPF4C_MODIFY_STAMP_N_TEXT3=""
            DPF4C_MODIFY_STAMP_N_TEXT4=""
            DPF4C_MODIFY_STAMP_N_TEXT5=""
            DPF4C_MODIFY_STAMP_N_TEXT6=""
            DPF4C_MODIFY_STAMP_N_TEXT7=""
            DPF4C_MODIFY_STAMP_N_TEXT8=""
            DPF4C_MODIFY_STAMP_N_TEXT9=""
            DPF4C_STAMP_DB_FILE=""
            DPF4C_STAMP_STP_FILE=""
            DPF4C_TIFFSTAMP_DB_FILE="default.db"
            DPF4C_TIFFSTAMP_EXT=""
            DPF4C_TIFFSTAMP_STP_FILE="default.stp"
            DPF4C_TIFFSTAMP_TXT_FILE="%PLSROOT%tools/convcfg/tiffstamp/default.txt"
            USE_DEFAULT_TXT_FILE="N"
            WU_ENABLE_DEBUG_LOOP="N"
            />
          <MAPPING/>
        </DPF4CONVERT.tiffstamp1>
        <END
          COMMENT=""
          NAME="END"
          POSX="1300"
          POSY="800"
          TYPE="STOP"
          />
        <FileTransfermv
          COMMENT=""
          ERROR="END"
          NAME="FileTransfer.mv"
          NEXT="MODIFY2"
          POSX="700"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            SOURCE="%CURRFILENAME%"
            TARGET="%JOBDIR%/%TARGETBASENAME%.pdf"
            />
        </FileTransfermv>
        <MAIN
          BEGIN="check_for_user_exit_pre_convert"
          NAME=""
          />
        <MAPPING/>
        <MODIFY2
          COMMENT=""
          NEXT="CONVERTWIZ.conversion_nativeCOPY3"
          POSX="900"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%JOBDIR%/%TARGETBASENAME%.pdf"
            />
        </MODIFY2>
        <SWITCH_CW_OFFICE_MAJOR_VERSIONCOPY6
          CASE:V10="CONVERTWIZ.conversion_office_10COPY9"
          CASE:V11="CONVERTWIZ.conversion_office_11COPY8"
          CASE:V12="CONVERTWIZ.conversion_office_12COPY5"
          COMMENT=""
          CONNECTION:DEFAULT="HORIZONTAL"
          CONNECTION:V10="HORIZONTAL"
          CONNECTION:V11="DIRECT"
          CONNECTION:V12="HORIZONTAL"
          DEFAULT="CONVERTWIZ.conversion_office_12COPY5"
          POSX="900"
          POSY="700"
          PROPERTY="CW_OFFICE_MAJOR_VERSION"
          TYPE="SWITCH"
          />
        <SWITCH_IS_OFFICE_FORMATCOPY4
          CASE:cgm="CONVERTWIZ.conversion_nativeCOPY3"
          CASE:doc="CONVERTWIZ.check_office_versionCOPY7"
          CASE:docm="CONVERTWIZ.check_office_versionCOPY7"
          CASE:docx="CONVERTWIZ.check_office_versionCOPY7"
          CASE:dot="CONVERTWIZ.check_office_versionCOPY7"
          CASE:dotm="CONVERTWIZ.check_office_versionCOPY7"
          CASE:dotx="CONVERTWIZ.check_office_versionCOPY7"
          CASE:potm="CONVERTWIZ.check_office_versionCOPY7"
          CASE:potx="CONVERTWIZ.check_office_versionCOPY7"
          CASE:pps="CONVERTWIZ.check_office_versionCOPY7"
          CASE:ppsm="CONVERTWIZ.check_office_versionCOPY7"
          CASE:ppsx="CONVERTWIZ.check_office_versionCOPY7"
          CASE:ppt="CONVERTWIZ.check_office_versionCOPY7"
          CASE:pptm="CONVERTWIZ.check_office_versionCOPY7"
          CASE:pptx="CONVERTWIZ.check_office_versionCOPY7"
          CASE:ppz="CONVERTWIZ.check_office_versionCOPY7"
          CASE:rtf="CONVERTWIZ.check_office_versionCOPY7"
          CASE:xls="CONVERTWIZ.check_office_versionCOPY7"
          CASE:xlsb="CONVERTWIZ.check_office_versionCOPY7"
          CASE:xlsm="CONVERTWIZ.check_office_versionCOPY7"
          CASE:xlst="CONVERTWIZ.check_office_versionCOPY7"
          CASE:xlsx="CONVERTWIZ.check_office_versionCOPY7"
          CASE:xlt="CONVERTWIZ.check_office_versionCOPY7"
          CASE:xltm="CONVERTWIZ.check_office_versionCOPY7"
          COMMENT=""
          CONNECTION:DEFAULT="HORIZONTAL"
          CONNECTION:cgm="HORIZONTAL"
          CONNECTION:doc="HORIZONTAL"
          CONNECTION:docm="DIRECT"
          CONNECTION:docx="DIRECT"
          CONNECTION:dot="DIRECT"
          CONNECTION:dotm="DIRECT"
          CONNECTION:dotx="DIRECT"
          CONNECTION:potm="DIRECT"
          CONNECTION:potx="DIRECT"
          CONNECTION:pps="DIRECT"
          CONNECTION:ppsm="DIRECT"
          CONNECTION:ppsx="DIRECT"
          CONNECTION:ppt="DIRECT"
          CONNECTION:pptm="DIRECT"
          CONNECTION:pptx="DIRECT"
          CONNECTION:ppz="DIRECT"
          CONNECTION:rtf="DIRECT"
          CONNECTION:xls="DIRECT"
          CONNECTION:xlsb="DIRECT"
          CONNECTION:xlsm="DIRECT"
          CONNECTION:xlst="DIRECT"
          CONNECTION:xlsx="DIRECT"
          CONNECTION:xlt="DIRECT"
          CONNECTION:xltm="DIRECT"
          DEFAULT="CONVERTWIZ.conversion_nativeCOPY3"
          POSX="500"
          POSY="500"
          PROPERTY="CURRFILETYPE"
          TYPE="SWITCH"
          />
        <check_for_stampfileCOPY
          COMMENT=""
          ELSE="END"
          OPERATOR="exists"
          POSX="1600"
          POSY="700"
          PROPERTY="STAMPFILE"
          THEN="check_for_stampfile_noneCOPY"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_stampfileCOPY>
        <check_for_stampfile_noneCOPY
          COMMENT=""
          ELSE="END"
          OPERATOR="ne"
          POSX="1800"
          POSY="700"
          PROPERTY="STAMPFILE"
          THEN="setup_for_stamping"
          TYPE="IF-THEN-ELSE"
          VALUE="none"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_stampfile_noneCOPY>
        <check_for_user_exit
          COMMENT=""
          ELSE="check_for_stampfileCOPY"
          OPERATOR="eq"
          POSX="1400"
          POSY="700"
          PROPERTY="CONVERTWIZ_SWITCH_DO_USER_POST_CONVERT"
          THEN="convertwiz_user_post_convert"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_user_exit>
        <check_for_user_exit_pre_convert
          COMMENT=""
          ELSE="check_how_to_convertCOPY"
          OPERATOR="eq"
          POSX="100"
          POSY="500"
          PROPERTY="CONVERTWIZ_SWITCH_DO_USER_PRE_START"
          THEN="CONVERTWIZ.Convertwiz_User_Pre_Convert1COPY"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_user_exit_pre_convert>
        <check_how_to_convertCOPY
          COMMENT=""
          ELSE="SWITCH_IS_OFFICE_FORMATCOPY4"
          OPERATOR="contains"
          POSX="300"
          POSY="500"
          PROPERTY="CW_FILETYPES_FOR_DPF4C_CONVERT"
          THEN="convert_native"
          TYPE="IF-THEN-ELSE"
          VALUE=",%CURRFILETYPE%,"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="HORIZONTAL"
            />
        </check_how_to_convertCOPY>
        <convert_native
          COMMENT=""
          ERROR="END"
          NEXT="FileTransfermv"
          POSX="500"
          POSY="200"
          TYPE="SWF"
          USE="DPF4CONVERT.convert_native"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </convert_native>
        <convertwiz_user_post_convert
          COMMENT=""
          ERROR="END"
          NEXT="check_for_stampfileCOPY"
          POSX="1500"
          POSY="500"
          TYPE="SWF"
          USE="CONVERTWIZ.convertwiz_user_post_convert"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </convertwiz_user_post_convert>
        <setup_for_stamping
          COMMENT=""
          NEXT="DPF4CONVERT.tiffstamp1"
          POSX="2000"
          POSY="700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_MODIFY_STAMP_N_TEXT9="%JOBNAME%"
            DPF4C_STAMP_STP_FILE="%STAMPFILE%"
            />
        </setup_for_stamping>
