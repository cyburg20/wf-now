<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <HTML.EditFrameset
        COMMAND="%DPFSRV%/scripts/html/editframeset.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="EditFrameset"
        PROJECT="HTML"
        Status="Running"
        THREADS="1"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-editframeset.xml,v 1.3 2007/09/14 05:32:34 tobias Exp $"
        >
        <DEFAULTS/>
        <INPUT>
          <CLASS
            DESCRIPTION="value of class attribute (optional)"
            TYPE="TEXT"
            />
          <COLS
            DESCRIPTION="value of cols attribute (optional)"
            TYPE="TEXT"
            />
          <FRAMESET_DIR
            DESCRIPTION="dir containing %FRAMESET_FILE% (optional); default is %JOBDIR%"
            TYPE="TEXT"
            />
          <FRAMESET_FILE
            DESCRIPTION="file containing the frameset (mandatory)"
            TYPE="TEXT"
            />
          <ID
            DESCRIPTION="value of id attribute (optional)"
            TYPE="TEXT"
            />
          <POS
            DESCRIPTION="path where to add the new frameset (corresponding to opening tag); &apos;+...&apos; means insert after; &apos;-...&apos; means insert before; &apos;...&apos; means replace; if tag does not exist, it will be tried to be appended as last tag on the level"
            TYPE="TEXT"
            />
          <ROWS
            DESCRIPTION="value of rows attribute (optional)"
            TYPE="TEXT"
            />
          <STYLE
            DESCRIPTION="value of style attribute (optional)"
            TYPE="TEXT"
            />
          <TITLE
            DESCRIPTION="value of title attribute (optional)"
            TYPE="TEXT"
            />
        </INPUT>
      </HTML.EditFrameset>
    </workingunits>
  </config>
</DPF>
