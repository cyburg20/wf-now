<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.createTwoSubJobs
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="createTwoSubJobs"
        PROJECT="Test"
        VERSION="$Id: wf-createtwosubjobs.xml,v 1.2 2010/06/24 15:34:09 stefan Exp $"
        >
        <DEFAULTS
          SUB_SUB_WFNAME="test.ok"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="900"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="init"
          NAME=""
          />
        <MAPPING/>
        <callSubSubJobs
          COMMENT=""
          ERROR="END"
          NAME="Test.callSubSubJobs"
          NEXT="setFinish"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            SUB_WFNAME="%SUB_SUB_WFNAME%"
            />
        </callSubSubJobs>
        <init
          COMMENT=""
          NEXT="init2"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            NUM_OF_JOBS="2"
            />
        </init>
        <init2
          COMMENT=""
          NEXT="callSubSubJobs"
          POSX="300"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfDebugSubJobs="Y"
            DpfJobMessage="Creating %NUM_OF_JOBS% sub jobs..."
            />
        </init2>
        <setFinish
          COMMENT=""
          NEXT="END"
          POSX="700"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobMessage="%NUM_OF_JOBS% sub jobs created."
            />
        </setFinish>
      </Test.createTwoSubJobs>
    </workflows>
  </config>
</DPF>
