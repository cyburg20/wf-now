<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.testProcessLines
        COMMENT=""
        DPFINT-Type="WorkflowDescription"
        NAME="Test ProcessLines"
        PROJECT="Test"
        VERSION="$Id: wf-testprocesslines.xml,v 1.2 2009/01/13 11:08:18 sek Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="1100"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="init"
          NAME=""
          />
        <MAPPING/>
        <createSollRli
          COMMENT=""
          ERROR="END"
          NAME="Test.createTestRli"
          NEXT="processLines"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING
            CreateSollRli="Y"
            DestFileName="%SollRliFileName%"
            />
        </createSollRli>
        <createTestRli
          COMMENT=""
          ERROR="END"
          NAME="Test.createTestRli"
          NEXT="createSollRli"
          POSX="300"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING
            DestFileName="%CurrRliFIleName%"
            />
        </createTestRli>
        <init
          COMMENT=""
          NEXT="createTestRli"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            CurrRliFileName="%JOBDIR%/test.rli"
            SollRliFileName="%JOBDIR%/soll.rli"
            />
        </init>
        <processLines
          COMMENT=""
          ERROR="END"
          NAME="Rli.processLines"
          NEXT="END"
          POSX="700"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            IgnoreSubJobErrors="Y"
            SubProcess="Test.ProcessLinesSub"
            />
        </processLines>
      </Test.testProcessLines>
    </workflows>
  </config>
</DPF>
