<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.testRepeat
        COMMENT="Dieser Testprozess muss mit

dpfclient -wf Test.testLogoff -s VALUE=3

aufgerufen werden. Damit wird getestet, ob Originale Parameter nach einem Import und Repeat
"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Test Repeat"
        PROJECT="Test"
        VERSION="$Id: wf-testrepeat.xml,v 1.1 2009/08/04 09:43:10 stefan Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="precondition"
          NAME=""
          />
        <MAPPING/>
        <Test.increment1
          COMMENT=""
          ERROR="END"
          NAME="Test.increment"
          NEXT="Test.increment2"
          POSX="200"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Test.increment1>
        <Test.increment2
          COMMENT=""
          ERROR="END"
          NAME="Test.increment"
          NEXT="Test.increment3"
          POSX="400"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Test.increment2>
        <Test.increment3
          COMMENT=""
          ERROR="END"
          NAME="Test.increment"
          NEXT="postcondition"
          POSX="600"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Test.increment3>
        <postcondition
          COMMENT=""
          ELSE="wrongValue"
          OPERATOR="=="
          POSX="800"
          POSY="100"
          PROPERTY="VALUE"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE="6"
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </postcondition>
        <precondition
          COMMENT=""
          ELSE="undefinedValue"
          OPERATOR="exists"
          POSX="0"
          POSY="100"
          PROPERTY="VALUE"
          THEN="Test.increment1"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </precondition>
        <undefinedValue
          COMMENT=""
          ERROR="END"
          NAME="error"
          NEXT="END"
          POSX="0"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <ERR_CODE
              val="1"
              />
          </DEFAULTS>
          <MAPPING
            ERR_REASON="Parameter VALUE nicht definiert"
            />
        </undefinedValue>
        <wrongValue
          COMMENT=""
          ERROR="END"
          NAME="error"
          NEXT="END"
          POSX="800"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <ERR_CODE
              val="1"
              />
          </DEFAULTS>
          <MAPPING
            ERR_REASON="VALUE ist nicht 6, sondern %VALUE%."
            />
        </wrongValue>
      </Test.testRepeat>
    </workflows>
  </config>
</DPF>
