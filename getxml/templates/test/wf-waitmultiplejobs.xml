<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.waitMultipleJobs
        COMMENT=""
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="Wait Multiple Jobs"
        PROJECT="Test"
        VERSION="$Id: wf-waitmultiplejobs.xml,v 1.1 2010/05/07 10:24:38 stefan Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="setJobName"
          NAME=""
          />
        <MAPPING/>
        <doSomething
          COMMENT=""
          ERROR="END"
          NAME="Test.exit0"
          NEXT="waitMultipleJobs"
          POSX="400"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <CURRFILENAME
              val=""
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </doSomething>
        <doSomethingElse
          COMMENT=""
          ERROR="END"
          NAME="Test.getBaseName"
          NEXT="END"
          POSX="800"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </doSomethingElse>
        <setJobName
          COMMENT=""
          NEXT="doSomething"
          POSX="200"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            JobName="%ExternalJobID%"
            />
        </setJobName>
        <waitMultipleJobs
          COMMENT=""
          ERROR="END"
          NAME="Test.waitMultipleJobs"
          NEXT="doSomethingElse"
          POSX="600"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            WuEnableDebugLoop="N"
            />
        </waitMultipleJobs>
      </Test.waitMultipleJobs>
    </workflows>
  </config>
</DPF>
