<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.ucttTestDpfClient
        COMMENT="see tools/testfiles/dpfalice/dpfclient/ for UCTT tests running this DPF process."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Uctt Test DPF Client"
        PROJECT="Test"
        VERSION="$Id: wf-uctttestdpfclient.xml,v 1.1 2008/11/21 14:53:02 stefan Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="init"
          NAME=""
          />
        <MAPPING/>
        <init
          COMMENT=""
          NEXT="latin1"
          POSX="100"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            FirstParFileName="%JOBDIR%/latin1.par"
            LogFileName="%JOBDIR%/job.log"
            />
        </init>
        <latin1
          COMMENT=""
          ERROR="END"
          NAME="example"
          NEXT="sendback"
          POSX="100"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <CURRFILENAME
              val=""
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </latin1>
        <sendback
          COMMENT=""
          ERROR="END"
          NAME="sendback"
          NEXT="END"
          POSX="300"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <IOR
              val="%ORIGFILESERVERIOR%"
              />
            <OVERWRITE
              val="Y"
              />
            <SOURCE
              val="%CURRFILENAME%"
              />
            <TARGETFILE
              val="%TARGETFILENAME%"
              />
          </DEFAULTS>
          <MAPPING
            SEND_FILE_PROPERTIES="FirstParFileName LogFileName"
            />
        </sendback>
      </Test.ucttTestDpfClient>
    </workflows>
  </config>
</DPF>
