<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.ProgressBar
        COMMENT="Teste die Fortschrittsanzeige in DPF Tracker"
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Type="WorkflowDescription"
        INTERNALNAME="Test.ProgressBar"
        NAME="Progress Bar"
        PROJECT="Test"
        VERSION="$Id: wf-progressbar.xml,v 1.5 2006/11/14 17:24:41 stefan Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <FirstSleepMulti
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep10threads"
          NEXT="set25"
          POSX="300"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </FirstSleepMulti>
        <FirstSleepSingle
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep1thread"
          NEXT="set25"
          POSX="100"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS>
            <TIME_TO_SLEEP
              val=""
              />
          </DEFAULTS>
        </FirstSleepSingle>
        <FourthSleepMulti
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep10threads"
          NEXT="set100"
          POSX="300"
          POSY="1900"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </FourthSleepMulti>
        <FourthSleepSingle
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep1thread"
          NEXT="set100"
          POSX="100"
          POSY="1900"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS>
            <TIME_TO_SLEEP
              val=""
              />
          </DEFAULTS>
        </FourthSleepSingle>
        <IF-MULTI1
          COMMENT=""
          ELSE="FirstSleepSingle"
          OPERATOR="is true"
          POSX="200"
          POSY="300"
          PROPERTY="MULTI_THREAD"
          THEN="FirstSleepMulti"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </IF-MULTI1>
        <IF-MULTI2
          COMMENT=""
          ELSE="SecondSleepSingle"
          OPERATOR="is true"
          POSX="200"
          POSY="800"
          PROPERTY="MULTI_THREAD"
          THEN="SecondSleepMulti"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </IF-MULTI2>
        <IF-MULTI3
          COMMENT=""
          ELSE="ThirdSleepSingle"
          OPERATOR="is true"
          POSX="200"
          POSY="1300"
          PROPERTY="MULTI_THREAD"
          THEN="ThirdSleepMulti"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </IF-MULTI3>
        <IF-MULTI4
          COMMENT=""
          ELSE="FourthSleepSingle"
          OPERATOR="is true"
          POSX="200"
          POSY="1800"
          PROPERTY="MULTI_THREAD"
          THEN="FourthSleepMulti"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </IF-MULTI4>
        <MAIN
          BEGIN="set00"
          ERROR="END"
          />
        <SecondSleepMulti
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep10threads"
          NEXT="set50"
          POSX="300"
          POSY="900"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </SecondSleepMulti>
        <SecondSleepSingle
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep1thread"
          NEXT="set50"
          POSX="100"
          POSY="900"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS>
            <TIME_TO_SLEEP
              val=""
              />
          </DEFAULTS>
        </SecondSleepSingle>
        <ThirdSleepMulti
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep10threads"
          NEXT="set75"
          POSX="300"
          POSY="1400"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ThirdSleepMulti>
        <ThirdSleepSingle
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep1thread"
          NEXT="set75"
          POSX="100"
          POSY="1400"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS>
            <TIME_TO_SLEEP
              val=""
              />
          </DEFAULTS>
        </ThirdSleepSingle>
        <set00
          COMMENT=""
          NEXT="IF-MULTI1"
          POSX="200"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF_JOB_PROGRESS="0"
            USER="%ORIGUSERNAME%"
            />
        </set00>
        <set100
          COMMENT=""
          NEXT="END"
          POSX="200"
          POSY="2100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF_JOB_PROGRESS="100"
            />
        </set100>
        <set25
          COMMENT=""
          NEXT="IF-MULTI2"
          POSX="200"
          POSY="600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF_JOB_PROGRESS="25"
            />
        </set25>
        <set50
          COMMENT=""
          NEXT="IF-MULTI3"
          POSX="200"
          POSY="1100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF_JOB_PROGRESS="50"
            />
        </set50>
        <set75
          COMMENT=""
          NEXT="IF-MULTI4"
          POSX="200"
          POSY="1600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF_JOB_PROGRESS="75"
            />
        </set75>
      </Test.ProgressBar>
    </workflows>
  </config>
</DPF>
