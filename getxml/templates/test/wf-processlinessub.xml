<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.ProcessLinesSub
        COMMENT="sub process for Test.ProcessLines"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="ProcessLinesSub"
        PROJECT="Test"
        VERSION="$Id: wf-processlinessub.xml,v 1.1 2008/08/20 07:06:09 stefan Exp $"
        >
        <DEFAULTS/>
        <MAIN
          BEGIN="init"
          NAME=""
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="END"
          POSX="900"
          POSY="200"
          TYPE="MODIFY"
          >
          <SET
            CurrFileName="%ParentJobDir%/dummy%RliIndex%.pdf"
            d="d%RliIndex%"
            />
        </MODIFY1>
        <Test.exit01
          COMMENT=""
          ERROR="END"
          NAME="Test.exit0"
          NEXT="sleep1"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Test.exit01>
        <Test.exit51
          COMMENT=""
          ERROR="END"
          NAME="Test.exit5"
          NEXT="MODIFY1"
          POSX="500"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Test.exit51>
        <init
          COMMENT=""
          NEXT="switch"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            a="a%RliIndex%"
            b="b%RliIndex%"
            c="c%RliIndex%"
            />
        </init>
        <sleep1
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep1thread"
          NEXT="MODIFY1"
          POSX="700"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING
            TimeToSleep="1"
            />
        </sleep1>
        <sleep2
          COMMENT=""
          ERROR="END"
          NAME="Test.sleep10threads"
          NEXT="MODIFY1"
          POSX="500"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sleep2>
        <switch
          CASE:14="Test.exit51"
          CASE:15="sleep2"
          CASE:19="Test.exit51"
          CASE:21="sleep2"
          CASE:22="MODIFY1"
          CASE:23="Test.exit51"
          CASE:26="Test.exit51"
          CASE:27="MODIFY1"
          CASE:3="Test.exit51"
          CASE:4="sleep2"
          CASE:6="MODIFY1"
          CASE:7="Test.exit51"
          CASE:8="MODIFY1"
          COMMENT=""
          CONNECTION:14="HORIZONTAL"
          CONNECTION:15="HORIZONTAL"
          CONNECTION:19="HORIZONTAL"
          CONNECTION:21="HORIZONTAL"
          CONNECTION:22="HORIZONTAL"
          CONNECTION:23="HORIZONTAL"
          CONNECTION:26="HORIZONTAL"
          CONNECTION:27="HORIZONTAL"
          CONNECTION:3="HORIZONTAL"
          CONNECTION:4="HORIZONTAL"
          CONNECTION:6="HORIZONTAL"
          CONNECTION:7="HORIZONTAL"
          CONNECTION:8="HORIZONTAL"
          CONNECTION:DEFAULT="HORIZONTAL"
          DEFAULT="Test.exit01"
          POSX="300"
          POSY="100"
          PROPERTY="RliIndex"
          TYPE="SWITCH"
          />
      </Test.ProcessLinesSub>
    </workflows>
  </config>
</DPF>
