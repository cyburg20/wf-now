<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Test.testperlcommand
        COMMAND="perl %DPFSRV%/scripts/test/exit0.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Test Perl command"
        PROJECT="Test"
        Status="Running"
        THREADS="5"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-testperlcommand.xml,v 1.6 2008/10/06 07:26:23 stefan Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <ENVIRONMENTS
          HERE_YOU_COULD_SET_EXTRA_ENV_VARS="to a value"
          />
        <INPUT
          CURRFILENAME="*"
          DPFINT-Icon="doc://:/wu-input.gif"
          WU_ENABLE_DEBUG_LOOP="*"
          >
          <CURRFILENAME
            TYPE="TEXT"
            />
          <JOBDIR
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WU_ENABLE_DEBUG_LOOP>
        </INPUT>
      </Test.testperlcommand>
    </workingunits>
  </config>
</DPF>
