<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Test.solarisRemoteCall
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/test/solarisremotecall.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Solaris Remote Call"
        NODENAMES=""
        PROJECT="Test"
        THREADS="1"
        TIMEOUT=""
        TYPE="WURemoteCall"
        VERSION="$Id: wu-solarisremotecall.xml,v 1.2 2010/06/23 13:30:42 stefan Exp $"
        >
        <DEFAULTS
          InputFileList="CurrFileName"
          OutputFileList="CurrFileName"
          />
        <INPUT>
          <InputFileList
            DEFAULT="CurrFileName"
            DESCRIPTION="list of DPF parameters to send to unix as input file(s)"
            TYPE="TEXT"
            />
          <OutputFileList
            DEFAULT="CurrFileName"
            DESCRIPTION="list of DPF parameters to send back from unix call as resulting file(s)"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </Test.solarisRemoteCall>
    </workingunits>
  </config>
</DPF>
