<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Test.createJobs
        COMMENT=""
        DPFINT-Type="WorkflowDescription"
        NAME="Create Jobs"
        PROJECT="Test"
        VERSION="$Id: wf-createjobs.xml,v 1.1 2008/04/21 11:29:53 sek Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="init"
          NAME=""
          />
        <Test.callJobs1
          COMMENT=""
          ERROR="END"
          NAME="Test.callJobs"
          NEXT="setFinish"
          POSX="300"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            NO_SUBJOBS="Y"
            />
        </Test.callJobs1>
        <init
          COMMENT=""
          NEXT="Test.callJobs1"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobMessage="Creating %NUM_OF_JOBS% jobs..."
            />
        </init>
        <setFinish
          COMMENT=""
          NEXT="END"
          POSX="500"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobMessage="%NUM_OF_JOBS% jobs created."
            />
        </setFinish>
      </Test.createJobs>
    </workflows>
  </config>
</DPF>
