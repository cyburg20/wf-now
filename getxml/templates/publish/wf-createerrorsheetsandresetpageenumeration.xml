<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Publish.CreateErrorSheetsAndResetPageEnumeration
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Create Error Sheets And Reset Page Enumeration"
        PROJECT="Publish"
        VERSION="$Id: wf-createerrorsheetsandresetpageenumeration.xml,v 1.8 2013/04/04 12:46:47 sek Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="300"
          POSY="100"
          TYPE="STOP"
          />
        <GetPageNumbers
          COMMENT=""
          ERROR="END"
          NAME="Rli.getPageNumbers"
          NEXT="Rli.fillRliColumn1COPY1"
          POSX="600"
          POSY="1100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            FilterOperator2="=~"
            FilterOperator="-f"
            FilterValue2="/pdf/i"
            RlicFilter2="CurrFileType"
            RlicFilter="CurrFileName"
            />
        </GetPageNumbers>
        <MAIN
          BEGIN="MarkTemplatesForSheetCreation"
          NAME=""
          />
        <MAPPING
          ErrSheet="y"
          />
        <MarkErrorsForSheetCreation
          COMMENT=""
          ERROR="END"
          NAME="Rli.fillRliColumn"
          NEXT="SetErrorAsDefaultTemplate"
          POSX="600"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <UseEval
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="CreateSheet"
            FillData="Y"
            FilterOperator2="!~"
            FilterOperator3="eq"
            FilterOperator="ne"
            FilterValue2="/[yj1x]/i"
            FilterValue3=""
            RlicFilter2="ErrSheet"
            RlicFilter3="TEMPLATE"
            RlicFilter="ErrReason"
            />
        </MarkErrorsForSheetCreation>
        <MarkTemplatesForSheetCreation
          COMMENT=""
          ERROR="END"
          NAME="Rli.fillRliColumn"
          NEXT="MarkErrorsForSheetCreation"
          POSX="600"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <UseEval
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="CreateSheet"
            FillData="Y"
            FilterOperator="ne"
            RlicFilter="TEMPLATE"
            />
        </MarkTemplatesForSheetCreation>
        <Rli.createSheets1
          COMMENT=""
          ERROR="END"
          NAME="Rli.createSheets"
          NEXT="GetPageNumbers"
          POSX="600"
          POSY="900"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <COVER_ShowSinglePage
              val="N"
              />
            <DEFAULT_COLOR
              val="#000000"
              />
            <DEFAULT_FONT
              val="Helvetica"
              />
            <DEFAULT_SIZE
              val="20"
              />
            <UpdatePageCount
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            DEFAULT_ALIGN="%SheetDefaultAlign%"
            DEFAULT_COLOR="%SheetDefaultColor%"
            DEFAULT_FONT="%SheetDefaultFont%"
            DEFAULT_SIZE="%SheetDefaultSize%"
            ErrorReason="%%ErrReason%%"
            FilterOperator2="!~"
            FilterOperator="=~"
            FilterPrefix=""
            FilterValue2="/[yj1x]/i"
            FilterValue="/[yj1x]/i"
            LABEL_LIST="ErrorReason"
            RlicFilter2="ErrSheet"
            RlicFilter="CreateSheet"
            />
        </Rli.createSheets1>
        <Rli.fillRliColumn1
          COMMENT=""
          ERROR="END"
          NAME="Rli.fillRliColumn"
          NEXT="Rli.createSheets1"
          POSX="600"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <UseEval
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="COVER_BACKGROUND_PDF"
            FillData="%DPFSRV%/conf/multiakte2go/%%TEMPLATE%%.pdf"
            FilterOperator2="!~"
            FilterOperator="=~"
            FilterValue2="/[yj1x]/i"
            FilterValue="/[yj1x]/i"
            RlicFilter2="ErrSheet"
            RlicFilter="CreateSheet"
            />
        </Rli.fillRliColumn1>
        <Rli.fillRliColumn1COPY1
          COMMENT=""
          ERROR="END"
          NAME="Rli.fillRliColumn"
          NEXT="Rli.fillRliColumn1COPY1COPY"
          POSX="600"
          POSY="1300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <UseEval
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="TocEntry"
            FillData="[%%TEMPLATE%%] %%TocEntry%%"
            FilterOperator2="!~"
            FilterOperator="=~"
            FilterValue2="/[yj1x]/i"
            FilterValue="/[yj1x]/i"
            RlicFilter2="ErrSheet"
            RlicFilter="CreateSheet"
            />
        </Rli.fillRliColumn1COPY1>
        <Rli.fillRliColumn1COPY1COPY
          COMMENT=""
          ERROR="END"
          NAME="Rli.fillRliColumn"
          NEXT="Rli.fillRliColumn1COPY1COPYCOPY"
          POSX="600"
          POSY="1500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <UseEval
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="TocColor"
            FillData="#990000"
            FilterOperator2="!~"
            FilterOperator="=~"
            FilterValue2="/[yj1x]/i"
            FilterValue="/[yj1x]/i"
            RlicFilter2="ErrSheet"
            RlicFilter="CreateSheet"
            />
        </Rli.fillRliColumn1COPY1COPY>
        <Rli.fillRliColumn1COPY1COPYCOPY
          COMMENT=""
          ERROR="END"
          NAME="Rli.fillRliColumn"
          NEXT="END"
          POSX="600"
          POSY="1700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <UseEval
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="ErrSheet"
            FillData="Y"
            FilterOperator2="!~"
            FilterOperator="=~"
            FilterValue2="/[yj1x]/i"
            FilterValue="/[yj1x]/i"
            RlicFilter2="ErrSheet"
            RlicFilter="CreateSheet"
            />
        </Rli.fillRliColumn1COPY1COPYCOPY>
        <SetErrorAsDefaultTemplate
          COMMENT=""
          ERROR="END"
          NAME="Rli.fillRliColumn"
          NEXT="Rli.fillRliColumn1"
          POSX="600"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <UseEval
              val="N"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="TEMPLATE"
            FillData="ERROR"
            FilterOperator2="!~"
            FilterOperator3="eq"
            FilterOperator="ne"
            FilterValue2="/[yj1x]/i"
            FilterValue3=""
            RlicFilter2="ErrSheet"
            RlicFilter3="TEMPLATE"
            RlicFilter="ErrReason"
            />
        </SetErrorAsDefaultTemplate>
      </Publish.CreateErrorSheetsAndResetPageEnumeration>
    </workflows>
  </config>
</DPF>
