<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Publish.cleanup_multiakte2go
        COMMENT="Publish Unterprozess zum Aufraeumen"
        DPFINT-Customer="0"
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="cleanup (Multiakte 2 Go)"
        PROJECT="Publish"
        VERSION="$Id: wf-cleanup_multiakte2go.xml,v 1.1 2009/05/13 13:10:45 sek Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="1100"
          POSY="100"
          TYPE="STOP"
          />
        <IF-Error
          COMMENT="Nur löschen wenn kein Fehler passiert ist"
          ELSE="SWITCH2"
          OPERATOR="exists"
          POSX="700"
          POSY="300"
          PROPERTY="ERR_WFNAME"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-Error>
        <MAIN
          BEGIN="SWITCH1"
          NAME=""
          />
        <MAPPING/>
        <SWITCH1
          CASE:E="IF-Error"
          CASE:N="END"
          CASE:Y="cleanup"
          CASE:x="END"
          COMMENT="Wie soll geloescht werden?"
          CONNECTION:DEFAULT="DIRECT"
          CONNECTION:E="VERTICAL"
          CONNECTION:Y="DIRECT"
          CONNECTION:x="DIRECT"
          DEFAULT="END"
          POSX="100"
          POSY="100"
          PROPERTY="CLEANUP_JOBDIR"
          TYPE="SWITCH"
          />
        <SWITCH2
          CASE:C="END"
          CASE:P="cleanup"
          CASE:Z="cleanup"
          COMMENT="Don&apos;t delete if error occured during conversion process because SAP will do after reading state information"
          CONNECTION:C="DIRECT"
          CONNECTION:DEFAULT="DIRECT"
          CONNECTION:P="DIRECT"
          CONNECTION:Z="DIRECT"
          DEFAULT="END"
          POSX="700"
          POSY="500"
          PROPERTY="ACTIONCODE"
          TYPE="SWITCH"
          />
        <cleanup
          COMMENT="Cleanup SAP job directory"
          ERROR="END"
          NAME="Publish.cleanup"
          NEXT="END"
          POSX="100"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            WU_ENABLE_DEBUG_LOOP="false"
            />
          <MAPPING/>
        </cleanup>
      </Publish.cleanup_multiakte2go>
    </workflows>
  </config>
</DPF>
