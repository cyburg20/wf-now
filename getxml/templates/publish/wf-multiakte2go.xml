<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Publish.multiakte2go
        COMMENT="
"
        DPFINT-Customer="0"
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="Direct Publishing - Multiakte 2 Go"
        PROJECT="Publish"
        VERSION="$Id: wf-multiakte2go.xml,v 1.57 2013/08/14 15:52:24 sek Exp $"
        >
        <DEFAULTS
          DPF_DEBUG_SUBJOBS="Y"
          />
        <DPF4Publish
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="400"
          POSY="1200"
          TYPE="SWF"
          USE="DPF4Publish.Main"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4Publish>
        <END
          COMMENT=""
          NAME="END"
          POSX="600"
          POSY="800"
          TYPE="STOP"
          />
        <ErrorHandling
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="800"
          POSY="800"
          TYPE="SWF"
          USE="DPF4Publish.ErrorHandling"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ErrorHandling>
        <MAIN
          BEGIN="SetDpf4PublishProperties"
          NAME=""
          />
        <MAPPING/>
        <ReadErrorMsgs
          COMMENT=""
          ERROR="ErrorHandling"
          NAME="FileSystem.readIniFile"
          NEXT="DPF4Publish"
          POSX="400"
          POSY="1000"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            CURRFILENAME="%SecondIniFileName%"
            SECTION_NAMES="%PLS_LANG%"
            />
        </ReadErrorMsgs>
        <RenameRliColumns
          COMMENT=""
          ERROR="ErrorHandling"
          NAME="Rli.renameColumns"
          NEXT="SetPagenrVisible"
          POSX="400"
          POSY="600"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            RenameSourceColumns="CurrFileName STAMP_* STEMPEL_*"
            RenameTargetColumns="SourceFileName PLS_STAMP_* PLS_STAMP_*"
            />
        </RenameRliColumns>
        <ResetCurrFileName
          COMMENT="Resets the input file property. This is required for DPF4Publish.Initialize later."
          NEXT="RenameRliColumns"
          POSX="400"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME=""
            />
        </ResetCurrFileName>
        <SetDpf4PublishProperties
          COMMENT="Set the reprolist property to the input file.

Use extra config file to adapt dpf4publish to multiakte."
          NEXT="ResetCurrFileName"
          POSX="400"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            AllowDuplicateRliColumns="Y"
            CURRRLIFILENAME="%CURRFILENAME%"
            SecondIniFileName="%DPFSRV%/conf/multiakte2go/multiakte2go.ini"
            UseSourceFileName="Y"
            />
        </SetDpf4PublishProperties>
        <SetPagenrVisible
          COMMENT=""
          ERROR="ErrorHandling"
          NAME="Rli.fillRliColumn"
          NEXT="ReadErrorMsgs"
          POSX="400"
          POSY="800"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <Replacement
              val="_"
              />
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            FillColumn="PAGENR_VISIBLE"
            FillData="Y"
            />
        </SetPagenrVisible>
      </Publish.multiakte2go>
    </workflows>
  </config>
</DPF>
