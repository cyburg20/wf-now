<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Publish.createContents
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/publish/createcontents.pl -parfile %PARFILENAME%"
        DPFINT-FileLinks="doc://:/%DPFSRV%/scripts/publish/createcontents.pl"
        DPFINT-Icon="doc://:/publish.createcontents.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Create Table of Contents"
        NODENAMES=""
        PROJECT="Publish"
        THREADS="2"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-createcontents.xml,v 1.24 2016/04/18 15:17:07 sek Exp $"
        >
        <DEFAULTS
          AtLeastOneFilterMustMatch="N"
          BOOKMARK_DEPTH_OFFSET="0"
          CREATETOCPDF_SIMPLE_PAGE_ENUMERATION="N"
          CREATETOCPDF_WANT_TOC_BOOKMARK="Y"
          CreateTocPdfInsertAtRliIndex="N"
          CreateTocPdfInsertAtRliIndexAdjustBmOffset="Y"
          DEFAULT_ALIGN="left"
          DEFAULT_COLOR="#000000"
          DEFAULT_FONT="Helvetica"
          DEFAULT_SIZE="20"
          DPFINT-Icon="doc://:/defaults.gif"
          RLIC_ExternalLinkFileName="RelFileName"
          RlicKproUrl="KproUrl"
          TargetRliFileName="%JobDir%/%WFPOSITION%.rli"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          CREATETOCPDF_HEADLINE_TEXT="*"
          CREATETOCPDF_NOTICE_TEXT="*"
          CREATETOCPDF_TITLE_TEXT="*"
          CREATETOCPDF_TITLE_UNDER_TEXT="*"
          DPFINT-Icon="doc://:/wu-input.gif"
          TOC_TITLE="*"
          WU_ENABLE_DEBUG_LOOP="*"
          >
          <AtLeastOneFilterMustMatch
            DEFAULT="N"
            DESCRIPTION="links the filters with &quot;OR&quot;"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </AtLeastOneFilterMustMatch>
          <BOOKMARK_DEPTH_OFFSET
            DEFAULT="0"
            TYPE="TEXT"
            />
          <CREATETOCPDF_BACKGROUND_PDF
            TYPE="TEXT"
            />
          <CREATETOCPDF_CONFIG
            TYPE="TEXT"
            />
          <CREATETOCPDF_FILLDUPLEX
            TYPE="TEXT"
            />
          <CREATETOCPDF_HEADLINE_TEXT
            TYPE="TEXT"
            />
          <CREATETOCPDF_INPUT_TEXT_ENCODING
            TYPE="TEXT"
            />
          <CREATETOCPDF_NOTICE_TEXT
            TYPE="TEXT"
            />
          <CREATETOCPDF_POST_OFFSET
            TYPE="TEXT"
            />
          <CREATETOCPDF_PRE_OFFSET
            TYPE="TEXT"
            />
          <CREATETOCPDF_SIMPLE_PAGE_ENUMERATION
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </CREATETOCPDF_SIMPLE_PAGE_ENUMERATION>
          <CREATETOCPDF_TARGET_TOC_PROPERTY
            TYPE="TEXT"
            />
          <CREATETOCPDF_TITLE_TEXT
            TYPE="TEXT"
            />
          <CREATETOCPDF_TITLE_UNDER_TEXT
            TYPE="TEXT"
            />
          <CREATETOCPDF_WANT_TOC_BOOKMARK
            DEFAULT="Y"
            DESCRIPTION="turn on or off the TOC&apos;s additional bookmark"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </CREATETOCPDF_WANT_TOC_BOOKMARK>
          <CURRRLIFILENAME
            TYPE="TEXT"
            />
          <CreateTocPdfBorderBottom
            TYPE="TEXT"
            />
          <CreateTocPdfBorderLeft
            TYPE="TEXT"
            />
          <CreateTocPdfBorderTop
            TYPE="TEXT"
            />
          <CreateTocPdfBorderTopFirstPage
            TYPE="TEXT"
            />
          <CreateTocPdfBorderTopRight
            TYPE="TEXT"
            />
          <CreateTocPdfInsertAtRliIndex
            DEFAULT="N"
            DESCRIPTION="useful for toc after cover and simple page enumeration"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </CreateTocPdfInsertAtRliIndex>
          <CreateTocPdfInsertAtRliIndexAdjustBmOffset
            DEFAULT="Y"
            DESCRIPTION="needed for merged pdf output with toc after cover"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </CreateTocPdfInsertAtRliIndexAdjustBmOffset>
          <DEFAULT_ALIGN
            DEFAULT="left"
            TYPE="TEXT"
            />
          <DEFAULT_COLOR
            DEFAULT="#000000"
            TYPE="TEXT"
            />
          <DEFAULT_FONT
            DEFAULT="Helvetica"
            TYPE="TEXT"
            />
          <DEFAULT_SIZE
            DEFAULT="20"
            TYPE="TEXT"
            />
          <EscapeNewlinesAndBackslashesParamList
            DESCRIPTION="blank separated list of dpf parameters. \\n and \\ will be escaped in values of each parameter in the list."
            TYPE="TEXT"
            />
          <FilterOperator
            DESCRIPTION="optional operator to compare value(s), default: eq"
            TYPE="TEXT"
            />
          <FilterOperator2
            DESCRIPTION="second optional operator to compare value(s), default: eq"
            TYPE="TEXT"
            />
          <FilterPrefix
            DESCRIPTION="rlifilter prefix, if none was passed to the rlifilter object. if a prefix was defined either way, it must be inserted before rlifilter property names: &lt;Prefix&gt;RlicFilter"
            TYPE="TEXT"
            />
          <FilterValue
            DESCRIPTION="colon separated values to work on these specific RLI lines only"
            TYPE="TEXT"
            />
          <FilterValue2
            DESCRIPTION="second set of colon separated values to work on these specific RLI lines only"
            TYPE="TEXT"
            />
          <ImageList
            DESCRIPTION="optional list of images for all pages"
            TYPE="TEXT"
            />
          <ImageListFirstPage
            DESCRIPTION="optional list of images only for first page"
            TYPE="TEXT"
            />
          <JOBDIR
            TYPE="TEXT"
            />
          <LabelList
            DESCRIPTION="optional list of label texts for all pages"
            TYPE="TEXT"
            />
          <LabelListFirstPage
            DESCRIPTION="optional list of label texts only for first page"
            TYPE="TEXT"
            />
          <RLIC_BMTARGETPAGE
            TYPE="TEXT"
            />
          <RLIC_BookmarkName
            TYPE="TEXT"
            />
          <RLIC_CHAPTERDEPTH
            TYPE="TEXT"
            />
          <RLIC_CHAPTERNAME1
            TYPE="TEXT"
            />
          <RLIC_CHAPTERNAME2
            TYPE="TEXT"
            />
          <RLIC_ExternalLinkFileName
            DEFAULT="RelFileName"
            DESCRIPTION="optional name of RLI column to use for external PDF link"
            TYPE="TEXT"
            />
          <RLIC_PageNumber
            TYPE="TEXT"
            />
          <RlicFilter
            DESCRIPTION="name of RLI column to use to filter specific RLI lines only"
            TYPE="TEXT"
            />
          <RlicFilter2
            DESCRIPTION="name of second RLI column to use to filter specific RLI lines only"
            TYPE="TEXT"
            />
          <RlicKproUrl
            DEFAULT="KproUrl"
            DESCRIPTION="optional name of RLI column to use for URL link instead of value of RLIC_ExternalLinkFileName"
            TYPE="TEXT"
            />
          <SPRACHMERKMAL
            TYPE="TEXT"
            />
          <TOC_TITLE
            TYPE="TEXT"
            />
          <TargetRliFileName
            DEFAULT="%JobDir%/%WFPOSITION%.rli"
            DESCRIPTION="target rli file"
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WU_ENABLE_DEBUG_LOOP>
        </INPUT>
        <OUTPUT/>
      </Publish.createContents>
    </workingunits>
  </config>
</DPF>
