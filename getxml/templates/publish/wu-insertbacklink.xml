<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Publish.insertBackLink
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/publish/insertbacklink.pl -parfile %PARFILENAME%"
        DPFINT-FileLinks="doc://:/%DPFSRV%/scripts/publish/insertbacklink.pl"
        DPFINT-Icon="doc://:/publish.insertbacklink.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Insert back link to index"
        NODENAMES=""
        PROJECT="Publish"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-insertbacklink.xml,v 1.7 2011/01/14 14:20:48 sek Exp $"
        >
        <INPUT>
          <BACKLINKCOLOR
            TYPE="TEXT"
            />
          <BACKLINKTEXT
            TYPE="TEXT"
            />
          <CURRFILENAME
            TYPE="TEXT"
            />
          <DESTFILENAME
            TYPE="TEXT"
            />
          <IndexFileName
            DESCRIPTION="name of index PDF, the path is ignored if given"
            TYPE="TEXT"
            />
          <JOBDIR
            TYPE="TEXT"
            />
          <RELFILENAME
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </Publish.insertBackLink>
    </workingunits>
  </config>
</DPF>
