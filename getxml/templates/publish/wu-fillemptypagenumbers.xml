<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Publish.fillEmptyPageNumbers
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/publish/fillemptypagenumbers.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Fill Empty Page Numbers"
        NODENAMES=""
        PROJECT="Publish"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-fillemptypagenumbers.xml,v 1.2 2010/05/11 15:04:53 stefan Exp $"
        >
        <DEFAULTS/>
        <INPUT>
          <CURRRLIFILENAME
            DESCRIPTION="input rli"
            TYPE="TEXT"
            />
          <FillColumn
            DESCRIPTION="Name of the column to be filled"
            TYPE="TEXT"
            />
          <FillColumn2
            DESCRIPTION="Name of optional 2nd column to be filled"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </Publish.fillEmptyPageNumbers>
    </workingunits>
  </config>
</DPF>
