<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <Publish.createCoverSheet
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/publish/createcoversheet.pl -parfile %PARFILENAME%"
        DPFINT-FileLinks="doc://:/%DPFSRV%/scripts/publish/createcoversheet.pl"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Create Cover Sheet"
        PROJECT="Publish"
        Status="Running"
        THREADS="2"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-createcoversheet.xml,v 1.8 2011/02/08 16:30:33 sek Exp $"
        >
        <DEFAULTS
          DEFAULT_ALIGN="left"
          DEFAULT_COLOR="#000000"
          DEFAULT_FONT="Helvetica"
          DEFAULT_SIZE="20"
          DPFINT-Icon="doc://:/defaults.gif"
          WuEnableDebugLoop="N"
          />
        <INPUT
          DEFAULT_ALIGN="*"
          DEFAULT_COLOR="*"
          DEFAULT_FONT="*"
          DEFAULT_SIZE="*"
          DPFINT-Icon="doc://:/wu-input.gif"
          >
          <COVER_BACKGROUND_PDF
            TYPE="TEXT"
            />
          <COVER_FILLDUPLEX
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </COVER_FILLDUPLEX>
          <DEFAULT_ALIGN
            DEFAULT="left"
            TYPE="TEXT"
            />
          <DEFAULT_COLOR
            DEFAULT="#000000"
            TYPE="TEXT"
            />
          <DEFAULT_FONT
            DEFAULT="Helvetica"
            TYPE="TEXT"
            />
          <DEFAULT_SIZE
            DEFAULT="20"
            TYPE="TEXT"
            />
          <DestFileName
            TYPE="TEXT"
            />
          <IMAGE_LIST
            DESCRIPTION="list of parameter names for images"
            TYPE="TEXT"
            />
          <LABEL_LIST
            DESCRIPTION="list of parameter names for text labes"
            TYPE="TEXT"
            />
          <PDFARRANGE
            TYPE="TEXT"
            />
          <WuEnableDebugLoop
            DEFAULT="N"
            DESCRIPTION="To run this wu in a debug loop."
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WuEnableDebugLoop>
          <xxx
            TYPE="TEXT"
            />
          <xxx_ALIGN
            TYPE="TEXT"
            />
          <xxx_COLOR
            TYPE="TEXT"
            />
          <xxx_FONT
            TYPE="TEXT"
            />
          <xxx_SIZE
            TYPE="TEXT"
            />
          <xxx_X
            TYPE="TEXT"
            />
          <xxx_Y
            TYPE="TEXT"
            />
        </INPUT>
      </Publish.createCoverSheet>
    </workingunits>
  </config>
</DPF>
