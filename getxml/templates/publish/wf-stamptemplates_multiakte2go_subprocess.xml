<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <Publish.stampTemplates_multiakte2go_subprocess
        COMMENT=""
        DPFINT-Type="WorkflowDescription"
        NAME="Stamp Templates (Multiakte 2 Go Subprocess)"
        PROJECT="Publish"
        VERSION="$Id: wf-stamptemplates_multiakte2go_subprocess.xml,v 1.1 2009/11/20 13:47:51 sek Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <IfExistsTemplatePdfStpFile
          COMMENT=""
          ELSE="UseEmptyTemplate"
          OPERATOR="-f"
          POSX="500"
          POSY="300"
          PROPERTY="CURRFILENAME"
          THEN="IfExistsTemplateStpFile"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IfExistsTemplatePdfStpFile>
        <IfExistsTemplateStpFile
          COMMENT=""
          ELSE="UseEmptyTemplateCOPY"
          OPERATOR="-f"
          POSX="500"
          POSY="500"
          PROPERTY="TemplateStpFile"
          THEN="pdfstamp1"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IfExistsTemplateStpFile>
        <MAIN
          BEGIN="SetTemplateFiles"
          NAME=""
          />
        <MAPPING/>
        <SetTemplateFiles
          COMMENT=""
          NEXT="IfExistsTemplatePdfStpFile"
          POSX="500"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%DPFSRV%/conf/multiakte2go/%TEMPLATE%.pdf"
            TOC_ENTRY="[%TEMPLATE%] %TOC_ENTRY%"
            TemplateStpFile="%DPFSRV%/conf/multiakte2go/%TEMPLATE%.stp"
            TocColor="#990000"
            />
        </SetTemplateFiles>
        <UseEmptyTemplate
          COMMENT=""
          NEXT="IfExistsTemplateStpFile"
          POSX="700"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%DPFSRV%/conf/multiakte2go/empty.pdf"
            TEMPLATE="empty"
            />
        </UseEmptyTemplate>
        <UseEmptyTemplateCOPY
          COMMENT=""
          NEXT="pdfstamp1"
          POSX="700"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%DPFSRV%/conf/multiakte2go/empty.pdf"
            TEMPLATE="empty"
            />
        </UseEmptyTemplateCOPY>
        <pdfstamp1
          COMMENT=""
          ERROR="END"
          NAME="pdfstamp"
          NEXT="END"
          POSX="500"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <DPF4C_MODIFY_STAMP_N_TEXT0
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT1
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT2
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT3
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT4
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT5
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT6
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT7
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT8
              val=""
              />
            <DPF4C_MODIFY_STAMP_N_TEXT9
              val=""
              />
            <DPF4C_PDFSTAMP_DB_FILE
              val="default.db"
              />
            <DPF4C_PDFSTAMP_EXT
              val=""
              />
            <DPF4C_PDFSTAMP_STP_FILE
              val="default.stp"
              />
            <DPF4C_PDFSTAMP_TXT_FILE
              val="%PLSROOT%tools/convcfg/pdfstamp/default.txt"
              />
            <DPF4C_STAMP_DB_FILE
              val=""
              />
            <DPF4C_STAMP_STP_FILE
              val=""
              />
            <USE_DEFAULT_TXT_FILE
              val="N"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            DPF4C_MODIFY_STAMP_N_TEXT0="%ERR_REASON%"
            DPF4C_PDFSTAMP_DB_FILE="%DPFSRV%/conf/multiakte2go/default.db"
            DPF4C_PDFSTAMP_STP_FILE="%DPFSRV%/conf/multiakte2go/%TEMPLATE%.stp"
            />
        </pdfstamp1>
      </Publish.stampTemplates_multiakte2go_subprocess>
    </workflows>
  </config>
</DPF>
