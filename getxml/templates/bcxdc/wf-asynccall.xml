<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <BCXDC.asynccall
        COMMENT=""
        DPFINT-Type="WorkflowDescription"
        NAME="Asynccall"
        PROJECT="BCXDC"
        VERSION="$Id: wf-asynccall.xml,v 1.8 2010/02/11 09:41:28 dh Exp $"
        >
        <BCXDC.UserPostBcxdcAsynccall
          COMMENT=""
          ERROR="get_jobstateC"
          NEXT="plm_checkin_files_sap_directC"
          POSX="1600"
          POSY="500"
          TYPE="SWF"
          USE="BCXDC.UserPostBcxdcAsynccall"
          >
          <CONNECTIONS
            ERROR="HORIZONTAL"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </BCXDC.UserPostBcxdcAsynccall>
        <BCXDC.UserPostCheckinBcxdcAsynccall
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="2000"
          POSY="400"
          TYPE="SWF"
          USE="BCXDC.UserPostCheckinBcxdcAsynccall"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </BCXDC.UserPostCheckinBcxdcAsynccall>
        <BCXDC.UserPreBcxdcAsynccall
          COMMENT=""
          ERROR="get_jobstateC"
          NEXT="bcxdc.buildrli1"
          POSX="800"
          POSY="500"
          TYPE="SWF"
          USE="BCXDC.UserPreBcxdcAsynccall"
          >
          <CONNECTIONS
            ERROR="HORIZONTAL"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </BCXDC.UserPreBcxdcAsynccall>
        <BCXDC.initandmapvalues
          COMMENT=""
          ERROR="get_jobstateC"
          NEXT="check_for_user_pre_bcxdc_asynccall"
          POSX="600"
          POSY="200"
          TYPE="SWF"
          USE="BCXDC.initandmapvalues"
          >
          <CONNECTIONS
            ERROR="HORIZONTAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </BCXDC.initandmapvalues>
        <DEFAULTS/>
        <DPF4CONVERT.readcustomconfig
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.readcustomconfig"
          NEXT="BCXDC.initandmapvalues"
          POSX="400"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            DPF4C_CUSTOM_CONFIG="bcxdc::asynccall"
            />
        </DPF4CONVERT.readcustomconfig>
        <END
          COMMENT=""
          NAME="END"
          POSX="2300"
          POSY="800"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="2000"
          POSY="200"
          PROPERTY="SWITCH_DO_USER_POSTCHECKIN_BCXDC_ASYNCCALL"
          THEN="BCXDC.UserPostCheckinBcxdcAsynccall"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <MAIN
          BEGIN="setup_asynccall"
          NAME=""
          />
        <MAPPING/>
        <MODIFY_Filenames_for_checkin
          COMMENT=""
          NEXT="check_for_user_post_bcxdc_asynccall"
          POSX="1400"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%DPF4C_COMMON_OUTFILE1%"
            />
        </MODIFY_Filenames_for_checkin>
        <Rli.convertFiles
          COMMENT=""
          ERROR="get_jobstateC"
          NAME="Rli.convertFiles"
          NEXT="MODIFY_Filenames_for_checkin"
          POSX="1200"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="HORIZONTAL"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <WuEnableDebugLoop
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            DpfDebugSubJobs="Y"
            SUB_WFNAME="bcxdc_subconvert"
            />
        </Rli.convertFiles>
        <bcxdc.buildrli1
          COMMENT=""
          ERROR="get_jobstateC"
          NAME="bcxdc.buildrli"
          NEXT="Rli.convertFiles"
          POSX="1000"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="HORIZONTAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </bcxdc.buildrli1>
        <check_for_user_post_bcxdc_asynccall
          COMMENT=""
          ELSE="plm_checkin_files_sap_directC"
          OPERATOR="eq"
          POSX="1600"
          POSY="200"
          PROPERTY="SWITCH_DO_USER_POST_BCXDC_ASYNCCALL"
          THEN="BCXDC.UserPostBcxdcAsynccall"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_user_post_bcxdc_asynccall>
        <check_for_user_pre_bcxdc_asynccall
          COMMENT=""
          ELSE="bcxdc.buildrli1"
          OPERATOR="eq"
          POSX="800"
          POSY="200"
          PROPERTY="SWITCH_DO_USER_PRE_BCXDC_ASYNCCALL"
          THEN="BCXDC.UserPreBcxdcAsynccall"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_user_pre_bcxdc_asynccall>
        <check_for_zero_conv
          COMMENT=""
          ELSE="modify_return_single_errors"
          OPERATOR="eq"
          POSX="1300"
          POSY="600"
          PROPERTY="ZERO_CONV_ERRORS"
          THEN="modify_return_single_errorsC"
          TYPE="IF-THEN-ELSE"
          VALUE="X"
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </check_for_zero_conv>
        <get_jobstateC
          COMMENT=""
          ERROR="END"
          NAME="get_jobstate"
          NEXT="check_for_zero_conv"
          POSX="1300"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </get_jobstateC>
        <modify_return_single_errors
          COMMENT=""
          NEXT="send_error_message_sap_direct1"
          POSX="1100"
          POSY="700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            CHECK_DPF4C_COMMON_OUTFILES_FOR_SEND_OK_AND_ERROR="Y"
            />
        </modify_return_single_errors>
        <modify_return_single_errorsC
          COMMENT=""
          NEXT="send_error_message_sap_direct1"
          POSX="1500"
          POSY="700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            CHECK_DPF4C_COMMON_OUTFILES_FOR_SEND_OK_AND_ERROR="Y"
            />
        </modify_return_single_errorsC>
        <plm_checkin_files_sap_directC
          COMMENT=""
          ERROR="get_jobstateC"
          NAME="plm_checkin_files_sap_direct"
          NEXT="IF-THEN-ELSE1"
          POSX="1800"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="HORIZONTAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </plm_checkin_files_sap_directC>
        <send_error_message_sap_direct1
          COMMENT=""
          ERROR="END"
          NAME="send_error_message_sap_direct"
          NEXT="END"
          POSX="1300"
          POSY="800"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <CADRFC_DIRECTORY
              val="%PLSSV%/sapserv/bin_%PLS_OSFULLNAME%"
              />
            <CHECK_DPF4C_COMMON_OUTFILES_FOR_SEND_OK_AND_ERROR
              val="N"
              />
            <SAPRFC_FILE
              val="%PLSSV%/sapserv/conf/saprfc.ini"
              />
            <USE_JSAPCLI
              val="Y"
              />
          </DEFAULTS>
          <MAPPING/>
        </send_error_message_sap_direct1>
        <setup_asynccall
          COMMENT=""
          NEXT="DPF4CONVERT.readcustomconfig"
          POSX="200"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            JOBNAME="SPOOLREQUEST=%SPOOLREQUEST%"
            />
        </setup_asynccall>
      </BCXDC.asynccall>
    </workflows>
  </config>
</DPF>
