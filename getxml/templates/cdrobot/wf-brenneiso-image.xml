<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <BrenneISO-Image
        COMMENT=""
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        INTERNALNAME="BrenneISO-Image"
        NAME="Brenne ISO-Imagedatei auf CD nur mit Hintergrundbild"
        PROJECT="CdRobot"
        VERSION="$Id: wf-brenneiso-image.xml,v 1.3 2006/12/15 16:25:07 stefan Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="300"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="initEmptyCdLabel"
          COMMENT="Brenne CD"
          DPFINT-Interactive="1"
          PROJECT="CdRobot"
          VERSION="$Id: wf-brenneiso-image.xml,v 1.3 2006/12/15 16:25:07 stefan Exp $"
          />
        <UserInterface
          ORDER="IMAGEFILE CD_LABEL TOAST_NUM_COPIES SIMULATE"
          >
          <CD_LABEL
            DEFAULT=""
            RANGE="JPG (*.jpg)|PDF (*.pdf)|PNG (*.png)|TIFF (*.tif)|GIF (*.gif)|Alle (*.*)"
            TYPE="File"
            />
          <DPF_SRV_GET_FILES
            DEFAULT="CD_LABEL IMAGEFILE"
            RANGE=""
            TYPE="Hidden"
            />
          <IMAGEFILE
            DEFAULT="Bitte ISO-Imagedatei wählen"
            RANGE="ISO (*.iso)|Alle (*.*)"
            TYPE="File"
            />
          <PLS_SRCNODE
            DEFAULT="UNKNOWN"
            RANGE=""
            TYPE="Hidden"
            />
          <PLS_USERNAME
            DEFAULT="UNKNOWN"
            RANGE=""
            TYPE="Hidden"
            />
          <SIMULATE
            DEFAULT="N"
            RANGE="Y|N"
            TYPE="Check"
            />
          <TOAST_NUM_COPIES
            DEFAULT="1"
            RANGE="1,25"
            TYPE="Spin"
            />
        </UserInterface>
        <buildcd
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="100"
          POSY="300"
          TYPE="SWF"
          USE="CdRobot.buildCd"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </buildcd>
        <initEmptyCdLabel
          COMMENT=""
          NEXT="buildcd"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            INIT_LABEL_BACKGROUND="N"
            INIT_LABEL_LOGO="N"
            INIT_LABEL_TEXT="N"
            JOBNAME="ISO Image"
            LABEL_BACKGROUND=""
            LABEL_PDF_LOGO=""
            LABEL_TEXT_LABELS="- - - - -"
            LABEL_TEXT_PROPERTIES=""
            />
        </initEmptyCdLabel>
      </BrenneISO-Image>
    </workflows>
  </config>
</DPF>
