<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <CdRobot.createIsoFile
        COMMENT="progress 40...60"
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Create ISO File"
        PROJECT="CdRobot"
        VERSION="$Id: wf-createisofile.xml,v 1.6 2009/05/22 05:37:15 wolfi Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="recalculateSize"
          NAME=""
          />
        <MAPPING/>
        <compressTarGz
          COMMENT=""
          ERROR="END"
          NAME="FileCompress.compressGZIP"
          NEXT="set50"
          POSX="400"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <NAME
              val="DEFAULTS"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </compressTarGz>
        <createCdImage
          COMMENT=""
          ERROR="END"
          NAME="CdRobot.createCdImage"
          NEXT="END"
          POSX="200"
          POSY="1200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            MKISO_JOLIET="Y"
            />
          <MAPPING/>
        </createCdImage>
        <ifTarTooBig
          COMMENT=""
          ELSE="compressTarGz"
          OPERATOR="le"
          POSX="200"
          POSY="500"
          PROPERTY="TREESIZE"
          THEN="createCdImage"
          TYPE="IF-THEN-ELSE"
          VALUE="%MAXCDSIZE%"
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </ifTarTooBig>
        <recalculateCompressedSize
          COMMENT=""
          ERROR="END"
          NAME="FileSystem.getTreeSize"
          NEXT="createCdImage"
          POSX="400"
          POSY="1100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="HORIZONTAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </recalculateCompressedSize>
        <recalculateSize
          COMMENT=""
          ERROR="END"
          NAME="FileSystem.getTreeSize"
          NEXT="set45"
          POSX="200"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </recalculateSize>
        <set45
          COMMENT=""
          NEXT="ifTarTooBig"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF_JOB_PROGRESS="45"
            />
        </set45>
        <set50
          COMMENT=""
          NEXT="recalculateCompressedSize"
          POSX="400"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF_JOB_PROGRESS="50"
            />
        </set50>
      </CdRobot.createIsoFile>
    </workflows>
  </config>
</DPF>
