<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <BrenneKundenCD
        COMMENT=""
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        INTERNALNAME="BrenneKundenCD"
        NAME="Brenne Verzeichnis für Kundenauftrag auf CD"
        PROJECT="CdRobot"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="300"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="initKundenCD"
          COMMENT="Brennt eine Kunden-CD"
          DPFINT-Interactive="1"
          ERROR="END"
          PROJECT="CdRobot"
          VERSION="$Id: wf-brennekundencd.xml,v 1.8 2006/12/15 16:25:07 stefan Exp $"
          />
        <UserInterface
          ORDER="TARROOTDIR CD_LABEL TOAST_NUM_COPIES MKISO_VOLUME_ID DO_TAR WANT_UNIX LABEL_HELPER CUSTOMER SOFTWARE PLATFORM AUFTRAG SIMULATE"
          >
        <CD_LABEL
          TYPE = "File"
          RANGE = "JPG (*.jpg)|PDF (*.pdf)|PNG (*.png)|TIFF (*.tif)|GIF (*.gif)|Alle (*.*)"
          DEFAULT = ""
          />
        <SIMULATE
          RANGE="Y|N"
          TYPE="Check"
          DEFAULT="N"
          />
        <DPF_SRV_GET_FILES
          RANGE=""
          TYPE="Hidden"
          DEFAULT="CD_LABEL"
          />
        <PLS_USERNAME
          RANGE=""
          TYPE="Hidden"
          DEFAULT="UNKNOWN"
          />
        <PLS_SRCNODE
          RANGE=""
          TYPE="Hidden"
          DEFAULT="UNKNOWN"
          />
        <TOAST_NUM_COPIES
          RANGE="1,25"
          TYPE="Spin"
          DEFAULT="1"
          />
        <TARROOTDIR
          RANGE="59"
          TYPE="Dir"
          DEFAULT="Bitte Verzeichnis wählen"
          />
          <LABEL_HELPER
            DEFAULT=""
            RANGE="59"
            TYPE="Static"
            />
          <AUFTRAG
            DEFAULT=""
            RANGE="59"
            TYPE="Edit"
            />
          <CUSTOMER
            DEFAULT=""
            RANGE="59"
            TYPE="Edit"
            />
          <DO_TAR
            DEFAULT="Y"
            RANGE="Y|N"
            TYPE="Check"
            />
          <MKISO_VOLUME_ID
            DEFAULT="SEAL"
            RANGE="12"
            TYPE="Edit"
            />
          <PLATFORM
            DEFAULT=""
            RANGE="59"
            TYPE="Edit"
            />
          <SOFTWARE
            DEFAULT=""
            RANGE="59"
            TYPE="Edit"
            />
          <WANT_UNIX
            DEFAULT="N"
            RANGE="Y|N"
            TYPE="Check"
            />
        </UserInterface>
        <buildcd
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="100"
          POSY="300"
          TYPE="SWF"
          USE="CdRobot.buildWithClientUI"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </buildcd>
        <initKundenCD
          COMMENT=""
          NEXT="buildcd"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            LABEL_TEXT_LABELS="Kunde Software Plattform Auftrag"
            LABEL_TEXT_PROPERTIES="CUSTOMER  Software Platform Auftrag Datum"
            />
        </initKundenCD>
      </BrenneKundenCD>
    </workflows>
  </config>
</DPF>
