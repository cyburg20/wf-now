<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <CdRobot.buildWithClientUI
        COMMENT="Typical CD creation workflow for dpfclientui"
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        INTERNALNAME="CdRobot.buildWithClientUI"
        NAME="Build with dpfclientui"
        PROJECT="CdRobot"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="500"
          POSY="500"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="ifHaveSoftware"
          COMMENT="Helper WF fuer alle dpfclientui Auftraege"
          PROJECT="CdRobot"
          VERSION="$Id: wf-buildwithclientui.xml,v 1.7 2007/03/15 09:06:18 stefan Exp $"
          />
        <buildcd
          COMMENT="call general CD creation workflow"
          ERROR="END"
          NEXT="END"
          POSX="200"
          POSY="1100"
          TYPE="SWF"
          USE="CdRobot.buildCd"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </buildcd>
        <clientNetUse
          COMMENT="call net use on client machine"
          ERROR="END"
          NAME="CdRobot.clientNetUse"
          NEXT="setnetusefile"
          POSX="400"
          POSY="600"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </clientNetUse>
        <ifHaveSoftware
          COMMENT=""
          ELSE="ifHaveSoftwareCOPY1"
          OPERATOR="exists"
          POSX="200"
          POSY="100"
          PROPERTY="SOFTWARE"
          THEN="useSoftware"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </ifHaveSoftware>
        <ifHaveSoftwareCOPY1
          COMMENT=""
          ELSE="clientNetUse"
          OPERATOR="exists"
          POSX="400"
          POSY="300"
          PROPERTY="MKISO_VOLUME_ID"
          THEN="useVolumeId"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </ifHaveSoftwareCOPY1>
        <setnetusefile
          COMMENT=""
          NEXT="buildcd"
          POSX="400"
          POSY="800"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%NET_USE_FILE%"
            />
        </setnetusefile>
        <useSoftware
          COMMENT=""
          NEXT="clientNetUse"
          POSX="200"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            JOBNAME="%SOFTWARE%"
            />
        </useSoftware>
        <useVolumeId
          COMMENT=""
          NEXT="clientNetUse"
          POSX="600"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            JOBNAME="%MKISO_VOLUME_ID%"
            />
        </useVolumeId>
      </CdRobot.buildWithClientUI>
    </workflows>
  </config>
</DPF>
