<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <BrenneConvertWIZ
        COMMENT=""
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        INTERNALNAME="BrenneConvertWIZ"
        NAME="Brenne Verzeichnis für ConvertWIZ auf CD"
        PROJECT="CdRobot"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="300"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="initConvertWIZLabel"
          COMMENT="Brennt eine CD mit ConvertWIZ Label"
          DPFINT-Interactive="1"
          ERROR="END"
          PROJECT="CdRobot"
          VERSION="$Id: wf-brenneconvertwiz.xml,v 1.8 2006/12/15 16:25:07 stefan Exp $"
          />
        <UserInterface
          ORDER="TARROOTDIR TOAST_NUM_COPIES MKISO_VOLUME_ID LABEL_HELPER VERSION REVISION SIMULATE"
          >
        <SIMULATE
          RANGE="Y|N"
          TYPE="Check"
          DEFAULT="N"
          />
        <PLS_USERNAME
          RANGE=""
          TYPE="Hidden"
          DEFAULT="UNKNOWN"
          />
        <PLS_SRCNODE
          RANGE=""
          TYPE="Hidden"
          DEFAULT="UNKNOWN"
          />
        <TOAST_NUM_COPIES
          RANGE="1,25"
          TYPE="Spin"
          DEFAULT="1"
          />
        <TARROOTDIR
          RANGE="59"
          TYPE="Dir"
          DEFAULT="Bitte Verzeichnis wählen"
          />
          <MKISO_VOLUME_ID
            DEFAULT="ConvertWIZ"
            RANGE="12"
            TYPE="Edit"
            />
          <LABEL_HELPER
            DEFAULT=""
            RANGE="59"
            TYPE="Static"
            />
          <REVISION
            DEFAULT="1.12"
            RANGE="10"
            TYPE="Edit"
            />
          <VERSION
            DEFAULT="1.1.1"
            RANGE="10"
            TYPE="Edit"
            />
        </UserInterface>
        <buildcd
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="100"
          POSY="300"
          TYPE="SWF"
          USE="CdRobot.buildWithClientUI"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </buildcd>
        <initConvertWIZLabel
          COMMENT=""
          NEXT="buildcd"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            INIT_LABEL_BACKGROUND="N"
            INIT_LABEL_LOGO="N"
            INIT_LABEL_TEXT="N"
            LABEL_BACKGROUND="ConvertWIZ_Label.pdf"
            LABEL_PDF_LOGO=""
            LABEL_TEXT_COLOR="#ffffff"
            LABEL_TEXT_LABELS="- - -"
            LABEL_TEXT_OFFSET_X="-5.3 cm"
            LABEL_TEXT_OFFSET_Y="0.2 cm"
            LABEL_TEXT_PROPERTIES="COMBI_VERSION REVISION Datum"
            LABEL_TEXT_SHADOW="Y"
            LABEL_TEXT_SHADOW_COLOR="#000000"
            SOFTWARE="ConvertWIZ"
            />
        </initConvertWIZLabel>
      </BrenneConvertWIZ>
    </workflows>
  </config>
</DPF>
