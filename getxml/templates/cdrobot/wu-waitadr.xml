<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <CdRobot.waitAdr
        COMMAND="%DPFSRV%/scripts/cdrobot/waitadr.pl -parfile %PARFILENAME%"
