<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <CdRobot.Settings
        COMMENT="All settings for CD creation workflows"
        DPFINT-Icon="doc://:/wf.gif"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Settings"
        PROJECT="CdRobot"
        VERSION="$Id: wf-settings.xml,v 1.12 2011/05/04 09:01:46 stefan Exp $"
        >
        <DEFAULTS
          DPFINT-Icon="doc://:/defaults.gif"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="400"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="init"
          NAME=""
          />
        <MAPPING/>
        <ifLabelInit
          COMMENT=""
          ELSE="setLabelBasics"
          OPERATOR="exists"
          POSX="200"
          POSY="300"
          PROPERTY="INIT_LABEL_BASICS"
          THEN="ifLabelInitText"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </ifLabelInit>
        <ifLabelInitBackground
          COMMENT=""
          ELSE="setLabelBackground"
          OPERATOR="exists"
          POSX="200"
          POSY="1200"
          PROPERTY="INIT_LABEL_BACKGROUND"
          THEN="ifLabelInitLogo"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </ifLabelInitBackground>
        <ifLabelInitLogo
          COMMENT=""
          ELSE="setLabelLogo"
          OPERATOR="exists"
          POSX="200"
          POSY="1500"
          PROPERTY="INIT_LABEL_LOGO"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </ifLabelInitLogo>
        <ifLabelInitText
          COMMENT=""
          ELSE="ifLabelTextPropertiesDefined"
          OPERATOR="exists"
          POSX="200"
          POSY="600"
          PROPERTY="INIT_LABEL_TEXT"
          THEN="ifLabelInitBackground"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </ifLabelInitText>
        <ifLabelTextPropertiesDefined
          COMMENT=""
          ELSE="setLabelTextProperties"
          OPERATOR="exists"
          POSX="500"
          POSY="700"
          PROPERTY="LABEL_TEXT_PROPERTIES"
          THEN="setLabelText"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </ifLabelTextPropertiesDefined>
        <init
          COMMENT="customize your values here.
The SEAL_ values are used if SEAL inhouse switch is enabled.
 CD: 716800"
          NEXT="ifLabelInit"
          POSX="200"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFSERVERNAME="%COMPUTERNAME%"
            DPFTRACKERURL="http://%COMPUTERNAME%/cgi-bin/dpftracker"
            DPFWORKCLIENT="\\%COMPUTERNAME%\dpfwork"
            DPFWORKSERVER="%DPFDATA%/dpfwork"
            JOBUSERNAME="%ORIGUSERNAME%"
            LDAP_ATTRIBUTES="initials mail cn"
            LDAP_BASEDN="ou=users,o=SEAL"
            LDAP_HOSTNAME=""
            LDAP_SCOPE="one"
            LDAP_SEARCHSTRING="uid=%USER%"
            MAIL_DPFADMIN="nobody@nowhere.com"
            MAIL_FROM="cd-robot@nowhere.com"
            MAIL_SMTP=""
            MAXCDSIZE="4700000"
            NOVELL_HOSTNAMES=""
            SAMBA_HOSTNAMES=""
            SEAL_DEFAULT_MAIL="twiki@sealsystems.de"
            SEAL_DPFTRACKERURL="http://%COMPUTERNAME%/cgi-bin/dpftracker"
            SEAL_FTP_HOSTNAME="roettlin6"
            SEAL_FTP_PASSWORD="Pepsi02"
            SEAL_FTP_USERNAME="dpf"
            SEAL_HOST_MAP_FILE="%DPFSRV%/conf/CdRobot/seal-node-email-map.txt"
            SEAL_LDAP_ATTRIBUTES="initials mail cn"
            SEAL_LDAP_BASEDN="ou=users,o=SEAL"
            SEAL_LDAP_HOSTNAME="roettldap1"
            SEAL_LDAP_SCOPE="one"
            SEAL_LDAP_SEARCHSTRING="uid=%USER%"
            SEAL_MAIL_DPFADMIN="twiki@sealsystems.de"
            SEAL_MAIL_FROM="cd-robot@sealsystems.de"
            SEAL_MAIL_IGNORE_ERRORS="Y"
            SEAL_MAIL_SMTP="192.168.99.2"
            SEAL_NOVELL_HOSTNAMES="NW_SERVER_1 CDSERV1_NW"
            SEAL_RSH_HOSTNAME="roettlin6"
            SEAL_RSH_USERNAME="dpf"
            SEAL_SAMBA_HOSTNAMES="ROETTFS1 SEPPFS1"
            SEAL_TOAST_NODENAME="CD-ROBOT"
            TOAST_NODENAME=""
            />
        </init>
        <setLabelBackground
          COMMENT=""
          NEXT="ifLabelInitLogo"
          POSX="400"
          POSY="1300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            LABEL_BACKGROUND="dpf-world-cdcover.jpg"
            SEAL_LABEL_BACKGROUND="labyrinth.jpg"
            />
        </setLabelBackground>
        <setLabelBasics
          COMMENT=""
          NEXT="ifLabelInitText"
          POSX="400"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            LABEL_CLIPPING="1.9 cm"
            LABEL_GS_PARAMS="-sDEVICE=jpeg -r600 -dJPEGQ=100"
            LABEL_TEXT_SIZE="12"
            SEAL_LABEL_CLIPPING="1.25 cm"
            />
        </setLabelBasics>
        <setLabelLogo
          COMMENT=""
          NEXT="END"
          POSX="400"
          POSY="1600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            SEAL_LABEL_PDF_LOGO="SEALLogo_BlauSchwarzSchatten.pdf"
            SEAL_LABEL_PDF_LOGO_OFFSET_X="-3.45 cm"
            SEAL_LABEL_PDF_LOGO_OFFSET_Y="3.0 cm"
            />
        </setLabelLogo>
        <setLabelText
          COMMENT=""
          NEXT="ifLabelInitBackground"
          POSX="500"
          POSY="1000"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            LABEL_CIRCLE_TEXT=""
            LABEL_INNER_CIRCLE_TEXT="Created with the Digital Process Factory by SEAL Systems.    www.sealsystems.com"
            LABEL_TEXT_SHADOW="Y"
            LABEL_TEXT_SIZE="12"
            SEAL_LABEL_TEXT_COLOR="#0D9BD5"
            SEAL_LABEL_TEXT_OFFSET_X="-1.5 cm"
            SEAL_LABEL_TEXT_OFFSET_Y="-2.4 cm"
            SEAL_LABEL_TEXT_SHADOW_COLOR="#000000"
            />
        </setLabelText>
        <setLabelTextProperties
          COMMENT=""
          NEXT="setLabelText"
          POSX="700"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            LABEL_TEXT_PROPERTIES="Empfänger Software Version Datum"
            />
        </setLabelTextProperties>
      </CdRobot.Settings>
    </workflows>
  </config>
</DPF>
