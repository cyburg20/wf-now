<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4Publish.Finish
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Finish"
        PROJECT="DPF4Publish"
        VERSION="$Id: wf-finish.xml,v 1.5 2010/04/23 14:25:22 sek Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <IfWantSapReply
          COMMENT="Reply the stati of all documents to sap?"
          ELSE="IfWantSendMail"
          OPERATOR="is true"
          POSX="700"
          POSY="100"
          PROPERTY="WantSapReply"
          THEN="SapReply"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IfWantSapReply>
        <IfWantSendMail
          COMMENT="Send an email?"
          ELSE="END"
          OPERATOR="is true"
          POSX="700"
          POSY="300"
          PROPERTY="WantSendMail"
          THEN="SendMail"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IfWantSendMail>
        <MAIN
          BEGIN="IfWantSapReply"
          NAME=""
          />
        <MAPPING/>
        <SapReply
          COMMENT="Reply the stati of all documents to sap."
          ERROR="END"
          NAME="Sap.replyStatusOfDocumentsInRli"
          NEXT="IfWantSendMail"
          POSX="900"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            FilterPrefix="%WFPOSITION%"
            />
        </SapReply>
        <SendMail
          COMMENT="Mail output file path."
          ERROR="END"
          NAME="Mail.sendMail"
          NEXT="END"
          POSX="900"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            MAIL_EXPAND_TEXT="N"
            NAME="DEFAULTS"
            >
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </SendMail>
      </DPF4Publish.Finish>
    </workflows>
  </config>
</DPF>
