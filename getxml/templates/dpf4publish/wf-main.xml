<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4Publish.Main
        COMMENT="Main entry point for DPF4Publish"
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="Main"
        PROJECT="DPF4Publish"
        VERSION="$Id: wf-main.xml,v 1.9 2010/10/21 15:50:32 stefan Exp $"
        >
        <BuildDocumentation
          COMMENT=""
          ERROR="ErrorHandling"
          NEXT="setProgress70"
          POSX="100"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4Publish.BuildDocumentation"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </BuildDocumentation>
        <DEFAULTS
          DPF4Publish.UserExitBeforeEnd="DPF4Publish.UserExitBeforeEnd"
          />
        <Distribute
          COMMENT=""
          ERROR="ErrorHandling"
          NEXT="setProgress90"
          POSX="100"
          POSY="1900"
          TYPE="SWF"
          USE="DPF4Publish.Distribute"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Distribute>
        <END
          COMMENT=""
          NAME="END"
          POSX="1100"
          POSY="2500"
          TYPE="STOP"
          />
        <ErrorHandling
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="600"
          POSY="1300"
          TYPE="SWF"
          USE="DPF4Publish.ErrorHandling"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ErrorHandling>
        <Initialize
          COMMENT=""
          ERROR="ErrorHandling"
          NEXT="setProgress10"
          POSX="100"
          POSY="300"
          TYPE="SWF"
          USE="DPF4Publish.Initialize"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Initialize>
        <MAIN
          BEGIN="setProgress0"
          NAME=""
          />
        <MAPPING/>
        <Notify
          COMMENT=""
          ERROR="ErrorHandling"
          NEXT="UserExitBeforeEnd"
          POSX="100"
          POSY="2300"
          TYPE="SWF"
          USE="DPF4Publish.Notify"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Notify>
        <Preprocess
          COMMENT=""
          ERROR="ErrorHandling"
          NEXT="setProgress50"
          POSX="100"
          POSY="1100"
          TYPE="SWF"
          USE="DPF4Publish.Preprocess"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Preprocess>
        <Retrieve
          COMMENT=""
          ERROR="ErrorHandling"
          NEXT="setProgress20"
          POSX="100"
          POSY="700"
          TYPE="SWF"
          USE="DPF4Publish.Retrieve"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </Retrieve>
        <UserExitBeforeEnd
          COMMENT=""
          ERROR="END"
          NEXT="setProgress100"
          POSX="100"
          POSY="2500"
          TYPE="USER_EXIST"
          USE="%DPF4Publish.UserExitBeforeEnd%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </UserExitBeforeEnd>
        <setProgress0
          COMMENT=""
          NEXT="Initialize"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobProgress="0"
            />
        </setProgress0>
        <setProgress10
          COMMENT=""
          NEXT="Retrieve"
          POSX="100"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobProgress="10"
            />
        </setProgress10>
        <setProgress100
          COMMENT=""
          NEXT="END"
          POSX="100"
          POSY="2700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobProgress="100"
            />
        </setProgress100>
        <setProgress20
          COMMENT=""
          NEXT="Preprocess"
          POSX="100"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobProgress="20"
            />
        </setProgress20>
        <setProgress50
          COMMENT=""
          NEXT="BuildDocumentation"
          POSX="100"
          POSY="1300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobProgress="50"
            />
        </setProgress50>
        <setProgress70
          COMMENT=""
          NEXT="Distribute"
          POSX="100"
          POSY="1700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobProgress="70"
            />
        </setProgress70>
        <setProgress90
          COMMENT=""
          NEXT="Notify"
          POSX="100"
          POSY="2100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DpfJobProgress="90"
            />
        </setProgress90>
      </DPF4Publish.Main>
    </workflows>
  </config>
</DPF>
