<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.pdfpostproc
        COMMENT="The DPF-process PDFPOSTPROC performs a post processing of a PDF-file. This includes optimize/remove elements, checking/creating PDF/A, set PDF-properties, set view preferences, attach an original file, enable reader extensions, apply PDF-protection and set a signature."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Pdfpostproc - Postprocess a PDF file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdfpostproc.xml,v 1.10 2016/02/04 16:29:50 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.Pdfsetmetadata1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.pdfsetviewpref1"
          POSX="700"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.Pdfsetmetadata"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.Pdfsetmetadata1>
        <DPF4CONVERT.dpf4c_user_pre_pdfpostproc1
          COMMENT=""
          ERROR="END"
          NEXT="check_enable"
          POSX="400"
          POSY="300"
          TYPE="SWF"
          USE="DPF4CONVERT.dpf4c_user_pre_pdfpostproc"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.dpf4c_user_pre_pdfpostproc1>
        <DPF4CONVERT.pdfadapt1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.Pdfsetmetadata1"
          POSX="500"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfadapt"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfadapt1>
        <DPF4CONVERT.pdfattachorigfile1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.pdfsetreaderext1"
          POSX="900"
          POSY="100"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfattachorigfile"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfattachorigfile1>
        <DPF4CONVERT.pdfoptimize1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.pdfadapt1"
          POSX="500"
          POSY="100"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfoptimize"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfoptimize1>
        <DPF4CONVERT.pdfprotect1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.pdfsign1"
          POSX="1100"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprotect"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfprotect1>
        <DPF4CONVERT.pdfsetreaderext1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.pdfprotect1"
          POSX="900"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfsetreaderext"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfsetreaderext1>
        <DPF4CONVERT.pdfsetviewpref1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.pdfattachorigfile1"
          POSX="700"
          POSY="100"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfsetviewpref"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfsetviewpref1>
        <DPF4CONVERT.pdfsign1
          COMMENT=""
          ERROR="END"
          NEXT="set_outfile_pdf"
          POSX="1100"
          POSY="100"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfsign"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfsign1>
        <DPF4CONVERT.readcustomconfig1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.readcustomconfig"
          NEXT="IF-THEN-ELSE1"
          POSX="100"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.readcustomconfig1>
        <END
          COMMENT=""
          NAME="END"
          POSX="1000"
          POSY="2100"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="check_enable"
          OPERATOR="eq"
          POSX="300"
          POSY="500"
          PROPERTY="DPF4C_SWITCH_USER_PRE_PDFPOSTPROC"
          THEN="DPF4CONVERT.dpf4c_user_pre_pdfpostproc1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <MAIN
          BEGIN="MODIFY1"
          NAME=""
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="check_read_custom_config"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_CUSTOM_CONFIG="dpf4convert::pdfpostproc"
            />
        </MODIFY1>
        <check_enable
          COMMENT=""
          ELSE="DPF4CONVERT.pdfoptimize1"
          OPERATOR="eq"
          POSX="300"
          POSY="100"
          PROPERTY="ENABLE_PDFPOSTPROC"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_enable>
        <check_read_custom_config
          COMMENT=""
          ELSE="DPF4CONVERT.readcustomconfig1"
          OPERATOR="eq"
          POSX="100"
          POSY="300"
          PROPERTY="DPF4C_READ_CUSTOM_CONFIG_PDFPOSTPROC"
          THEN="IF-THEN-ELSE1"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_read_custom_config>
        <set_outfile_pdf
          COMMENT=""
          NEXT="END"
          POSX="1300"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_OUTFILEpdf="%CURRFILENAME%"
            />
        </set_outfile_pdf>
      </DPF4CONVERT.pdfpostproc>
    </workflows>
  </config>
</DPF>
