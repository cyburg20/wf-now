<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <pdfcrypt_decrypt
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfcrypt.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        LOGLEVEL="LOG_INFO"
        LOGNAME="dpf4c-pdfcrypt_decrypt"
        LOGSTDOUT="Y"
        NAME="dpf4c - Decrypt PDF with pdfcrypt"
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-pdfcrypt_decrypt.xml,v 1.4 2011/01/04 14:30:55 kig Exp $"
        >
        <DEFAULTS
          DPF4C_CRYPT_ADDNOTES="Y"
          DPF4C_CRYPT_ASSEMBLE="Y"
          DPF4C_CRYPT_BRAILLE="Y"
          DPF4C_CRYPT_CHANGE="N"
          DPF4C_CRYPT_COPY_TEXT="Y"
          DPF4C_CRYPT_ENCRYTION128="N"
          DPF4C_CRYPT_FILLFIELDS="N"
          DPF4C_CRYPT_KEEP_FILENAME="N"
          DPF4C_CRYPT_LOWRES="N"
          DPF4C_CRYPT_PRINT="N"
          DPF4C_CRYPT_OPEN_PW=""
          DPF4C_CRYPT_CHANGE_PW=""
          DPF4C_CRYPT_USER_PW=""
          DPF4C_CRYPT_OWNER_PW=""
          DPF4C_DECRYPT="N"
          DPF4C_DECRYPTALL="N"
          DPF4C_DO_CRYPT="N"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          >
          <DPF4C_CRYPT_ADDNOTES
            DEFAULT="Y"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_ASSEMBLE
            DEFAULT="Y"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_BRAILLE
            DEFAULT="Y"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_CHANGE
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_CHANGE_PW
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_COPY_TEXT
            DEFAULT="Y"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_ENCRYTION128
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_FILLFIELDS
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_KEEP_FILENAME
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_LOWRES
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_OPEN_PW
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_OWNER_PW
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_PRINT
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_CRYPT_USER_PW
            TYPE="TEXT"
            />
          <DPF4C_DECRYPT
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_DECRYPTALL
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_DO_CRYPT
            DEFAULT="N"
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
      </pdfcrypt_decrypt>
    </workingunits>
  </config>
</DPF>
