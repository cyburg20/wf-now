<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.Convertdocumentssubconvert
        COMMENT=""
        DPFINT-Type="WorkflowDescription"
        NAME="Convertdocumentssubconvert"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-convertdocumentssubconvert.xml,v 1.5 2016/02/16 12:11:38 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.DPF4Convert_Convert2PDF
          COMMENT=""
          ERROR="END"
          NEXT="check_skip_copy_target"
          POSX="600"
          POSY="400"
          TYPE="SWF"
          USE="DPF4CONVERT.DPF4Convert_Convert2PDF"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.DPF4Convert_Convert2PDF>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <FileTransfer.copyFiles
          COMMENT=""
          ERROR="END"
          NAME="FileTransfer.copyFiles"
          NEXT="END"
          POSX="1000"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <COPY_DEBUG
              val="N"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            COPY_SOURCE_PROPERTIES="CURRFILENAME"
            COPY_TARGET_PROPERTIES="TARGETFILENAME"
            />
        </FileTransfer.copyFiles>
        <MAIN
          BEGIN="MODIFY_CONVERSION_PARAM"
          NAME=""
          />
        <MAPPING/>
        <MODIFY_CONVERSION_PARAM
          COMMENT=""
          NEXT="DPF4CONVERT.DPF4Convert_Convert2PDF"
          POSX="400"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ACTIVATE_STAMP="N"
            DPF4C_COMMON_DEST_FORMAT="pdf"
            SAVE_ORI_TARGETFORMAT="%TARGETFORMAT%"
            TARGETFORMAT="pdf"
            />
        </MODIFY_CONVERSION_PARAM>
        <check_skip_copy_target
          COMMENT=""
          ELSE="FileTransfer.copyFiles"
          OPERATOR="is true"
          POSX="800"
          POSY="400"
          PROPERTY="SWITCH_SKIP_COPY_TARGET"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_skip_copy_target>
      </DPF4CONVERT.Convertdocumentssubconvert>
    </workflows>
  </config>
</DPF>
