<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.work_on_filesets
        COMMENT="dpf4convert - Summarize processing steps for filesets
This process converts a single or multiple (HP) filesets to PDF and performs PDF-process, create target formats and PDF-postproc on the PDF-files."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Work On Filesets - Summarize processing Filesets"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-work_on_filesets.xml,v 1.3 2013/03/15 15:00:21 kig Exp $"
        >
        <CheckFilesetType
          CASE:hp="SetHpglConvertParams"
          CASE:pdf="SetPdfFilesetParams"
          CASE:plt="SetHpglConvertParams"
          CASE:ps="SetPsFilesetParams"
          COMMENT=""
          CONNECTION:DEFAULT="DIRECT"
          CONNECTION:hp="DIRECT"
          CONNECTION:pdf="VERTICAL"
          CONNECTION:plt="DIRECT"
          CONNECTION:ps="VERTICAL"
          DEFAULT="DPF4CONVERT.work_on_filesets"
          POSX="200"
          POSY="300"
          PROPERTY="DPF4C_FILESET_TYPE"
          TYPE="SWITCH"
          />
        <CheckMultipleFilesets
          COMMENT=""
          ELSE="END"
          OPERATOR="ge"
          POSX="700"
          POSY="100"
          PROPERTY="DPF4C_NUM_FILESETS"
          THEN="DPF4CONVERT.work_on_filesets"
          TYPE="IF-THEN-ELSE"
          VALUE="2"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </CheckMultipleFilesets>
        <CheckSingleFileset
          COMMENT=""
          ELSE="CheckMultipleFilesets"
          OPERATOR="eq"
          POSX="200"
          POSY="100"
          PROPERTY="DPF4C_NUM_FILESETS"
          THEN="CheckFilesetType"
          TYPE="IF-THEN-ELSE"
          VALUE="1"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </CheckSingleFileset>
        <DEFAULTS/>
        <DPF4CONVERT.work_on_filesets
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.work_on_filesets"
          NEXT="END"
          POSX="700"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.work_on_filesets>
        <DPF4CONVERT.work_on_hp_fileset
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="200"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.work_on_hp_fileset"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.work_on_hp_fileset>
        <DPF4CONVERT.work_on_pdf_fileset
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="400"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.work_on_pdf_fileset"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.work_on_pdf_fileset>
        <DPF4CONVERT.work_on_ps_fileset
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="600"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.work_on_ps_fileset"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.work_on_ps_fileset>
        <END
          COMMENT=""
          NAME="END"
          POSX="0"
          POSY="400"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="CheckSingleFileset"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <SetHpglConvertParams
          COMMENT=""
          NEXT="DPF4CONVERT.work_on_hp_fileset"
          POSX="200"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            HPGL_CONVERT_FILESET_NAME="%DPF4C_FILESETS%"
            HPGL_CONVERT_FILESET_TYPE="%DPF4C_FILESET_TYPE%"
            />
        </SetHpglConvertParams>
        <SetPdfFilesetParams
          COMMENT=""
          NEXT="DPF4CONVERT.work_on_pdf_fileset"
          POSX="400"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_FILESET_NAME="%DPF4C_FILESETS%"
            />
        </SetPdfFilesetParams>
        <SetPsFilesetParams
          COMMENT=""
          NEXT="DPF4CONVERT.work_on_ps_fileset"
          POSX="600"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_FILESET_NAME="%DPF4C_FILESETS%"
            />
        </SetPsFilesetParams>
      </DPF4CONVERT.work_on_filesets>
    </workflows>
  </config>
</DPF>
