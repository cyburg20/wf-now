<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <pdfchecker
        NAME="dpf4c - Check PDF file with pdfchecker"
        VERSION="$Id: wu-pdfchecker.xml,v 1.15 2010/03/08 09:08:24 kig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-pdfchecker"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="5"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfchecker.pl -parfile %PARFILENAME%">
        <DEFAULTS
          WU_ENABLE_DEBUG_LOOP="N"
          PDFCHK_GENERAL_ENABLE="N"
          PDFCHK_CONFIG_FILE_DIRECTORY="%PLSTOOLS%/convcfg/pdflls"
          PDFCHK_CHECK_NAME="PDFA1BChecks"
          PDFCHK_CONFIG_FILE="dpf_filecheck.cfg"
          PDFCHK_CHECKS_FILE="%PLSDATA%/tmp/pdfchecker/pdfchecks.log" 
          PDFCHK_CHECKS_LOGGING_DISABLE="N"
          PDFCHK_ERROR_LEVEL="ERROR"
          PDFCHK_DEBUG_LEVEL="2"
          PDFCHK_PROTOCOL_KEEP_DIR="%PLSDATA%/tmp/pdfchecker"
          PDFCHK_PROTOCOL_KEEP_FLAG="OFF"
          PDFCHK_PROTOCOL_KEEP_ONLY_IN_PDFCHECKER_LAST_WU="N"
          PDFCHK_PROTOCOL_SEND_BACK="OFF"
          PDFCHK_SAVE_PDF_FLAG="OFF"
          PDF_GENCHECKLOG_PROTOCOL_SEVERITY="OK"
          SEND_BACK_PDF_CHECKER_SIMPLE_PROTOCOL="N"
          SEND_BACK_PDF_CHECKER_LOG_FILE="N"
          PDFCHK_NO_CHECKIN_FILE_NOT_NEEDED_ADJUST="N"
          PDFADJUST_REPAIR_IF_PDFCHECKER_WORSE_THAN="WARN"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          PDFCHK_GENERAL_ENABLE="Y|N"
          PDFCHK_CONFIG_FILE_DIRECTORY="dir"
          PDFCHK_CHECK_NAME="string"
          PDFCHK_CONFIG_FILE="file"
          PDFCHK_CHECKS_FILE="file"
          PDFCHK_CHECKS_LOGGING_DISABLE="Y|N"
          PDFCHK_ERROR_LEVEL="IGNORE|WARN|ERROR|FATAL"
          PDFCHK_DEBUG_LEVEL="1-8"
          PDFCHK_PROTOCOL_KEEP_DIR="dir"
          PDFCHK_PROTOCOL_KEEP_FLAG="OFF ALWAYS ONERROR ONWARNING"
          PDFCHK_PROTOCOL_KEEP_ONLY_IN_PDFCHECKER_LAST_WU="Y|N"
          PDFCHK_PROTOCOL_SEND_BACK="OFF ALWAYS ONERROR ONWARNING"
          PDFCHK_SAVE_PDF_FLAG="OFF ALWAYS ONERROR ONWARNING"
          PDF_GENCHECKLOG_PROTOCOL_SEVERITY="!OK!|WARNING|ERROR"
          SAP_SOURCEFILE_DOCUMENTNUMBER="string"
          SAP_SOURCEFILE_DOCUMENTTYPE="string"
          SAP_SOURCEFILE_DOCUMENTVERSION="string"
          SAP_SOURCEFILE_DOCUMENTPART="string"
          DPF4C_COMMON_TIMEOUT="number"
          DPF4C_COMMON_DEST_FORMAT="string"
          PDF_CHECK_ADJUST_FILE_MODIFIED="Y|N"
          ENABLE_PDF_ADJUST_PROCESS="Y|N"
          DPF4C_COMMON_OUTFILEfirstprot="file"
          SEND_BACK_PDF_CHECKER_SIMPLE_PROTOCOL="Y|N"
          SEND_BACK_PDF_CHECKER_LOG_FILE="Y|N"
          PDFCHK_NO_CHECKIN_FILE_NOT_NEEDED_ADJUST="Y|N"
          PDFADJUST_REPAIR_IF_PDFCHECKER_WORSE_THAN="OK|WARN"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          DPF4C_COMMON_OUTFILEprot="file"
          ERR_REASON="text"
          FILE_CHECK_PDFCHECKER_RESULT_LEVEL="OK|WARN|ERROR|FATAL"
          PDFCHECKER_EXIT_CODE="number"
          FILE_CHECK_IS_ENCRYPTED="Y|N"
          FILE_CHECK_IS_PASSWORD_PROTECTED="Y|N"
          FILE_CHECK_HAS_SECURITY_SET="Y|N"
          DPF4C_COMMON_OUTFILEfirstprot="file"
          DPF4C_COMMON_DEST_FORMAT="xxx, prot"
          SWITCH_CHECKIN_CURRFILENAME="Y|N"
          ENALBE_PDF_ADJUST_PROCESS="Y|N"
          />
      </pdfchecker>
    </workingunits>
  </config>
</DPF>
