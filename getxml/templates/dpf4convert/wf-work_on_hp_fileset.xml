<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.work_on_hp_fileset
        COMMENT="dpf4convert - Summarize processing steps for a single HP fileset

This process performs the steps hpgl_convert, PDF-process, create target formats and PDF-postproc on a single HP fileset."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Work On HP Fileset - Summarize processing a single HP Fileset"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-work_on_hp_fileset.xml,v 1.3 2017/01/26 08:54:19 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.hpgl_convert1
          COMMENT=""
          ERROR="END"
          NAME="hpgl_convert"
          NEXT="restore_quality"
          POSX="700"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.hpgl_convert1>
        <DPF4CONVERT.work_on_pdf1
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1100"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.work_on_pdf"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.work_on_pdf1>
        <END
          COMMENT=""
          NAME="END"
          POSX="400"
          POSY="700"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="set_pdf_dest_format"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_pdfa
          COMMENT=""
          ELSE="hpgl_convert1"
          OPERATOR="contains"
          POSX="100"
          POSY="300"
          PROPERTY="DESTINATION_FORMAT_QUALITY"
          THEN="save_quality"
          TYPE="IF-THEN-ELSE"
          VALUE="pdfa"
          >
          <CONNECTIONS
            ELSE="HORIZONTAL"
            THEN="DIRECT"
            />
        </check_pdfa>
        <hpgl_convert1
          COMMENT=""
          ERROR="END"
          NAME="hpgl_convert"
          NEXT="DPF4CONVERT.work_on_pdf1"
          POSX="700"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </hpgl_convert1>
        <restore_quality
          COMMENT=""
          NEXT="DPF4CONVERT.work_on_pdf1"
          POSX="900"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            DESTINATION_FORMAT_QUALITY="%DESTINATION_FORMAT_QUALITY_SAVE%"
            />
        </restore_quality>
        <save_quality
          COMMENT=""
          NEXT="set_quality_temp"
          POSX="300"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DESTINATION_FORMAT_QUALITY_SAVE="%DESTINATION_FORMAT_QUALITY%"
            />
        </save_quality>
        <set_pdf_dest_format
          COMMENT=""
          NEXT="check_pdfa"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            HPGL_CONVERT_COMMON_DEST_FORMAT="pdf"
            />
        </set_pdf_dest_format>
        <set_quality_temp
          COMMENT=""
          NEXT="DPF4CONVERT.hpgl_convert1"
          POSX="500"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DESTINATION_FORMAT_QUALITY="default"
            />
        </set_quality_temp>
      </DPF4CONVERT.work_on_hp_fileset>
    </workflows>
  </config>
</DPF>
