<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.create_thumbnail
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/create_thumbnail.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="dpf4c - Create thumbnail from PDF or TIFF"
        LICENSE="COTHMB"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="5"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-create_thumbnail.xml,v 1.3 2017/02/08 12:46:27 thomas Exp $"
        >
        <DEFAULTS
          DPF4C_THUMBNAIL_DPI="72"
          DPF4C_THUMBNAIL_SIZE="200"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="*"
          DPF4C_THUMBNAIL_DPI="72"
          DPF4C_THUMBNAIL_FORMAT="jpg|png"
          DPF4C_THUMBNAIL_SIZE="int|intXint"
          />
      </DPF4CONVERT.create_thumbnail>
    </workingunits>
  </config>
</DPF>
