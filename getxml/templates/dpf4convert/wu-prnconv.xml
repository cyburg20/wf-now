<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.prnconv
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/prnconv.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Prnconv"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="5"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-prnconv.xml,v 1.3 2010/01/29 09:51:58 kig Exp $"
        >
        <DEFAULTS
          PRINTERDIR="%DPFSRV%/conf/prnconv"
          SCALETYPE="original"
          SCALEFACTOR="1.0"
          AUTOROTATE="Y"
          PAGESIZE="auto"
          ORIENTATION="auto"
          />
        <INPUT
          CURRFILENAME="*"
          TARGETFORMAT="*"
          PRINTERDIR="directory"
          DEVICETYPE="*"
          SCALETYPE="original|fit|abstract"
          SCALEFACTOR="*"
          AUTOROTATE="!Y!|N"
          PAGESIZE="*"
          ORIENTATION="auto|portrait|landscape|reverse_portrait|reverse_landscape"
            />
        <OUTPUT/>
      </DPF4CONVERT.prnconv>
    </workingunits>
  </config>
</DPF>
