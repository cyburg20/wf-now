<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.pdfsign
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Pdfsign - Apply signature to a PDF file"
        OrigIdentifier="WorkflowDPF4CONVERT.pdfsign"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdfsign.xml,v 1.1 2015/08/19 12:06:23 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_signing"
          NAME=""
          />
        <MAPPING/>
        <check_signing
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="100"
          POSY="100"
          PROPERTY="ENABLE_SIGNING"
          THEN="signlive_sign"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_signing>
        <signlive_sign
          COMMENT=""
          ERROR="END"
          NAME="signlive.sign"
          NEXT="END"
          POSX="300"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </signlive_sign>
      </DPF4CONVERT.pdfsign>
    </workflows>
  </config>
</DPF>
