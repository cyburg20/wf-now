<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <office2ps
        COMMENT="dpf4convert - Convert process Microsoft Office to PS"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - Microsoft Office to PS"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-office2ps.xml,v 1.5 2016/06/24 14:42:33 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_switch_property_update"
          ERROR="END"
          />
        <MAPPING/>
        <UserExitPreOfficeConv
          COMMENT=""
          ERROR="END"
          NEXT="office2ps"
          POSX="400"
          POSY="300"
          TYPE="USER_EXIT"
          USE="%DPF4CONVERT.UserExitPreOfficeConv%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </UserExitPreOfficeConv>
        <check_switch_property_update
          COMMENT=""
          ELSE="check_user_pre_office_conv"
          OPERATOR="contains"
          POSX="100"
          POSY="100"
          PROPERTY="DPF4C_PROPERTYUPDATE_ENABLE"
          THEN="property_update_office"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_property_update>
        <check_user_pre_office_conv
          COMMENT=""
          ELSE="office2ps"
          OPERATOR="ne"
          POSX="300"
          POSY="100"
          PROPERTY="DPF4CONVERT.UserExitPreOfficeConv"
          THEN="UserExitPreOfficeConv"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_user_pre_office_conv>
        <office2ps
          COMMENT=""
          ERROR="END"
          NAME="office2ps"
          NEXT="END"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </office2ps>
        <property_update_office
          COMMENT=""
          ERROR="END"
          NAME="property_update_office"
          NEXT="check_user_pre_office_conv"
          POSX="200"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </property_update_office>
      </office2ps>
    </workflows>
  </config>
</DPF>
