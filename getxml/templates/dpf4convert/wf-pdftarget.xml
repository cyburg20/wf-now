<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.pdftarget
        COMMENT="The DPF-process PDFTARGET creates the target formats from a PDF."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Pdftarget - Create target formats"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdftarget.xml,v 1.9 2017/01/26 08:05:41 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.create_thumbnail1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.create_thumbnail"
          NEXT="MODIFY1"
          POSX="400"
          POSY="1100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.create_thumbnail1>
        <DPF4CONVERT.dpf4c_user_pre_pdftarget1
          COMMENT=""
          ERROR="END"
          NEXT="check_enable"
          POSX="400"
          POSY="200"
          TYPE="SWF"
          USE="DPF4CONVERT.dpf4c_user_pre_pdftarget"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.dpf4c_user_pre_pdftarget1>
        <DPF4CONVERT.pdftarget_init1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.pdftarget_init"
          NEXT="check_create_tiff"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdftarget_init1>
        <END
          COMMENT=""
          NAME="END"
          POSX="800"
          POSY="500"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="IF-THEN-ELSE2"
          OPERATOR="eq"
          POSX="200"
          POSY="900"
          PROPERTY="PDFTARGET_CREATE_PNG"
          THEN="DPF4CONVERT.create_thumbnail1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <IF-THEN-ELSE2
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="200"
          POSY="1100"
          PROPERTY="PDFTARGET_CREATE_JPG"
          THEN="DPF4CONVERT.create_thumbnail1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE2>
        <MAIN
          BEGIN="check_user_pre_pdftarget"
          NAME=""
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="END"
          POSX="600"
          POSY="1100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%PDFTARGET_INFILE%"
            CURRFILETYPE="pdf"
            />
        </MODIFY1>
        <RestoreCurrfilename
          COMMENT=""
          NEXT="IF-THEN-ELSE1"
          POSX="600"
          POSY="700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            CURRFILENAME="%PDFTARGET_INFILE%"
            CURRFILETYPE="pdf"
            />
        </RestoreCurrfilename>
        <check_create_tiff
          COMMENT=""
          ELSE="IF-THEN-ELSE1"
          OPERATOR="eq"
          POSX="200"
          POSY="700"
          PROPERTY="PDFTARGET_CREATE_TIF"
          THEN="pdf2tiff1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_create_tiff>
        <check_enable
          COMMENT=""
          ELSE="DPF4CONVERT.pdftarget_init1"
          OPERATOR="eq"
          POSX="200"
          POSY="300"
          PROPERTY="ENABLE_PDFTARGET"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_enable>
        <check_user_pre_pdftarget
          COMMENT=""
          ELSE="check_enable"
          OPERATOR="eq"
          POSX="200"
          POSY="100"
          PROPERTY="DPF4C_SWITCH_USER_PRE_PDFTARGET"
          THEN="DPF4CONVERT.dpf4c_user_pre_pdftarget1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_user_pre_pdftarget>
        <pdf2tiff1
          COMMENT=""
          ERROR="END"
          NAME="pdf2tiff"
          NEXT="RestoreCurrfilename"
          POSX="400"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            TARGETFILENAME=""
            />
        </pdf2tiff1>
      </DPF4CONVERT.pdftarget>
    </workflows>
  </config>
</DPF>
