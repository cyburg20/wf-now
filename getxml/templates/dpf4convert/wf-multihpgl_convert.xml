<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <multihpgl_convert
        COMMENT="dpf4convert - PROCESS FOR DPF4CONVERT"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - Process multihpgl_convert"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-multihpgl_convert.xml,v 1.2 2009/04/27 09:34:43 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="1300"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="sp-read_ini_file"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="hpgl_convert1"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_DEST_FORMAT="%HPGL_CONVERT_COMMON_DEST_FORMAT%"
            />
        </MODIFY1>
        <checkin
          COMMENT=""
          ERROR="END"
          NAME="checkin"
          NEXT="sendback"
          POSX="200"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </checkin>
        <hpgl_convert1
          COMMENT=""
          ERROR="END"
          NAME="hpgl_convert"
          NEXT="checkin"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <DPF4C_COMMON_DEST_COLOR
              val="col"
              />
            <HPGL_CONVERT_COMMON_DEST_FORMAT
              val="pdf"
              />
            <HPGL_CONVERT_MODIFY_ROTATION
              val="0"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </hpgl_convert1>
        <sendback
          COMMENT=""
          ERROR="END"
          NAME="sendback"
          NEXT="END"
          POSX="200"
          POSY="900"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sendback>
        <sp-read_ini_file
          COMMENT=""
          ERROR="END"
          NAME="dpf4c_read_ini_file"
          NEXT="MODIFY1"
          POSX="200"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-read_ini_file>
      </multihpgl_convert>
    </workflows>
  </config>
</DPF>
