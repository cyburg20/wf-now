<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.txt2pdf
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/dpf4convert/txt2pdf.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="dpf4c - Convert Text to PDF"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-txt2pdf.xml,v 1.2 2012/03/23 14:01:12 sek Exp $"
        >
        <DEFAULTS/>
        <INPUT>
          <CurrFileName
            DESCRIPTION="the input text file"
            MANDATORY="true"
            TYPE="TEXT"
            />
          <TargetFileName
            DESCRIPTION="ouput pdf file name, per default .ps.pdf will be appended to the input file name"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </DPF4CONVERT.txt2pdf>
    </workingunits>
  </config>
</DPF>
