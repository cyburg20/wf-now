<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <sowoexport
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/sowoexport.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        LOGLEVEL="LOG_INFO"
        LOGNAME="dpf4c-sowoexport"
        LOGSTDOUT="Y"
        NAME="dpf4c - Convert CAD Solid Works DRW, ASM, PRT to PDF and other formats by export"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-sowoexport.xml,v 1.2 2013/11/08 14:52:54 dei Exp $"
        >
        <DEFAULTS
          DPF4C_COMMON_TIMEOUT="20"
          DPF4C_PROPERTYUPDATE_ENABLE="N"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="N"
          DPF4C_SLDDRW2PS_AUTOMISSINGCONFIG="N"
          DPF4C_SLDDRW2PS_CONFIG_FILE="sowo.cfg"
          DPF4C_SLDDRW2PS_NOCONV2QUITSOWO="10"
          DPF4C_SLDDRW2PS_RAPIDDRAFT="N"
          DPF4C_SLDDRW2PS_REBUILD="Y"
          DPF4C_SLDDRW2PS_USE_CUSTOM_SIZE="N"
          DPF4C_SLDDRW2PS_VISIBLE="Y"
          CAD_SOWO_ENABLE_MUTEX="Y"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="drw"
          DPF4C_PROPERTYUPDATE_ENABLE="Y|N"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y|N"
          DPF4C_SLDDRW2PS_AUTOMISSINGCONFIG="Y|N"
          DPF4C_SLDDRW2PS_CONFIG_FILE="file"
          DPF4C_SLDDRW2PS_EXT="string"
          DPF4C_SLDDRW2PS_NOCONV2QUITSOWO="number"
          DPF4C_SLDDRW2PS_PROG_ID="number"
          DPF4C_SLDDRW2PS_RAPIDDRAFT="Y|N"
          DPF4C_SLDDRW2PS_REBUILD="Y|N"
          DPF4C_SLDDRW2PS_USE_CUSTOM_SIZE="Y|N"
          DPF4C_SLDDRW2PS_VISIBLE="Y|N"
          CAD_SOWO_ENABLE_MUTEX="!Y!|N"
          PARFILENAME="*"
          TARGETFILE="target basename"
          >
          <DPF4C_COMMON_TIMEOUT
            DEFAULT="20"
            TYPE="TEXT"
            />
          <DPF4C_PROPERTYUPDATE_ENABLE
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_PROPERTYUPDATE_PROVIDE_FILES
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_AUTOMISSINGCONFIG
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_CONFIG_FILE
            DEFAULT="sowo.cfg"
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_EXT
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_NOCONV2QUITSOWO
            DEFAULT="10"
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_PROG_ID
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_RAPIDDRAFT
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_REBUILD
            DEFAULT="Y"
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_USE_CUSTOM_SIZE
            DEFAULT="N"
            TYPE="TEXT"
            />
          <DPF4C_SLDDRW2PS_VISIBLE
            DEFAULT="Y"
            TYPE="TEXT"
            />
          <TARGETFILE
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="ps"
          DPF4C_PROPERTYUPDATE_OUTFILE1="file|"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y|"
          />
      </sowoexport>
    </workingunits>
  </config>
</DPF>
