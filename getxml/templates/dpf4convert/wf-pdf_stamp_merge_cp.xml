<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <pdf_stamp_merge_cp
        COMMENT="dpf4convert - Stamp pdf file and merge it with given pdf file."
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - pdf_stamp_merge_cp"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdf_stamp_merge_cp.xml,v 1.3 2010/11/15 16:18:48 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.copy_currfilename1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.copy_currfilename"
          NEXT="END"
          POSX="500"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.copy_currfilename1>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="pdfstamp"
          ERROR="END"
          />
        <MAPPING/>
        <pdfmerge
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.copy_currfilename1"
          POSX="300"
          POSY="200"
          TYPE="SWF"
          USE="pdfmerge"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfmerge>
        <pdfstamp
          COMMENT=""
          ERROR="END"
          NAME="pdfstamp"
          NEXT="pdfmerge"
          POSX="100"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfstamp>
      </pdf_stamp_merge_cp>
    </workflows>
  </config>
</DPF>
