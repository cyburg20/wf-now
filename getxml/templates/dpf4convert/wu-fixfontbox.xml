<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <fixfontbox
        COMMAND="%DPFSRV%/scripts/dpf4convert/fixfontbox.pl -parfile %PARFILENAME%"
        DPFINT-FileLinks="doc://:/%DPFSRV%/scripts/dpf4convert/fixfontbox.pl"
        DPFINT-Icon="doc://:/wu.gif"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME="dpf4c-fixfontbox"
        LOGSTDOUT="N"
        NAME="dpf4c - Fix a defect font box"
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-fixfontbox.xml,v 1.2 2010/02/11 10:32:18 dei Exp $"
        >
        <DEFAULTS
          CURRFILENAME=""
          DPFINT-Icon="doc://:/defaults.gif"
          WU_ENABLE_DEBUG_LOOP="N"
          DPF4C_FIXFONTBOX_GLOBAL_LOGFILE=""
          DPF4C_FIXFONTBOX_GLOBAL_SAVDIR=""
          />
        <ENVIRONMENTS
          HERE_YOU_COULD_SET_EXTRA_ENV_VARS="to a value"
          />
        <INPUT
          CURRFILENAME="*"
          DPFINT-Icon="doc://:/wu-input.gif"
          WU_ENABLE_DEBUG_LOOP="*"
          />
        <OUTPUT
          DPFINT-Icon="doc://:/wu-output.gif"
          ERR_REASON="*"
          EXAMPLE_OUTPUT="*"
          />
      </fixfontbox>
    </workingunits>
  </config>
</DPF>
