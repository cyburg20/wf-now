<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.RunProcessWithSendback
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Run Process With Sendback"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-runprocesswithsendback.xml,v 1.1 2014/03/24 07:07:04 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="RunProcess"
          NAME=""
          />
        <MAPPING/>
        <RunProcess
          COMMENT=""
          ERROR="END"
          NEXT="Sendback"
          POSX="200"
          POSY="200"
          TYPE="USER_EXIT"
          USE="%RunProcess%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </RunProcess>
        <Sendback
          COMMENT=""
          ERROR="END"
          NAME="sendback"
          NEXT="END"
          POSX="500"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <IOR
              val="%ORIGFILESERVERIOR%"
              />
            <OVERWRITE
              val="Y"
              />
            <SOURCE
              val="%CURRFILENAME%"
              />
            <TARGETFILE
              val="%TARGETFILENAME%"
              />
          </DEFAULTS>
          <MAPPING/>
        </Sendback>
      </DPF4CONVERT.RunProcessWithSendback>
    </workflows>
  </config>
</DPF>
