<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.WriteCsvLine
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/writecsvline.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Write Csv Line"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-writecsvline.xml,v 1.1 2013/09/11 11:17:47 sek Exp $"
        >
        <DEFAULTS
          PDFLOG_CSV_FILE="%PDFLOG_DIR%/pdflog.csv"
          PDFLOG_CSV_SEP=";"
          />
        <INPUT>
          <PDFINFO_FILE
            DESCRIPTION="file generated by pdfinfo process"
            MANDATORY="true"
            TYPE="TEXT"
            />
          <PDFLOG_CSV_FILE
            DEFAULT="%PDFLOG_DIR%/pdflog.csv"
            DESCRIPTION="the csv file to write to or to create"
            MANDATORY="true"
            TYPE="TEXT"
            />
          <PDFLOG_CSV_SEP
            DEFAULT=";"
            DESCRIPTION="csv separator character"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </DPF4CONVERT.WriteCsvLine>
    </workingunits>
  </config>
</DPF>
