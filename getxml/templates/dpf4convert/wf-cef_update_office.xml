<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.cef_update_office
        COMMENT="dpf4convert - PROCESS CEF UPDATE OFFICE.

This process is used to update an Office-file on a Conversion Engine Client."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        LICENSE="CI3O CIDOC CIXLS CIPPT"
        NAME="CEF Update Office"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-cef_update_office.xml,v 1.1 2017/09/07 09:58:17 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.cef_get_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_wu_targetdir"
          POSX="100"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.cef_get_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.cef_get_data1>
        <DPF4CONVERT.convert_office1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="unset_wu_targetdir"
          POSX="100"
          POSY="900"
          TYPE="SWF"
          USE="DPF4CONVERT.convert_office"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.convert_office1>
        <DPF4CONVERT.net_put_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY4"
          POSX="100"
          POSY="1300"
          TYPE="SWF"
          USE="DPF4CONVERT.net_put_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_put_data1>
        <DPF4CONVERT.net_readcustomconfig
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.net_readcustomconfig"
          NEXT="check_types_to_abort"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_readcustomconfig>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1800"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <MODIFY4
          COMMENT=""
          NEXT="END"
          POSX="100"
          POSY="1500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="100"
            />
        </MODIFY4>
        <check_types_to_abort
          COMMENT=""
          ELSE="DPF4CONVERT.cef_get_data1"
          OPERATOR="eq"
          POSX="100"
          POSY="300"
          PROPERTY="DPF4C_NETCONV_FILETYPES_ABORT"
          THEN="set_types_to_abort"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_types_to_abort>
        <set_config
          COMMENT=""
          NEXT="DPF4CONVERT.net_readcustomconfig"
          POSX="300"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_PLM_SYSTEM_NAME="no_plm_system"
            DPF4C_CUSTOM_CONFIG="dpf4convert::cef_update_office,dpf4convert::cef_error_handling"
            DPF4C_PROPERTYUPDATE_ENABLE="Y"
            DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y"
            DPFJOBPROGRESS="0"
            />
        </set_config>
        <set_types_to_abort
          COMMENT=""
          NEXT="DPF4CONVERT.cef_get_data1"
          POSX="300"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_NETCONV_FILETYPES_ABORT="msg,eml,odt,ott,ods,ots,odp,otp,odq,otq"
            />
        </set_types_to_abort>
        <set_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.convert_office1"
          POSX="100"
          POSY="700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR="%JOBDIR%"
            DPFJOBPROGRESS="33"
            />
        </set_wu_targetdir>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="500"
          POSY="1700"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <unset_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.net_put_data1"
          POSX="100"
          POSY="1100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR=""
            DPFJOBPROGRESS="66"
            />
        </unset_wu_targetdir>
        <version_dpf4c
          COMMENT=""
          ERROR="END"
          NEXT="set_config"
          POSX="100"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c>
      </DPF4CONVERT.cef_update_office>
    </workflows>
  </config>
</DPF>
