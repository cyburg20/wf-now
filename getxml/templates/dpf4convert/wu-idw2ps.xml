<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <idw2ps
        COMMAND="%DPFSRV%/scripts/dpf4convert/idw2ps.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        LOGLEVEL="LOG_INFO"
        LOGNAME="dpf4c-idw2ps"
        LOGSTDOUT="Y"
        NAME="dpf4c - CAD Inventor IDW to PS"
        PROJECT="DPF4CONVERT"
        Status="Running"
        THREADS="1"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-idw2ps.xml,v 1.13 2007/03/28 10:39:35 ch Exp $"
        >
        <DEFAULTS
          DPF4C_COMMON_MULTIPAGE="Y"
          DPF4C_PROPERTYUPDATE_ENABLE="N"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="N"
          IDW2FILE_ENABLE_FORCE_PROP_MODEL="N"
          IDW2FILE_ENABLE_PROPERTY_LOG="Y"
          IDW2FILE_GENERAL_MULTISHEET="Y"
          IDW2FILE_IGNORE_LLC_ERRORS="N"
          IDW2FILE_PROPERTYUPDATE_DELETE_EMPTY_VALUES="N"
          IDW2FILE_START_VISIBLE="N"
          IDW2FILE_USE_SEPPEXECUTE_FOR_TIMEOUT="N"
          IDW2FILE_USE_SEPPSTARTER="N"
          IDW2PS_ALLOW_TEMP_MODIFY_CFG_FILE="Y"
          IDW2PS_PROPERTYUPDATE_DELETE_EMPTY_VALUES="N"
          REGSVRTOOL="regsvr32.exe"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          DPF4C_COMMON_MULTIPAGE="Y|N"
          DPF4C_PROPERTYUPDATE_ENABLE="Y|N"
          DPF4C_PROPERTYUPDATE_FILE="file"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y|N"
          IDW2FILE_START_VISIBLE="Y|N"
          IDW2FILE_USE_SEPPEXECUTE_FOR_TIMEOUT="Y|N"
          IDW2FILE_USE_SEPPSTARTER="Y|N"
          IDW2PS_PROPERTYUPDATE_DELETE_EMPTY_VALUES="Y|N"
          PARFILENAME="*"
          >
          <CURRFILENAME
            TYPE="TEXT"
            />
          <CURRFILETYPE
            TYPE="TEXT"
            />
          <DESTINATION_FORMAT_QUALITY
            TYPE="TEXT"
            />
          <DPF4C_COMMON_DEST_COLOR
            TYPE="TEXT"
            />
          <DPF4C_COMMON_DEST_FORMAT
            TYPE="TEXT"
            />
          <DPF4C_COMMON_MULTIPAGE
            DEFAULT="Y"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </DPF4C_COMMON_MULTIPAGE>
          <DPF4C_COMMON_TIMEOUT
            TYPE="TEXT"
            />
          <DPF4C_PROPERTYUPDATE_ENABLE
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </DPF4C_PROPERTYUPDATE_ENABLE>
          <DPF4C_PROPERTYUPDATE_FILE
            TYPE="TEXT"
            />
          <DPF4C_PROPERTYUPDATE_PROVIDE_FILES
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </DPF4C_PROPERTYUPDATE_PROVIDE_FILES>
          <IDW2FILE_ENABLE_FORCE_PROP_MODEL
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_ENABLE_FORCE_PROP_MODEL>
          <IDW2FILE_ENABLE_PROPERTY_LOG
            DEFAULT="Y"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_ENABLE_PROPERTY_LOG>
          <IDW2FILE_GENERAL_MULTISHEET
            DEFAULT="Y"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_GENERAL_MULTISHEET>
          <IDW2FILE_IGNORE_LLC_ERRORS
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_IGNORE_LLC_ERRORS>
          <IDW2FILE_PRINTER
            TYPE="TEXT"
            />
          <IDW2FILE_PROPERTYUPDATE_DELETE_EMPTY_VALUES
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_PROPERTYUPDATE_DELETE_EMPTY_VALUES>
          <IDW2FILE_SHEET_CONTROL_FLAG
            TYPE="TEXT"
            />
          <IDW2FILE_START_VISIBLE
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_START_VISIBLE>
          <IDW2FILE_USE_SEPPEXECUTE_FOR_TIMEOUT
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_USE_SEPPEXECUTE_FOR_TIMEOUT>
          <IDW2FILE_USE_SEPPSTARTER
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2FILE_USE_SEPPSTARTER>
          <IDW2PS_ALLOW_TEMP_MODIFY_CFG_FILE
            DEFAULT="Y"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2PS_ALLOW_TEMP_MODIFY_CFG_FILE>
          <IDW2PS_PROPERTYUPDATE_DELETE_EMPTY_VALUES
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </IDW2PS_PROPERTYUPDATE_DELETE_EMPTY_VALUES>
          <REGSVRTOOL
            DEFAULT="regsvr32.exe"
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="CHECK"
            >
            <RANGE
              VALUE1="false=N"
              VALUE2="true=Y"
              />
          </WU_ENABLE_DEBUG_LOOP>
        </INPUT>
      </idw2ps>
    </workingunits>
  </config>
</DPF>
