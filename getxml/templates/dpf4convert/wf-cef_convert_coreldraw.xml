<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.cef_convert_coreldraw
        COMMENT="dpf4convert - PROCESS CEF CONVERT CORELDAW.

This process is used to convert CorelDraw-Format on a Conversion Engine Client."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        LICENSE="CI3O CIDOC CIXLS CIPPT"
        NAME="CEF Convert CorelDraw"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-cef_convert_coreldraw.xml,v 1.1 2017/09/07 09:58:16 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.cef_get_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_wu_targetdir"
          POSX="100"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.cef_get_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.cef_get_data1>
        <DPF4CONVERT.convert_office
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="unset_wu_targetdir"
          POSX="100"
          POSY="900"
          TYPE="SWF"
          USE="DPF4CONVERT.convert_office"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.convert_office>
        <DPF4CONVERT.net_put_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY4"
          POSX="100"
          POSY="2700"
          TYPE="SWF"
          USE="DPF4CONVERT.net_put_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_put_data1>
        <DPF4CONVERT.net_readcustomconfig
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.net_readcustomconfig"
          NEXT="check_types_to_abort"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_readcustomconfig>
        <DPF4CONVERT.pdfpostproc
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY3"
          POSX="100"
          POSY="2300"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfpostproc"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfpostproc>
        <DPF4CONVERT.pdfprocess
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY1"
          POSX="100"
          POSY="1300"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprocess"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfprocess>
        <DPF4CONVERT.pdftarget
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY2"
          POSX="100"
          POSY="1700"
          TYPE="SWF"
          USE="DPF4CONVERT.pdftarget"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdftarget>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1800"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="DPF4CONVERT.pdftarget"
          POSX="100"
          POSY="1500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="50"
            />
        </MODIFY1>
        <MODIFY2
          COMMENT=""
          NEXT="check_activate_pdfadapt"
          POSX="100"
          POSY="1900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="66"
            />
        </MODIFY2>
        <MODIFY3
          COMMENT=""
          NEXT="DPF4CONVERT.net_put_data1"
          POSX="100"
          POSY="2500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="83"
            />
        </MODIFY3>
        <MODIFY4
          COMMENT=""
          NEXT="END"
          POSX="100"
          POSY="2900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="100"
            />
        </MODIFY4>
        <check_activate_pdfadapt
          COMMENT=""
          ELSE="DPF4CONVERT.pdfpostproc"
          OPERATOR="contains"
          POSX="100"
          POSY="2100"
          PROPERTY="DESTINATION_FORMAT_QUALITY"
          THEN="enable_pdfadapt"
          TYPE="IF-THEN-ELSE"
          VALUE="pdfa"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_activate_pdfadapt>
        <check_types_to_abort
          COMMENT=""
          ELSE="DPF4CONVERT.cef_get_data1"
          OPERATOR="eq"
          POSX="100"
          POSY="300"
          PROPERTY="DPF4C_NETCONV_FILETYPES_ABORT"
          THEN="set_types_to_abort"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_types_to_abort>
        <enable_pdfadapt
          COMMENT=""
          NEXT="DPF4CONVERT.pdfpostproc"
          POSX="300"
          POSY="2200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFADAPT="Y"
            />
        </enable_pdfadapt>
        <set_config
          COMMENT=""
          NEXT="DPF4CONVERT.net_readcustomconfig"
          POSX="300"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_PLM_SYSTEM_NAME="no_plm_system"
            DPF4C_CUSTOM_CONFIG="dpf4convert::cef_convert_coreldraw,dpf4convert::cef_convert,dpf4convert::cef_error_handling"
            DPFJOBPROGRESS="0"
            />
        </set_config>
        <set_types_to_abort
          COMMENT=""
          NEXT="DPF4CONVERT.cef_get_data1"
          POSX="300"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_NETCONV_FILETYPES_ABORT="doc,docm,docx,dot,dotm,dotx,eml,mpp,mpv,msg,odg,odp,ods,odt,otg,otp,ots,ott,pot,potm,potx,pps,ppsm,ppsx,ppt,pptm,pptx,rtf,txt,vdx,vsd,vsdx,vsdm,vss,vst,vsx,vssx,vssm,vstx,vstm,xla,xlam,xls,xlsb,xlsm,xlsx,xlt,xltm,xltx"
            />
        </set_types_to_abort>
        <set_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.convert_office"
          POSX="100"
          POSY="700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR="%JOBDIR%"
            DPFJOBPROGRESS="17"
            />
        </set_wu_targetdir>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="500"
          POSY="3100"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <unset_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.pdfprocess"
          POSX="100"
          POSY="1100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR=""
            DPFJOBPROGRESS="33"
            />
        </unset_wu_targetdir>
        <version_dpf4c
          COMMENT=""
          ERROR="END"
          NEXT="set_config"
          POSX="100"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c>
      </DPF4CONVERT.cef_convert_coreldraw>
    </workflows>
  </config>
</DPF>
