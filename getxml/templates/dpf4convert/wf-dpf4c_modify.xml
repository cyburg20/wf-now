          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfrotate>
        <pdfscale
          COMMENT=""
          ERROR="END"
          NAME="pdfscale"
          NEXT="check_switch_pdfcompress"
          POSX="2700"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfscale>
        <pdfstamp
          COMMENT=""
          ERROR="END"
          NEXT="check_switch_pdf2pdf_mono"
          POSX="1900"
          POSY="100"
          TYPE="SWF"
          USE="pdfstamp"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfstamp>
        <set_pdf_meta_data
          COMMENT=""
          ERROR="END"
          NAME="pdfmeta"
          NEXT="check_switch_pdfcheck"
          POSX="3900"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </set_pdf_meta_data>
        <sp-user_pre_dpf4c_modify
          COMMENT=""
          ERROR="END"
          NEXT="check_switch_dpf4c_modify"
          POSX="300"
          POSY="100"
          TYPE="SWF"
          USE="dpf4c_user_pre_modify"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-user_pre_dpf4c_modify>
        <tiffresolution
          COMMENT=""
          ERROR="END"
          NAME="tiffresolution"
          NEXT="END"
          POSX="2300"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </tiffresolution>
        <tiffrotate
          COMMENT=""
          ERROR="END"
          NAME="tiffrotate"
          NEXT="check_switch_tiffstamp"
          POSX="1100"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </tiffrotate>
        <tiffscale
          COMMENT=""
          ERROR="END"
          NAME="tiffscale"
          NEXT="check_switch_tiffresolution"
          POSX="1900"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </tiffscale>
        <tiffstamp
          COMMENT=""
          ERROR="END"
          NAME="tiffstamp"
          NEXT="check_switch_tiffscale"
          POSX="1500"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
