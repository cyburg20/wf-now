<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <dpf4c_modify
        COMMENT="dpf4convert - PROCESS FOR MODIFICATIONS ON GRAPHIC FILES"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - SubProcess dpf4c_modify"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-dpf4c_modify.xml,v 1.24 2011/05/11 15:18:49 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.pdfadapt1
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="4500"
          POSY="400"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfadapt"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfadapt1>
        <END
          COMMENT=""
          NAME="END"
          POSX="4500"
          POSY="700"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_switch_do_user_pre_dpf4c_modify"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_dpf4c_decrypt_pdf
          COMMENT=""
          ELSE="check_switch_pdfrotate"
          OPERATOR="contains"
          POSX="900"
          POSY="200"
          PROPERTY="DPF4C_MODIFY_DECRYPT_PDF"
          THEN="pdf_modify_decrypt"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_dpf4c_decrypt_pdf>
        <check_dpf4c_modify_pdf
          COMMENT=""
          ELSE="check_dpf4c_modify_tiff"
          OPERATOR="contains"
          POSX="700"
          POSY="200"
          PROPERTY="CURRFILETYPE"
          THEN="check_dpf4c_decrypt_pdf"
          TYPE="IF-THEN-ELSE"
          VALUE="pdf"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_dpf4c_modify_pdf>
        <check_dpf4c_modify_tiff
          COMMENT=""
          ELSE="END"
          OPERATOR="contains"
          POSX="700"
          POSY="400"
          PROPERTY="CURRFILETYPE"
          THEN="check_switch_tiffrotate"
          TYPE="IF-THEN-ELSE"
          VALUE="tif"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_dpf4c_modify_tiff>
        <check_enable_pdfadapt
          COMMENT=""
          ELSE="enable_pdfadapt"
          OPERATOR="eq"
          POSX="4300"
          POSY="200"
          PROPERTY="ENABLE_PDFADAPT"
          THEN="pdfcheck"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="HORIZONTAL"
            />
        </check_enable_pdfadapt>
        <check_switch_create_signature_page
          COMMENT=""
          ELSE="check_switch_set_pdf_meta_data"
          OPERATOR="contains"
          POSX="3300"
          POSY="200"
          PROPERTY="DPF4C_CREATE_SIGNATURE_PAGE"
          THEN="create_merge_signature_page"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_create_signature_page>
        <check_switch_do_user_pre_dpf4c_modify
          COMMENT=""
          ELSE="check_switch_dpf4c_modify"
          OPERATOR="contains"
          POSX="100"
          POSY="200"
          PROPERTY="DPF4C_SWITCH_DO_USER_PRE_DPF4C_MODIFY"
          THEN="sp-user_pre_dpf4c_modify"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_do_user_pre_dpf4c_modify>
        <check_switch_dpf4c_modify
          COMMENT=""
          ELSE="END"
          OPERATOR="contains"
          POSX="500"
          POSY="200"
          PROPERTY="DPF4C_SWITCH_MODIFY"
          THEN="check_dpf4c_modify_pdf"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_dpf4c_modify>
        <check_switch_pdf2pdf_mono
          COMMENT=""
          ELSE="check_switch_pdfscale"
          OPERATOR="contains"
          POSX="2100"
          POSY="200"
          PROPERTY="DPF4C_COMMON_DEST_COLOR"
          THEN="pdf2pdf_mono"
          TYPE="IF-THEN-ELSE"
          VALUE="black"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_pdf2pdf_mono>
        <check_switch_pdfcheck
          COMMENT=""
          ELSE="END"
          OPERATOR="contains"
          POSX="4100"
          POSY="200"
          PROPERTY="DPF4C_ENABLE_PDFCHECK"
          THEN="check_enable_pdfadapt"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_pdfcheck>
        <check_switch_pdfcompress
          COMMENT=""
          ELSE="check_switch_create_signature_page"
          OPERATOR="contains"
          POSX="2900"
          POSY="200"
          PROPERTY="DPF4C_MODIFY_COMPRESSION"
          THEN="pdfcompress"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_pdfcompress>
        <check_switch_pdfrotate
          COMMENT=""
          ELSE="check_switch_pdfstamp"
          OPERATOR="contains"
          POSX="1300"
          POSY="200"
          PROPERTY="DPF4C_MODIFY_ROTATE"
          THEN="pdfrotate"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_pdfrotate>
        <check_switch_pdfscale
          COMMENT=""
          ELSE="check_switch_pdfcompress"
          OPERATOR="exists"
          POSX="2500"
          POSY="200"
          PROPERTY="DPF4C_MODIFY_SCALE_FACTOR"
          THEN="pdfscale"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_pdfscale>
        <check_switch_pdfstamp
          COMMENT=""
          ELSE="check_switch_pdf2pdf_mono"
          OPERATOR="contains"
          POSX="1700"
          POSY="200"
          PROPERTY="DPF4C_MODIFY_STAMP"
          THEN="pdfstamp"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_pdfstamp>
        <check_switch_set_pdf_meta_data
          COMMENT=""
          ELSE="check_switch_pdfcheck"
          OPERATOR="contains"
          POSX="3700"
          POSY="200"
          PROPERTY="DPF4C_SET_PDF_META_DATA"
          THEN="set_pdf_meta_data"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_set_pdf_meta_data>
        <check_switch_tiffresolution
          COMMENT=""
          ELSE="END"
          OPERATOR="exists"
          POSX="2100"
          POSY="400"
          PROPERTY="DPF4C_MODIFY_RESOLUTION_DPI"
          THEN="tiffresolution"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_tiffresolution>
        <check_switch_tiffrotate
          COMMENT=""
          ELSE="check_switch_tiffstamp"
          OPERATOR="contains"
          POSX="900"
          POSY="400"
          PROPERTY="DPF4C_MODIFY_ROTATE"
          THEN="tiffrotate"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_tiffrotate>
        <check_switch_tiffscale
          COMMENT=""
          ELSE="check_switch_tiffresolution"
          OPERATOR="exists"
          POSX="1700"
          POSY="400"
          PROPERTY="DPF4C_MODIFY_SCALE_FACTOR"
          THEN="tiffscale"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_tiffscale>
        <check_switch_tiffstamp
          COMMENT=""
          ELSE="check_switch_tiffscale"
          OPERATOR="contains"
          POSX="1300"
          POSY="400"
          PROPERTY="DPF4C_MODIFY_STAMP"
          THEN="tiffstamp"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_tiffstamp>
        <create_merge_signature_page
          COMMENT=""
          ERROR="END"
          NEXT="check_switch_set_pdf_meta_data"
          POSX="3500"
          POSY="100"
          TYPE="SWF"
          USE="create_merge_signature_page"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </create_merge_signature_page>
        <enable_pdfadapt
          COMMENT=""
          NEXT="DPF4CONVERT.pdfadapt1"
          POSX="4300"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFADAPT="Y"
            />
        </enable_pdfadapt>
        <pdf2pdf_mono
          COMMENT=""
          ERROR="END"
          NAME="pdf2pdf_mono"
          NEXT="check_switch_pdfscale"
          POSX="2300"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdf2pdf_mono>
        <pdf_modify_decrypt
          COMMENT=""
          ERROR="END"
          NAME="pdfdecrypt"
          NEXT="check_switch_pdfrotate"
          POSX="1100"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdf_modify_decrypt>
        <pdfcheck
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="4500"
          POSY="200"
          TYPE="SWF"
          USE="pdfcheckadjust"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfcheck>
        <pdfcompress
          COMMENT=""
          ERROR="END"
          NAME="pdfcompress"
          NEXT="check_switch_create_signature_page"
          POSX="3100"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfcompress>
        <pdfrotate
          COMMENT=""
          ERROR="END"
          NAME="pdfrotate"
          NEXT="check_switch_pdfstamp"
