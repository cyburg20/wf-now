<DPF>
  <config>
    <workflows>
      <xls2pdf
        NAME="DPF 4 Convert - Microsoft Excel XLS to PDF"
        VERSION="$Id: wf-xls2pdf.xml,v 1.10 2007/06/29 10:18:12 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        COMMENT="dpf4convert - Convert process Microsoft Excel XLS to PDF"
        DPFINT-Type="WorkflowDescription"
        DPFINT-Changes="0"
        >
        <MAIN
          BEGIN="office2ps"
          ERROR="END"
          />
        <office2ps
          USE="office2ps"
          NEXT="check_currfiletype"
          />
        <check_currfiletype
          TYPE="IF-THEN-ELSE"
          PROPERTY="CURRFILETYPE"
          OPERATOR="contains"
          VALUE="pdf"
          THEN="END"
          ELSE="check_destformat"
          />
        <check_destformat
          TYPE="IF-THEN-ELSE"
          PROPERTY="DPF4C_COMMON_DEST_FORMAT"
          OPERATOR="contains"
          VALUE="ps"
          THEN="END"
          ELSE="ps2pdfg"
          />
        <ps2pdfg
          USE="ps2pdfg"
          NEXT="END"
          />
      </xls2pdf>
    </workflows>
  </config>
</DPF>
