<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <printpdf
        NAME="dpf4c - Print PDF using given settings."
        VERSION="$Id: wu-printpdf.xml,v 1.1 2006/01/26 13:02:42 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-printpdf"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/printpdf.pl -parfile %PARFILENAME%">
        <DEFAULTS
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          />
        <OUTPUT
          />
      </printpdf>
    </workingunits>
  </config>
</DPF>
