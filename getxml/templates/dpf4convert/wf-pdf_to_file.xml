<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.pdf_to_file
        COMMENT="dpf4convert - PROCESS CONVERT PDF TO FILE
This process is used by a webservice"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="PDF to File - Process to convert PDF to output format for a webservice"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdf_to_file.xml,v 1.1 2014/01/09 13:43:32 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.pdf_to_file_work1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="progress_100"
          POSX="300"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.pdf_to_file_work"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdf_to_file_work1>
        <DPF4CONVERT.pdf_to_file_init1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.pdf_to_file_init"
          NEXT="DPF4CONVERT.pdf_to_file_work1"
          POSX="300"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdf_to_file_init1>
        <END
          COMMENT=""
          NAME="END"
          POSX="1000"
          POSY="900"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c1"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <progress_0
          COMMENT=""
          NEXT="DPF4CONVERT.pdf_to_file_init1"
          POSX="300"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="0"
            />
        </progress_0>
        <progress_100
          COMMENT=""
          NEXT="END"
          POSX="300"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="95"
            />
        </progress_100>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="600"
          POSY="800"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <version_dpf4c1
          COMMENT=""
          ERROR="END"
          NEXT="progress_0"
          POSX="300"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c1>
      </DPF4CONVERT.pdf_to_file>
    </workflows>
  </config>
</DPF>
