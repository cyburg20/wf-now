<DPF>
  <config>
    <workingunits>
      <microstation2file
        NAME="dpf4c - Convert CAD Microstation."
        VERSION="$Id: wu-microstation2file.xml,v 1.10 2007/09/04 13:15:12 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-microstation2file"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/microstation2file.pl -parfile %PARFILENAME%">
        <DEFAULTS
          DPF4C_PROPERTYUPDATE_ENABLE="N"
          DPF4C_PROPERTYUPDATE_FILE=""
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="N"
          MICROSTATION2FILE_CAD_CONV_CFG="%PLSSV%\cad\microstation\MSv8Dgn2PS.ini"
          MICROSTATION2FILE_CELL_FRAME_NAMES="A0_STA,A1_STA,A2_STA,A3_STA,A4_STA"
          MICROSTATION2FILE_CLEAN_TEMP_FILE="Y"
          MICROSTATION2FILE_COLOR_REPRESENTATION="color"
          MICROSTATION2FILE_DEBUG_LEVEL="0"
          MICROSTATION2FILE_DEFAULT_PAPER_SPACE_NAMES=""
          MICROSTATION2FILE_DEST_OUTPUT_FORMAT=""
          MICROSTATION2FILE_DRAWING_FRAME_LAYERS=""
          MICROSTATION2FILE_FRAME_SEARCH_ORDER="CEL_FRAME,REFFILE,DrawingFrameLayer"
          MICROSTATION2FILE_MULTI_PAGE="N"
          MICROSTATION2FILE_MULTI_SHEET="N"
          MICROSTATION2FILE_NO_OF_SHEETS=""
          MICROSTATION2FILE_PRE_CUSTOM_COMMAND_FILE=""
          MICROSTATION2FILE_PLOT_CONVENTION_FILE="%PLSSV%\cad\microstation\plotordr.tbl"
          MICROSTATION2FILE_PRINTER="SEALPS4CAD"
          MICROSTATION2FILE_PRINTER_CONTROL_FILE="%PLSSV%\cad\microstation\printer.plt"
          MICROSTATION2FILE_PROPERTY_LOG="Y"
          MICROSTATION2FILE_SAVE_DRAWING="0"
          MICROSTATION2FILE_STOP_ON_ERROR="N"
          MICROSTATION2FILE_PLOT_STIL_HEADER_ITEM_1=""
          MICROSTATION2FILE_PLOT_STIL_HEADER_ITEM_2=""
          MICROSTATION2FILE_REF_DIR="%PLSSV%/cad/microstation/ref"
          MICROSTATION2FILE_SWITCH_EXIT_ON_MISSING_PLOTSTYLE_FILE="Y"
          />
        <INPUT
          DPF4C_PROPERTYUPDATE_ENABLE="Y|N"
          DPF4C_PROPERTYUPDATE_FILE="file"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y|N"
          MICROSTATION2FILE_CAD_CONV_CFG="file"
          MICROSTATION2FILE_CELL_FRAME_NAMES="string"
          MICROSTATION2FILE_CLEAN_TEMP_FILE="!Y!|N"
          MICROSTATION2FILE_COLOR_REPRESENTATION="black|!color!|grey"
          MICROSTATION2FILE_DEBUG_LEVEL="number"
          MICROSTATION2FILE_DEFAULT_PAPER_SPACE_NAMES="string"
          MICROSTATION2FILE_DEST_OUTPUT_FORMAT="format_name"
          MICROSTATION2FILE_DRAWING_FRAME_LAYERS="string"
          MICROSTATION2FILE_FRAME_SEARCH_ORDER="CEL_FRAME|REFFILE|DrawingFrameLayer"
          MICROSTATION2FILE_MULTI_PAGE="Y|!N!"
          MICROSTATION2FILE_MULTI_SHEET="Y|!N!"
          MICROSTATION2FILE_NO_OF_SHEETS="number"
          MICROSTATION2FILE_PRE_CUSTOM_COMMAND_FILE="basename of file"
          MICROSTATION2FILE_PLOT_CONVENTION_FILE="file"
          MICROSTATION2FILE_PRINTER="string"
          MICROSTATION2FILE_PRINTER_CONTROL_FILE="file"
          MICROSTATION2FILE_PROPERTY_LOG="!Y!|N"
          MICROSTATION2FILE_SAVE_DRAWING="number"
          MICROSTATION2FILE_STOP_ON_ERROR="Y|!N!"
          />
        <OUTPUT
          CURRFILENAME="file"
          CURRFILETYPE="pdf"
          DPF4C_PROPERTYUPDATE_OUTFILE1="file"
          DPF4C_COMMON_OUTFILE="file"
          />
      </microstation2file>
    </workingunits>
  </config>
</DPF>
