<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <!-- PROCESS FOR PLM DATA MAPPING -->  
      <dpf4c_plm_data_mapping
        NAME="DPF 4 Convert - SubProcess dpf4c_plm_data_mapping"
        VERSION="$Id: wf-dpf4c_plm_data_mapping.xml,v 1.9 2006/09/26 14:19:23 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        COMMENT="dpf4convert - PROCESS FOR PLM DATA MAPPING"
        DPFINT-Type="WorkflowDescription"
        DPFINT-Changes="0"
        >
        <MAIN
          BEGIN="check_switch_do_user_pre_dpf4c_plm_data_mapping"
          ERROR="END"
          LOGLEVEL="I"
        />
	<check_switch_do_user_pre_dpf4c_plm_data_mapping
          TYPE="IF-THEN-ELSE"
          PROPERTY="DPF4C_SWITCH_DO_USER_PRE_DPF4C_PLM_DATA_MAPPING"
          OPERATOR="contains"
          VALUE="Y"
          THEN="sp-user_pre_dpf4c_plm_data_mapping"
          ELSE="check_switch_dpf4c_plm_data_mapping"
          />
        <sp-user_pre_dpf4c_plm_data_mapping
          USE="dpf4c_user_pre_plm_data_mapping"
          NEXT="check_switch_dpf4c_plm_data_mapping"
          />
        <check_switch_dpf4c_plm_data_mapping
          TYPE="IF-THEN-ELSE"
          PROPERTY="DPF4C_SWITCH_PLM_DATA_MAPPING"
          OPERATOR="contains"
          VALUE="Y"
          THEN="check_plm_system"
          ELSE="plm_data_mapping"
          />
        <check_plm_system
          TYPE="SWITCH"
          PROPERTY="DPF4C_COMMON_PLM_SYSTEM_NAME"
          DEFAULT="error_not_supported_plmsystemname"
          >
          <cases
            sap="plm_data_mapping_sap"
            no_plm_system="plm_data_mapping"
          />
<!--
          <cases
            sap="plm_data_mapping_sap"
            axalant="plm_data_mapping_axalant"
            matrixone="plm_data_mapping_matrixone"
            no_plm_system="plm_data_mapping"
          />
-->
        </check_plm_system>
        <plm_data_mapping_sap 
          NAME="plm_data_mapping_sap"
          NEXT="plm_data_mapping"
          />
<!--
        <plm_data_mapping_axalant 
          NAME="plm_data_mapping_axalant"
          NEXT="plm_data_mapping"
          />
        <plm_data_mapping_matrixone 
          NAME="plm_data_mapping_matrixone"
          NEXT="plm_data_mapping"
          />
-->
        <plm_data_mapping
          NAME="plm_data_mapping"
          NEXT="END"
          />
        <error_not_supported_plmsystemname
          TYPE="MODIFY"
          NEXT="exit_process"
        >
          <SET
          ERR_REASON="Error: This dpf parameter value DPF4C_SWITCH_PLM_DATA_MAPPING -%DPF4C_SWITCH_PLM_DATA_MAPPING%- is not supported for plm data mapping!"
          ERR_WFNAME="DPF4C_PLM_DATA_MAPPING"
          ERR_WFPOSITION="check_plm_system"
          />
        </error_not_supported_plmsystemname>
        <exit_process
          NAME="exit_process"
          NEXT="END"
          />
      </dpf4c_plm_data_mapping>
    </workflows>
  </config>
</DPF>
