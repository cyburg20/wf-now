<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <ocr2pdf
        COMMENT="dpf4convert - perform OCR on a file and create a PDF"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - OCR on a file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-ocr2pdf.xml,v 1.4 2013/06/27 08:57:23 dei Exp $"
        >
        <DEFAULTS
          CHECK_ATTRIBUTES="N"
          />
        <DPF4CONVERT.checkattributes1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.checkattributes"
          NEXT="ocr2pdf1"
          POSX="600"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.checkattributes1>
        <DPF4CONVERT.tiffadapt1COPY
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.tiffadapt"
          NEXT="ocr2pdf1"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.tiffadapt1COPY>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="ocr2pdf1"
          OPERATOR="eq"
          POSX="500"
          POSY="100"
          PROPERTY="CHECK_ATTRIBUTES"
          THEN="DPF4CONVERT.checkattributes1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <IF-THEN-ELSE3COPY
          COMMENT=""
          ELSE="ocr2pdf1"
          OPERATOR="eq"
          POSX="300"
          POSY="100"
          PROPERTY="ENABLE_TIFFADAPT"
          THEN="MODIFY3COPY"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE3COPY>
        <MAIN
          BEGIN="check_filetype_pdf"
          NAME=""
          />
        <MAPPING/>
        <MODIFY3COPY
          COMMENT=""
          NEXT="DPF4CONVERT.tiffadapt1COPY"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            TIFFADAPT_ERROR_IF_REFUSED="Y"
            TIFFADAPT_FILECHECK_CONFIG_FILE="tiffocrcheck.cfg"
            />
        </MODIFY3COPY>
        <check_filetype_pdf
          COMMENT=""
          ELSE="IF-THEN-ELSE3COPY"
          OPERATOR="eq"
          POSX="400"
          POSY="0"
          PROPERTY="CURRFILETYPE"
          THEN="IF-THEN-ELSE1"
          TYPE="IF-THEN-ELSE"
          VALUE="pdf"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_filetype_pdf>
        <ocr2pdf1
          COMMENT=""
          ERROR="END"
          NAME="ocr2pdf"
          NEXT="END"
          POSX="400"
          POSY="600"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ocr2pdf1>
      </ocr2pdf>
    </workflows>
  </config>
</DPF>
