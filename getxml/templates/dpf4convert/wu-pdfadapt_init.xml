<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.pdfadapt_init
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfadapt_init.pl -parfile %PARFILENAME%"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Pdfadapt - Initialize"
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-pdfadapt_init.xml,v 1.6 2012/09/05 10:54:38 kig Exp $">
        <DEFAULTS
          PDFADAPT_ADJUST_CONFIG_FILE="pdfadjust.cfg"
          PDFADAPT_CHECK_NAME=""
          PDFADAPT_CHECKADJUST_CHECKS="CustomerChecks,PDFA1B-Colorspace,PDFA1B-Metadata,PDFA1B-Security,SEALChecks"
          PDFADAPT_CHECK_CONFIG_FILE="pdfchecker.cfg"
          PDFADAPT_ADAPT_CONFIG_FILE="pdfadapt.cfg"
          PDFADAPT_ERROR_IF_REFUSED="Y"
          PDFADAPT_STATES_ACCEPT_RESULT="OK"
          PDFADAPT_STATES_REQUEST_ADJUST="WARNING,ERROR"
          PDFADAPT_STATES_REQUEST_CONVERT="WARNING,ERROR,FATAL_FAILURE,INTERNAL_FAILURE,CHANGE_PROTECTED,SIGNATURE,UNKNOWN_FAILURE"
          />
      </DPF4CONVERT.pdfadapt_init>
    </workingunits>
  </config>
</DPF>
