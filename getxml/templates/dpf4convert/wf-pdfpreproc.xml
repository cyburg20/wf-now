<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.pdfpreproc
        COMMENT="The DPF-process PDFPREPROC performs a pre-processing of a PDF-file."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Pdfpreproc - Preprocess a PDF file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdfpreproc.xml,v 1.5 2017/06/28 08:55:53 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.pdfconvert2
          COMMENT=""
          ERROR="END"
          NEXT="modified_by_convert"
          POSX="500"
          POSY="1000"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfconvert"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfconvert2>
        <DPF4CONVERT.pdfexpand1
          COMMENT=""
          ERROR="END"
          NEXT="modified_by_expand"
          POSX="500"
          POSY="1400"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfexpand"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfexpand1>
        <END
          COMMENT=""
          NAME="END"
          POSX="900"
          POSY="1600"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="check_pdfconvert"
          OPERATOR="eq"
          POSX="300"
          POSY="700"
          PROPERTY="ENABLE_SIGNATURE_CHECK"
          THEN="pdfchecksignature"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <MAIN
          BEGIN="check_enable_preproc"
          NAME=""
          />
        <MAPPING/>
        <check_enable_preproc
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="100"
          POSY="100"
          PROPERTY="ENABLE_PDFPREPROC"
          THEN="check_pdf_status"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_enable_preproc>
        <check_expand
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="300"
          POSY="1300"
          PROPERTY="ENABLE_PDFEXPAND"
          THEN="DPF4CONVERT.pdfexpand1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_expand>
        <check_pdf_status
          COMMENT=""
          ELSE="END"
          OPERATOR="ne"
          POSX="300"
          POSY="100"
          PROPERTY="PDFFILE_STATUS"
          THEN="check_user_preproc"
          TYPE="IF-THEN-ELSE"
          VALUE="converted"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_pdf_status>
        <check_pdfconvert
          COMMENT=""
          ELSE="check_expand"
          OPERATOR="eq"
          POSX="300"
          POSY="900"
          PROPERTY="ENABLE_PDFCONVERT"
          THEN="DPF4CONVERT.pdfconvert2"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_pdfconvert>
        <check_user_preproc
          COMMENT=""
          ELSE="pdfinfo"
          OPERATOR="eq"
          POSX="300"
          POSY="300"
          PROPERTY="DPF4C_SWITCH_USER_PRE_PDFPREPROC"
          THEN="dpf4c_user_pre_pdfpreproc"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_user_preproc>
        <dpf4c_user_pre_pdfpreproc
          COMMENT=""
          ERROR="END"
          NEXT="pdfinfo"
          POSX="500"
          POSY="400"
          TYPE="SWF"
          USE="DPF4CONVERT.dpf4c_user_pre_pdfpreproc"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </dpf4c_user_pre_pdfpreproc>
        <modified_by_convert
          COMMENT=""
          NEXT="END"
          POSX="500"
          POSY="1200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFEXPAND="N"
            PDFINFO_FILE_MODIFIED="Y"
            />
        </modified_by_convert>
        <modified_by_expand
          COMMENT=""
          NEXT="END"
          POSX="500"
          POSY="1600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            PDFINFO_FILE_MODIFIED="Y"
            />
        </modified_by_expand>
        <pdfchecksignature
          COMMENT=""
          ERROR="END"
          NEXT="check_pdfconvert"
          POSX="500"
          POSY="800"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfchecksignature"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfchecksignature>
        <pdfinfo
          COMMENT=""
          ERROR="END"
          NEXT="IF-THEN-ELSE1"
          POSX="300"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfinfo"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfinfo>
      </DPF4CONVERT.pdfpreproc>
    </workflows>
  </config>
</DPF>
