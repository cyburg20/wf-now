<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.DPF4Convert_Convert2PDF
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF4Convert_Convert2PDF"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-dpf4convert_convert2pdf.xml,v 1.7 2015/12/08 14:53:28 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.getfiletypefromname1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.getfiletypefromname"
          NEXT="check_mapped_filename"
          POSX="900"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.getfiletypefromname1>
        <DPF4CONVERT.mapshortstampvalues1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.mapshortstampvalues"
          NEXT="check_if_currfiletype_is_set"
          POSX="800"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="HORIZONTAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.mapshortstampvalues1>
        <END
          COMMENT=""
          NAME="END"
          POSX="1400"
          POSY="500"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="modify_convert_params"
          NAME=""
          />
        <MAPPING/>
        <MODIFY_appkonv_section
          COMMENT=""
          NEXT="dpf4c_appkonv"
          POSX="1100"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            AK_SECTION=".%CURRFILETYPE%"
            />
        </MODIFY_appkonv_section>
        <checkForStamping
          COMMENT=""
          ELSE="check_user_post_stamp"
          OPERATOR="eq"
          POSX="1500"
          POSY="100"
          PROPERTY="ACTIVATE_STAMP"
          THEN="pdfstamp"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </checkForStamping>
        <check_for_stamps
          COMMENT=""
          ELSE="check_if_currfiletype_is_set"
          OPERATOR="eq"
          POSX="600"
          POSY="100"
          PROPERTY="ACTIVATE_STAMP"
          THEN="set_stamp_flags"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="HORIZONTAL"
            />
        </check_for_stamps>
        <check_if_currfiletype_is_set
          COMMENT=""
          ELSE="dpf4c_appkonv"
          OPERATOR="eq"
          POSX="900"
          POSY="100"
          PROPERTY="CURRFILETYPE"
          THEN="DPF4CONVERT.getfiletypefromname1"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_if_currfiletype_is_set>
        <check_if_pdfa_is_wanted
          COMMENT=""
          ELSE="check_for_stamps"
          OPERATOR="eq"
          POSX="300"
          POSY="100"
          PROPERTY="CREATE_PDFA"
          THEN="check_quality"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_if_pdfa_is_wanted>
        <check_mapped_filename
          COMMENT=""
          ELSE="dpf4c_appkonv"
          OPERATOR="eq"
          POSX="1100"
          POSY="300"
          PROPERTY="MAPPED_FILE_NAME"
          THEN="MODIFY_appkonv_section"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </check_mapped_filename>
        <check_quality
          COMMENT=""
          ELSE="set_quality_pdfa"
          OPERATOR="exists"
          POSX="300"
          POSY="300"
          PROPERTY="DESTINATION_FORMAT_QUALITY"
          THEN="set_pdfa_flags"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_quality>
        <check_user_post_stamp
          COMMENT=""
          ELSE="END"
          OPERATOR="ne"
          POSX="1700"
          POSY="100"
          PROPERTY="DPF4CONVERT.Convert2PdfUserPostStamp"
          THEN="do_user_post_stamp"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_user_post_stamp>
        <do_user_post_stamp
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1800"
          POSY="300"
          TYPE="USER_EXIT"
          USE="%DPF4CONVERT.Convert2PdfUserPostStamp%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </do_user_post_stamp>
        <dpf4c_appkonv
          COMMENT=""
          ERROR="END"
          NEXT="checkForStamping"
          POSX="1300"
          POSY="100"
          TYPE="SWF"
          USE="dpf4c_appkonv"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </dpf4c_appkonv>
        <modify_convert_params
          COMMENT=""
          NEXT="check_if_pdfa_is_wanted"
          POSX="100"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_SWITCH_APPKONV="Y"
            />
        </modify_convert_params>
        <pdfstamp
          COMMENT=""
          ERROR="END"
          NEXT="check_user_post_stamp"
          POSX="1600"
          POSY="300"
          TYPE="SWF"
          USE="pdfstamp"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfstamp>
        <set_pdfa_flags
          COMMENT=""
          NEXT="check_for_stamps"
          POSX="500"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            DPF4C_SWITCH_MODIFY="Y"
            ENABLE_PDFADAPT="Y"
            />
        </set_pdfa_flags>
        <set_quality_pdfa
          COMMENT=""
          NEXT="set_pdfa_flags"
          POSX="300"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            DESTINATION_FORMAT_QUALITY="pdfa"
            />
        </set_quality_pdfa>
        <set_stamp_flags
          COMMENT=""
          NEXT="DPF4CONVERT.mapshortstampvalues1"
          POSX="700"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            DPF4C_MODIFY_STAMP="Y"
            DPF4C_SWITCH_MODIFY="Y"
            />
        </set_stamp_flags>
      </DPF4CONVERT.DPF4Convert_Convert2PDF>
    </workflows>
  </config>
</DPF>
