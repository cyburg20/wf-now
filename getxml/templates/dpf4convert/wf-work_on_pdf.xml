<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.work_on_pdf
        COMMENT="dpf4convert - Summarize processing steps for a PDF-file

This process performs the steps PDF-process, create target formats and PDF-postproc on a PDF-file.
"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Work On PDF - Summarize processing a PDF-file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-work_on_pdf.xml,v 1.1 2010/11/18 11:27:48 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.pdfpostproc
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="200"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfpostproc"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfpostproc>
        <DPF4CONVERT.pdfprocess
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.pdftarget"
          POSX="200"
          POSY="100"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprocess"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfprocess>
        <DPF4CONVERT.pdftarget
          COMMENT=""
          ERROR="END"
          NEXT="check_activate_pdfadapt"
          POSX="200"
          POSY="300"
          TYPE="SWF"
          USE="DPF4CONVERT.pdftarget"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdftarget>
        <END
          COMMENT=""
          NAME="END"
          POSX="400"
          POSY="700"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="DPF4CONVERT.pdfprocess"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_activate_pdfadapt
          COMMENT=""
          ELSE="DPF4CONVERT.pdfpostproc"
          OPERATOR="contains"
          POSX="200"
          POSY="500"
          PROPERTY="DESTINATION_FORMAT_QUALITY"
          THEN="enable_pdfadapt"
          TYPE="IF-THEN-ELSE"
          VALUE="pdfa"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_activate_pdfadapt>
        <enable_pdfadapt
          COMMENT=""
          NEXT="DPF4CONVERT.pdfpostproc"
          POSX="400"
          POSY="600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFADAPT="Y"
            />
        </enable_pdfadapt>
      </DPF4CONVERT.work_on_pdf>
    </workflows>
  </config>
</DPF>
