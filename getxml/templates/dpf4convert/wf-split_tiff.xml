<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.split_tiff
        COMMENT="dpf4convert - PROCESS FOR SPLITTING MULTIPAGE TIFF FILES"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - SubProcess dpf4c_split_tiff"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-split_tiff.xml,v 1.2 2017/01/18 08:25:10 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="900"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_common_outfiletif"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_common_outfiletif
          COMMENT=""
          ELSE="check_currfiletype"
          OPERATOR="exists"
          POSX="100"
          POSY="100"
          PROPERTY="DPF4C_COMMON_OUTFILEtif"
          THEN="set_currfilename"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_common_outfiletif>
        <check_currfiletype
          COMMENT=""
          ELSE="error_not_supported_formatforsplitting"
          OPERATOR="contains"
          POSX="100"
          POSY="300"
          PROPERTY="CURRFILETYPE"
          THEN="tiffsplit_outfiletif"
          TYPE="IF-THEN-ELSE"
          VALUE="tif"
          >
          <CONNECTIONS
            ELSE="HORIZONTAL"
            THEN="VERTICAL"
            />
        </check_currfiletype>
        <error_not_supported_formatforsplitting
          COMMENT=""
          NEXT="exit_processCOPY"
          POSX="300"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            ERR_REASON="Error: This CURRFILETYPE -%CURRFILETYPE%- is not supported for splitting!"
            ERR_WFNAME="DPF4C_SPLIT_FILE"
            ERR_WFPOSITION="check_needed_split"
            />
        </error_not_supported_formatforsplitting>
        <exit_processCOPY
          COMMENT=""
          ERROR="END"
          NAME="exit_process"
          NEXT="END"
          POSX="500"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </exit_processCOPY>
        <set_currfilename
          COMMENT=""
          NEXT="tiffsplit_outfiletif"
          POSX="300"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILENAME="%DPF4C_COMMON_OUTFILEtif%"
            CURRFILETYPE="tif"
            />
        </set_currfilename>
        <tiffsplit_outfiletif
          COMMENT=""
          ERROR="END"
          NAME="tiffsplit"
          NEXT="END"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </tiffsplit_outfiletif>
      </DPF4CONVERT.split_tiff>
    </workflows>
  </config>
</DPF>
