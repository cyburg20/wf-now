<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.catdrawing2file
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Catdrawing2file - Convert Catia Drawing"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-catdrawing2file.xml,v 1.1 2012/01/24 10:04:20 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_converter"
          NAME=""
          />
        <MAPPING/>
        <catdrawing2file
          COMMENT=""
          ERROR="END"
          NAME="CENIT.catdrawing2file"
          NEXT="handle_cat_output"
          POSX="600"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </catdrawing2file>
        <catia2pdf1
          COMMENT=""
          ERROR="END"
          NAME="catia2pdf"
          NEXT="END"
          POSX="400"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <CATIA_CONVERSION_METHOD
              val="SPRING"
              />
            <CATIA_CONVERSION_METHOD_SPRING_SCRIPT
              val="%PLSROOT%tools/cad/catia/spring/dummy_conversion.bat"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </catia2pdf1>
        <check_converter
          COMMENT=""
          ELSE="check_property_update"
          OPERATOR="contains"
          POSX="200"
          POSY="200"
          PROPERTY="CATIA_CONVERTER"
          THEN="catia2pdf1"
          TYPE="IF-THEN-ELSE"
          VALUE="external"
          >
          <CONNECTIONS
            ELSE="HORIZONTAL"
            THEN="DIRECT"
            />
        </check_converter>
        <check_property_update
          COMMENT=""
          ELSE="catdrawing2file"
          OPERATOR="eq"
          POSX="400"
          POSY="400"
          PROPERTY="DPF4C_PROPERTYUPDATE_ENABLE"
          THEN="property_update_catia"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_property_update>
        <handle_cat_output
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.handle_cat_output"
          NEXT="END"
          POSX="800"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </handle_cat_output>
        <property_update_catia
          COMMENT=""
          ERROR="END"
          NAME="CENIT.property_update_catia"
          NEXT="catdrawing2file"
          POSX="500"
          POSY="600"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </property_update_catia>
      </DPF4CONVERT.catdrawing2file>
    </workflows>
  </config>
</DPF>
