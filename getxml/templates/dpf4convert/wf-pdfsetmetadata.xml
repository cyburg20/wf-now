<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.Pdfsetmetadata
        COMMENT="The DPF-process PDFSETMETADATA sets meta data in a PDF-file"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Pdfsetmetadata - set meta data in a PDF file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdfsetmetadata.xml,v 1.1 2016/02/04 16:29:49 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.pdfmeta1
          COMMENT=""
          ERROR="END"
          NAME="pdfmeta"
          NEXT="END"
          POSX="100"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfmeta1>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_set_pdf_metadata"
          NAME=""
          />
        <MAPPING/>
        <check_set_pdf_metadata
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="100"
          POSY="100"
          PROPERTY="DPF4C_SET_PDF_META_DATA"
          THEN="DPF4CONVERT.pdfmeta1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_set_pdf_metadata>
      </DPF4CONVERT.Pdfsetmetadata>
    </workflows>
  </config>
</DPF>
