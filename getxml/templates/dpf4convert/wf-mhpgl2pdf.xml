<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <hpgl2pdf_cp
        COMMENT="dpf4convert - Convert hpgl file to pdf file and copy back"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - hpgl file to pdf file and copy back"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-mhpgl2pdf.xml,v 1.9 2012/12/21 15:01:55 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.copy_currfilename1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.copy_currfilename"
          NEXT="END"
          POSX="500"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.copy_currfilename1>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="read_ini_file"
          ERROR="END"
          />
        <MAPPING/>
        <hpgl2pdf
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.copy_currfilename1"
          POSX="300"
          POSY="200"
          TYPE="SWF"
          USE="hpgl2pdf"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </hpgl2pdf>
        <read_ini_file
          COMMENT=""
          ERROR="END"
          NAME="dpf4c_read_ini_file"
          NEXT="hpgl2pdf"
          POSX="100"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </read_ini_file>
      </hpgl2pdf_cp>
      <mhpgl2pdf
        COMMENT="dpf4convert - Convert multi hpgl files to single multipage pdf file"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - multi hpgl files to single multipage pdf file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-mhpgl2pdf.xml,v 1.9 2012/12/21 15:01:55 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.copy_currfilename1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.copy_currfilename"
          NEXT="END"
          POSX="500"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.copy_currfilename1>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="mhpgl2mpdf"
          ERROR="END"
          />
        <MAPPING/>
        <mhpgl2mpdf
          COMMENT=""
          ERROR="END"
          NAME="mhpgl2mpdf"
          NEXT="pdfmerge"
          POSX="100"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            SubProcess="hpgl2pdf_cp"
            />
          <MAPPING/>
        </mhpgl2mpdf>
        <pdfmerge
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.copy_currfilename1"
          POSX="300"
          POSY="200"
          TYPE="SWF"
          USE="pdfmerge"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfmerge>
      </mhpgl2pdf>
    </workflows>
  </config>
</DPF>
