<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <coreldesigner2file
        NAME="dpf4c - Convert CorelDesigner to PDF"
        VERSION="$Id: wu-coreldesigner2file.xml,v 1.1 2006/10/23 12:55:17 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-coreldesigner2file"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/coreldesigner2file.pl -parfile %PARFILENAME%">
        <DEFAULTS
          WU_ENABLE_DEBUG_LOOP="N"
          DPF4C_DSF2PDF_EXT=""
          />
        <INPUT
          PARFILENAME="*"
          CURRFILENAME="*"
          JOBID="*"
          CURRFILETYPE="dsf"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
      </coreldesigner2file>
    </workingunits>
  </config>
</DPF>
