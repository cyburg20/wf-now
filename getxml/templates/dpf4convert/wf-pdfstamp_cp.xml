<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <pdfstamp_cp
        COMMENT="dpf4convert - Stamp pdf file and copy file to targetdir"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - pdfstamp_cp"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdfstamp_cp.xml,v 1.2 2010/11/15 16:18:48 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.copy_currfilename1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.copy_currfilename"
          NEXT="END"
          POSX="300"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.copy_currfilename1>
        <END
          COMMENT=""
          NAME="END"
          POSX="500"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="pdfstamp"
          ERROR="END"
          />
        <MAPPING/>
        <pdfstamp
          COMMENT=""
          ERROR="END"
          NAME="pdfstamp"
          NEXT="DPF4CONVERT.copy_currfilename1"
          POSX="100"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfstamp>
      </pdfstamp_cp>
    </workflows>
  </config>
</DPF>
