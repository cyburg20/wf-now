<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <dvsocr2pdf
        COMMENT="dpf4convert - process for OCR "
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF - Main process dvsocr2pdf"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-dvsocr2pdf.xml,v 1.9 2013/06/28 10:28:57 dei Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.pdfadapt1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="IF-THEN-ELSE1COPY1"
          POSX="700"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfadapt"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfadapt1>
        <END
          COMMENT=""
          NAME="END"
          POSX="1500"
          POSY="200"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="MODIFY1"
          OPERATOR="contains"
          POSX="300"
          POSY="400"
          PROPERTY="DPF4C_SWITCH_DO_USER_PRE_OCR2PDF"
          THEN="ocr_user_pre_ocr"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <IF-THEN-ELSE1COPY1
          COMMENT=""
          ELSE="ocr_checkin"
          OPERATOR="contains"
          POSX="300"
          POSY="1500"
          PROPERTY="DPF4C_SWITCH_DO_USER_POST_OCR2PDF"
          THEN="ocr_user_post_ocr"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1COPY1>
        <IF-THEN-ELSE2
          COMMENT=""
          ELSE="IF-THEN-ELSE1COPY1"
          OPERATOR="eq"
          POSX="300"
          POSY="1000"
          PROPERTY="DPF4C_ENABLE_PDFCHECK"
          THEN="check_pdfadapt"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE2>
        <MAIN
          BEGIN="set_ocr"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="ocr2pdf1"
          POSX="300"
          POSY="600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_ENABLE_PDFCHECK="N"
            />
        </MODIFY1>
        <MODIFY2
          COMMENT=""
          NEXT="pdfcheckadjust1"
          POSX="500"
          POSY="1200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFADJUST_PROCESS="Y"
            />
        </MODIFY2>
        <check_pdfadapt
          COMMENT=""
          ELSE="set_enable_pdfadapt"
          OPERATOR="eq"
          POSX="500"
          POSY="1000"
          PROPERTY="ENABLE_PDFADAPT"
          THEN="MODIFY2"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </check_pdfadapt>
        <dpf4c_read_ini_file1
          COMMENT=""
          ERROR="END"
          NAME="dpf4c_read_ini_file"
          NEXT="ocr_read_sap_files"
          POSX="300"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </dpf4c_read_ini_file1>
        <ocr2pdf1
          COMMENT=""
          ERROR="END"
          NEXT="IF-THEN-ELSE2"
          POSX="300"
          POSY="800"
          TYPE="SWF"
          USE="ocr2pdf"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ocr2pdf1>
        <ocr_checkin
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="plm_checkin_files_sap"
          NEXT="END"
          POSX="300"
          POSY="1700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ocr_checkin>
        <ocr_checkout
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="checkout_files_local_copy"
          NEXT="IF-THEN-ELSE1"
          POSX="700"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="HORIZONTAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ocr_checkout>
        <ocr_read_sap_files
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="plm_data_mapping_sap"
          NEXT="ocr_checkout"
          POSX="500"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ocr_read_sap_files>
        <ocr_user_post_ocr
          COMMENT=""
          ERROR="END"
          NEXT="ocr_checkin"
          POSX="100"
          POSY="2200"
          TYPE="SWF"
          USE="DPF4CONVERT.dvsocr_user_post_ocr"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ocr_user_post_ocr>
        <ocr_user_pre_ocr
          COMMENT=""
          ERROR="END"
          NEXT="MODIFY1"
          POSX="100"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.dvsocr_user_pre_ocr"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ocr_user_pre_ocr>
        <pdfcheckadjust1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="IF-THEN-ELSE1COPY1"
          POSX="500"
          POSY="1400"
          TYPE="SWF"
          USE="pdfcheckadjust"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfcheckadjust1>
        <set_enable_pdfadapt
          COMMENT=""
          NEXT="DPF4CONVERT.pdfadapt1"
          POSX="700"
          POSY="1200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFADAPT="Y"
            />
        </set_enable_pdfadapt>
        <set_ocr
          COMMENT=""
          NEXT="dpf4c_read_ini_file1"
          POSX="100"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_DEST_FORMAT="pdf"
            />
        </set_ocr>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="900"
          POSY="1700"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
      </dvsocr2pdf>
    </workflows>
  </config>
</DPF>
