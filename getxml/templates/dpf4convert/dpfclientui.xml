    <UserInterface>
      <CommonSettings
        SHOW_HELPMENU="Y"
        SHOW_FILEMENU="Y"
        SHOW_LANGUAGEMENU="Y"
        WINDOW_SIZE="384 , 800"
        HELPROOT="%DPFROOT%/source/dpf/dpfserver/dpfclientui_help"
        CONFIG_PRIVATEPLOTTERLIST="N"
        EXPANDED="Y"
        PLOTTERLIST="PLOTTERLIST"
        SHOW_DECORATEDPRINTERNAMES="N"
        ITEM_COLUMNS="1"
        SPLIT_PARTS="180 , 169"
        PLOTTERPARAM="EXPANDABLE"
        MUSTTEXTCOLOR="Black"
        MUSTBACKGROUNDCOLOR="LightCoral"
        BACKGROUNDCOLOR="Red"
        CONFIG_PLOTTER="Y"
        GLOBAL="GLOBAL"
        PLOTTERPARAMoff="Y|N|EXPANDABLE"
        />
      <WorkflowDescriptor
        ORDER="COMMENT LOCATION"
        >
        <COMMENT
          TYPE="Static"
          DEFAULT="PLOTTER_COMMENT"
          />
        <LOCATION
          TYPE="Static"
          DEFAULT="PLOTTER_LOCATION"
          />
      </WorkflowDescriptor>
      <Global
        ORDER="CURRFILENAME"
        >
        <CURRFILENAME
          TYPE = "File"
          RANGE = "All (*.*)|AutoCAD (*.dft)|AutoCAD (*.dwg)|Computer Graphics Metafile CGM (*.cgm)|Corel Draw CDR (*.cdr)|HP Graphics Language HPGL (*.hpgl)|Inventor (*.idw)|ME10 (*.me)|Microsoft Excel XLS (*.xls)|Microsoft PowerPoint MPP (*.mpp)|Microsoft PowerPoint MPV (*.mpv)|Microsoft PowerPoint PPT (*.ppt)|Microsoft Visio VSD (*.vsd)|Microsoft Word DOC (*.doc)|Microsoft Word DOT (*.dot)|PostScript PS (*.ps)|ProE (*.proe)|Rich Text Format RTF (*.rtf)|SAPGof (*.otf)|Solid Edge (*.dft)|Solid Works (*.drw)|TXT (*.txt)"
          DEFAULT = ""
          />
        <DPF_SRV_GET_FILES
          RANGE=""
          TYPE="Hidden"
          DEFAULT="CURRFILENAME"
          />
        <PLS_USERNAME
          RANGE=""
          TYPE="Hidden"
          DEFAULT="UNKNOWN"
          />
        <PLS_SRCNODE
          RANGE=""
          TYPE="Hidden"
          DEFAULT="UNKNOWN"
          />
        <DELETEJOBoff
          RANGE=""
          TYPE="Hidden"
          DEFAULT="3d"
          />
      </Global>
      <WorkflowDescriptorOFF>
        <COMMENT
          TYPE="Static"
          COMMENT="Wenn der Defaultwert leer ist, kommt hier der Kommentar aus der Workflowdefinition rein"
          DEFAULT="WORKFLOW_COMMENT"
          />
        <VERSION
          TYPE="Static"
          COMMENT="Wenn der Defaultwert leer ist, kommt hier die Version aus der Workflowdefinition rein"
          DEFAULT=""
          />
      </WorkflowDescriptorOFF>
