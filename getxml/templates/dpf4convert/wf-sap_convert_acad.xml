<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.sap_convert_acad
        COMMENT="dpf4convert - PROCESS SAP CONVERT AUTOACAD.
This process is used to convert AutoCAD-files within an SAP integration."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="SAP Convert Acad"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-sap_convert_acad.xml,v 1.4 2012/05/10 12:48:42 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.sap_readcustomconfig
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.sap_readcustomconfig"
          NEXT="DPF4CONVERT.sap_get_data"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.sap_readcustomconfig>
        <DPF4CONVERT.sap_get_data
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY1"
          POSX="200"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.sap_get_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.sap_get_data>
        <DPF4CONVERT.sap_put_data
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY3"
          POSX="200"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4CONVERT.sap_put_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.sap_put_data>
        <DPF4CONVERT.work_on_acad1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY2"
          POSX="200"
          POSY="1100"
          TYPE="SWF"
          USE="DPF4CONVERT.work_on_acad"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.work_on_acad1>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1800"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="DPF4CONVERT.work_on_acad1"
          POSX="200"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="33"
            />
        </MODIFY1>
        <MODIFY2
          COMMENT=""
          NEXT="DPF4CONVERT.sap_put_data"
          POSX="200"
          POSY="1300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="66"
            />
        </MODIFY2>
        <MODIFY3
          COMMENT=""
          NEXT="END"
          POSX="200"
          POSY="1700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="100"
            />
        </MODIFY3>
        <set_config
          COMMENT=""
          NEXT="DPF4CONVERT.sap_readcustomconfig"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILES_SUPPORTED="Y"
            DPF4C_COMMON_PLM_SYSTEM_NAME="sap"
            DPF4C_CUSTOM_CONFIG="dpf4convert::sap_convert,dpf4convert::convert_acad,dpf4convert::error_handling"
            DPFJOBPROGRESS="0"
            />
        </set_config>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="600"
          POSY="1700"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <version_dpf4c
          COMMENT=""
          ERROR="END"
          NEXT="set_config"
          POSX="200"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c>
      </DPF4CONVERT.sap_convert_acad>
    </workflows>
  </config>
</DPF>
