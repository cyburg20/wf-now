<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.ocr2txt
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/dpf4convert/ocr2txt.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGNAME=""
        LOGSTDOUT="Y"
        NAME="Ocr2txt - Retrieve text from a file by OCR"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-ocr2txt.xml,v 1.4 2017/03/14 06:37:56 kig Exp $"
        >
        <DEFAULTS
          DPF4C_OCR2TXT_DA_TYPE="2"
          DPF4C_OCR2TXT_DETECT_ORIENTATION="Y"
          DPF4C_OCR2TXT_LANGUAGES="German,English"
          DPF4C_OCR2TXT_MIN_VOLUME=""
          DPF4C_OCR2TXT_PROFILE="DocumentConversion_Accuracy"
          DPF4C_OCR2TXT_SKEWMODE=""
          DPF4C_OCR2TXT_USERPROFILE=""
          />
        <INPUT>
          <DPF4C_OCR2TXT_DA_TYPE
            TYPE="TEXT"
            />
          <DPF4C_OCR2TXT_DETECT_ORIENTATION
            TYPE="TEXT"
            />
          <DPF4C_OCR2TXT_LANGUAGES
            TYPE="TEXT"
            />
          <DPF4C_OCR2TXT_MIN_VOLUME
            TYPE="TEXT"
            />
          <DPF4C_OCR2TXT_PROFILE
            TYPE="TEXT"
            />
          <DPF4C_OCR2TXT_SKEWMODE
            TYPE="TEXT"
            />
          <DPF4C_OCR2TXT_USERPROFILE
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </DPF4CONVERT.ocr2txt>
    </workingunits>
  </config>
</DPF>
