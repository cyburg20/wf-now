<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.sap_convert_custom
        COMMENT="DPF4CONVERT - PROCESS SAP_CONVERT_CUSTOM.
This process is used for custom specific conversions out of SAP."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="SAP Convert Custom"
        LICENSE="CCUSTOM"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-sap_convert_custom.xml,v 1.5 2017/02/08 12:46:26 thomas Exp $"
        >
        <DEFAULTS
          SapConvertCustomUserExitConvert="dpf4convert.sample_convert"
          />
        <DPF4CONVERT.sap_get_data
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_wu_targetdir"
          POSX="200"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.sap_get_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.sap_get_data>
        <DPF4CONVERT.sap_put_data
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_final_jobprogress"
          POSX="200"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4CONVERT.sap_put_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.sap_put_data>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1800"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <custom_convert
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="unset_wu_targetdir"
          POSX="200"
          POSY="1100"
          TYPE="USER_EXIT"
          USE="%SapConvertCustomUserExitConvert%"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </custom_convert>
        <readcustomconfig
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.readcustomconfig"
          NEXT="DPF4CONVERT.sap_get_data"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </readcustomconfig>
        <set_config
          COMMENT=""
          NEXT="readcustomconfig"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_PLM_SYSTEM_NAME="sap"
            DPF4C_CUSTOM_CONFIG="dpf4convert::convert_custom,dpf4convert::sap_convert,dpf4convert::error_handling"
            DPFJOBPROGRESS="0"
            />
        </set_config>
        <set_final_jobprogress
          COMMENT=""
          NEXT="END"
          POSX="200"
          POSY="1700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="100"
            />
        </set_final_jobprogress>
        <set_wu_targetdir
          COMMENT=""
          NEXT="custom_convert"
          POSX="200"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR="%JOBDIR%"
            DPFJOBPROGRESS="33"
            />
        </set_wu_targetdir>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="500"
          POSY="1700"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <unset_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.sap_put_data"
          POSX="200"
          POSY="1300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR=""
            DPFJOBPROGRESS="66"
            />
        </unset_wu_targetdir>
        <version_dpf4c
          COMMENT=""
          ERROR="END"
          NEXT="set_config"
          POSX="200"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c>
      </DPF4CONVERT.sap_convert_custom>
    </workflows>
  </config>
</DPF>
