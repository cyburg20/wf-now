<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <idw2file
        NAME="dpf4c - CAD Inventor IDW to PS"
        VERSION="$Id: wu-idw2file.xml,v 1.9 2007/07/11 15:09:51 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-idw2file"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/idw2file.pl -parfile %PARFILENAME%">
        <DEFAULTS
          DESTINATION_FORMAT_QUALITY=""
          DPF4C_COMMON_DEST_COLOR="color"
          DPF4C_COMMON_DEST_FORMAT="pdf"
          DPF4C_COMMON_MULTIPAGE="Y"
          DPF4C_COMMON_TIMEOUT="20"
          DPF4C_PROPERTYUPDATE_ENABLE="N"
          DPF4C_PROPERTYUPDATE_FILE=""
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="N"
          IDW2FILE_ENABLE_PROPERTY_LOG="Y"
          IDW2FILE_ENABLE_FORCE_PROP_MODEL="N"
          IDW2FILE_GENERAL_MULTISHEET="Y"
          IDW2FILE_IGNORE_LLC_ERRORS="Y"
          IDW2FILE_LLC_CONFIG_FILE="%PLSROOT%server/cad/inventor/layer_control/inventor_llc_mapping.xml"
          IDW2FILE_LLC_MAPPING_ID=""
          IDW2FILE_PRINTER="SEALPS4CAD"
          IDW2FILE_PROPERTYUPDATE_DELETE_EMPTY_VALUES="N"
          IDW2FILE_PUBLISH_COMPONENT_PROPS="N"
          IDW2FILE_PUBLISH_MASS_PROPS="N"
          IDW2FILE_SHEET_CONTROL_FLAG=""
          IDW2FILE_START_VISIBLE="N"
          IDW2FILE_USE_SEPPEXECUTE_FOR_TIMEOUT="N"
          IDW2FILE_USE_SEPPSTARTER="N"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="idw"
          DESTINATION_FORMAT_QUALITY="pdfa|"
          DPF4C_COMMON_DEST_COLOR="black|color"
          DPF4C_COMMON_DEST_FORMAT="pdf,dwg,dxf,dwf"
          DPF4C_COMMON_MULTIPAGE="Y|N"
          DPF4C_COMMON_TIMEOUT="number"
          DPF4C_PROPERTYUPDATE_ENABLE="Y|N"
          DPF4C_PROPERTYUPDATE_FILE="file"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y|N"
          IDW2FILE_ENABLE_PROPERTY_LOG="Y|N"
          IDW2FILE_ENABLE_FORCE_PROP_MODEL="Y|N"
          IDW2FILE_GENERAL_MULTISHEET="Y|N"
          IDW2FILE_IGNORE_LLC_ERRORS="Y|N"
          IDW2FILE_LLC_CONFIG_FILE="file"
          IDW2FILE_LLC_MAPPING_ID="string"
          IDW2FILE_PRINTER="printername"
          IDW2FILE_PROPERTYUPDATE_DELETE_EMPTY_VALUES="Y|N"
          IDW2FILE_PUBLISH_COMPONENT_PROPS="Y|N"
          IDW2FILE_PUBLISH_MASS_PROPS="Y|N"
          IDW2FILE_SHEET_CONTROL_FLAG="string"
          IDW2FILE_START_VISIBLE="Y|N"
          IDW2FILE_USE_SEPPEXECUTE_FOR_TIMEOUT="Y|N"
          IDW2FILE_USE_SEPPSTARTER="Y|N"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf|tif"
          DPF4C_COMMON_DEST_COLOR="color"
          DPF4C_COMMON_DEST_FORMAT="string"
          DPF4C_COMMON_OUTFILEx="file"
          DPF4C_PROPERTYUPDATE_OUTFILE1="idw file"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y|"
          DPF4C_SWITCH_CREATE_DESTFORMATS="N"
          DPF4C_SWITCH_MODIFY="N"
          ERR_REASON="text"
          />
      </idw2file>
    </workingunits>
  </config>
</DPF>
