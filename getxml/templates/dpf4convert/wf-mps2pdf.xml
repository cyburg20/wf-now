<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <mps2pdf
        COMMENT="dpf4convert - Convert multi ps files to single multipage pdf file"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - multi ps files to single multipage pdf file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-mps2pdf.xml,v 1.5 2012/12/21 14:41:21 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.copy_currfilename1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.copy_currfilename"
          NEXT="END"
          POSX="500"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.copy_currfilename1>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="mps2mpdf"
          ERROR="END"
          />
        <MAPPING/>
        <mps2mpdf
          COMMENT=""
          ERROR="END"
          NAME="mps2mpdf"
          NEXT="pdfmerge"
          POSX="100"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            SubProcess="ps2pdf_cp"
            />
          <MAPPING/>
        </mps2mpdf>
        <pdfmerge
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.copy_currfilename1"
          POSX="300"
          POSY="200"
          TYPE="SWF"
          USE="pdfmerge"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfmerge>
      </mps2pdf>
    </workflows>
  </config>
</DPF>
