<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.cef_get_data
        COMMENT="Cef Convert - SUBPROCESS CEF GET DATA.

This subprocess is used from the main CEF CONVERT processes to get files."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="CEF Convert - SubProcess CEF Get Data"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-cef_get_data.xml,v 1.1 2017/08/24 09:30:33 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.cef_get_files1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.cef_get_files"
          NEXT="END"
          POSX="200"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.cef_get_files1>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="1200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_switch_do_user_pre_get_files"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <cef_user_pre_get_data
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.cef_get_files1"
          POSX="400"
          POSY="200"
          TYPE="USER_EXIT"
          USE="%DPF4CONVERT.CefUserPreGetFiles%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </cef_user_pre_get_data>
        <check_switch_do_user_pre_get_files
          COMMENT=""
          ELSE="DPF4CONVERT.cef_get_files1"
          OPERATOR="ne"
          POSX="200"
          POSY="100"
          PROPERTY="DPF4CONVERT.CefUserPreGetFiles"
          THEN="cef_user_pre_get_data"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_do_user_pre_get_files>
      </DPF4CONVERT.cef_get_data>
    </workflows>
  </config>
</DPF>
