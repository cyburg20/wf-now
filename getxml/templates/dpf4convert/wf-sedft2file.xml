<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <sedft2file
        COMMENT="dpf4convert - Convert process SolidEdge 2D/3D to file"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - CAD SolidEdge File Converter"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-sedft2file.xml,v 2.1 2008/06/02 12:08:36 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.se_ps2pdf
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.se_ps2pdf"
          NEXT="END"
          POSX="900"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.se_ps2pdf>
        <DPF4CONVERT.setcadmodelspace1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.setcadmodelspace"
          NEXT="IF-THEN-ELSE4"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.setcadmodelspace1>
        <END
          COMMENT=""
          NAME="END"
          POSX="900"
          POSY="200"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="IF-THEN-ELSE3"
          OPERATOR="eq"
          POSX="100"
          POSY="100"
          PROPERTY="CAD_SE_USE_XML_CONV_BIN"
          THEN="sedft2file1"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <IF-THEN-ELSE2
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="700"
          POSY="300"
          PROPERTY="CAD_SE_SWITCH_COMBINE_PS2PDF"
          THEN="DPF4CONVERT.se_ps2pdf"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE2>
        <IF-THEN-ELSE3
          COMMENT=""
          ELSE="DPF4CONVERT.setcadmodelspace1"
          OPERATOR="exists"
          POSX="100"
          POSY="400"
          PROPERTY="DPF4C_CAD_MODEL_SPACE"
          THEN="IF-THEN-ELSE4"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE3>
        <IF-THEN-ELSE4
          COMMENT=""
          ELSE="se_3d_2file1"
          OPERATOR="eq"
          POSX="300"
          POSY="400"
          PROPERTY="DPF4C_CAD_MODEL_SPACE"
          THEN="se_2d_2file"
          TYPE="IF-THEN-ELSE"
          VALUE="2D"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE4>
        <MAIN
          BEGIN="IF-THEN-ELSE1"
          ERROR="END"
          />
        <check_switch_portrait_rotatation
          COMMENT=""
          ELSE="END"
          OPERATOR="exists"
          POSX="500"
          POSY="100"
          PROPERTY="DPF4C_MODIFY_ROTATION_TRANSLATION_FILE"
          THEN="pdfrotate"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_switch_portrait_rotatation>
        <pdfrotate
          COMMENT=""
          ERROR="END"
          NAME="pdfrotate"
          NEXT="set_after_rotate"
          POSX="700"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfrotate>
        <se_2d_2file
          COMMENT=""
          ERROR="END"
          NAME="se_2d_2file"
          NEXT="IF-THEN-ELSE2"
          POSX="500"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </se_2d_2file>
        <se_3d_2file1
          COMMENT=""
          ERROR="END"
          NAME="se_3d_2file"
          NEXT="END"
          POSX="500"
          POSY="500"
          TYPE="WU"
          >
          <DEFAULTS/>
          <MAPPING/>
        </se_3d_2file1>
        <sedft2file1
          COMMENT=""
          ERROR="END"
          NAME="sedft2file"
          NEXT="check_switch_portrait_rotatation"
          POSX="300"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sedft2file1>
        <set_after_rotate
          COMMENT=""
          NEXT="END"
          POSX="900"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_MODIFY_ROTATE="%DPF4C_MODIFY_ROTATE_save%"
            DPF4C_MODIFY_ROTATION_TRANSLATION_FILE=""
            />
        </set_after_rotate>
      </sedft2file>
    </workflows>
  </config>
</DPF>
