<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <pdfencrypt
        NAME="dpf4c - Encrypt PDF"
        LICENSE="CWPROTECT"
        VERSION="$Id: wu-pdfencrypt.xml,v 1.4 2017/02/08 12:46:28 thomas Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-pdfencrypt"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfencrypt.pl -parfile %PARFILENAME%">
        <DEFAULTS
          WU_ENABLE_DEBUG_LOOP="N"
          DPF4C_AUTHORIZE_ACTIVATE_ENCRYPT="N"
          DPF4C_AUTHORIZE_USER_PW=""
          DPF4C_AUTHORIZE_OWNER_PW=""
          DPF4C_AUTHORIZE_PRINT="N"
          DPF4C_AUTHORIZE_CHANGE="N"
          DPF4C_AUTHORIZE_COPY_TEXT="Y"
          DPF4C_AUTHORIZE_ADDNOTES="Y"
          DPF4C_AUTHORIZE_FILLFIELDS="N"
          DPF4C_AUTHORIZE_BRAILLE="Y"
          DPF4C_AUTHORIZE_ASSEMBLE="Y"
          DPF4C_AUTHORIZE_LOWRES="N"
          DPF4C_AUTHORIZE_IGNORE_SIGNATURE="N"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          DPF4C_AUTHORIZE_USER_PW=""
          DPF4C_AUTHORIZE_ACTIVATE_ENCRYPT="Y|N"
          DPF4C_AUTHORIZE_OWNER_PW=""
          DPF4C_AUTHORIZE_PRINT="Y|N"
          DPF4C_AUTHORIZE_CHANGE="Y|N"
          DPF4C_AUTHORIZE_COPY_TEXT="Y|N"
          DPF4C_AUTHORIZE_ADDNOTES="Y|N"
          DPF4C_AUTHORIZE_FILLFIELDS="Y|N"
          DPF4C_AUTHORIZE_BRAILLE="Y|N"
          DPF4C_AUTHORIZE_ASSEMBLE="Y|N"
          DPF4C_AUTHORIZE_LOWRES="Y|N"
          DPF4C_AUTHORIZE_IGNORE_SIGNATURE="Y|N"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
      </pdfencrypt>
    </workingunits>
  </config>
</DPF>
