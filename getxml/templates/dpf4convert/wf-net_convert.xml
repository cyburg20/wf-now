<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.net_convert
        COMMENT="dpf4convert - PROCESS NET CONVERT.
This process is used to convert Office- and Native formats on the local net."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="NET Convert"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-net_convert.xml,v 1.2 2016/03/18 10:18:06 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.exit_process1
          COMMENT=""
          ERROR="END"
          NAME="exit_process"
          NEXT="END"
          POSX="200"
          POSY="1900"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.exit_process1>
        <DPF4CONVERT.net_get_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_wu_targetdir"
          POSX="200"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.net_get_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_get_data1>
        <DPF4CONVERT.net_readcustomconfig
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.net_readcustomconfig"
          NEXT="DPF4CONVERT.net_get_data1"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_readcustomconfig>
        <DPF4CONVERT.pdfpostproc
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="jobprogress_83"
          POSX="600"
          POSY="2800"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfpostproc"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfpostproc>
        <DPF4CONVERT.pdftarget
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="jobprogress_66"
          POSX="600"
          POSY="2200"
          TYPE="SWF"
          USE="DPF4CONVERT.pdftarget"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdftarget>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="1600"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_activate_pdfadapt
          COMMENT=""
          ELSE="DPF4CONVERT.pdfpostproc"
          OPERATOR="contains"
          POSX="600"
          POSY="2600"
          PROPERTY="DESTINATION_FORMAT_QUALITY"
          THEN="enable_pdfadapt"
          TYPE="IF-THEN-ELSE"
          VALUE="pdfa"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_activate_pdfadapt>
        <check_application_type
          COMMENT=""
          ERROR="END"
          NEXT="switch_for_input_type"
          POSX="200"
          POSY="1100"
          TYPE="SWF"
          USE="DPF4CONVERT.check_application_type"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </check_application_type>
        <check_custom_config
          COMMENT=""
          ELSE="set_default_custom_config"
          OPERATOR="exists"
          POSX="400"
          POSY="100"
          PROPERTY="DPF4C_CUSTOM_CONFIG"
          THEN="jobprogress_0"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_custom_config>
        <check_extracted
          COMMENT=""
          ELSE="unset_wu_targetdir"
          OPERATOR="eq"
          POSX="600"
          POSY="1300"
          PROPERTY="Z2P_RESULT"
          THEN="check_application_type"
          TYPE="IF-THEN-ELSE"
          VALUE="extract"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="HORIZONTAL"
            />
        </check_extracted>
        <convert_native
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="check_extracted"
          POSX="400"
          POSY="1300"
          TYPE="SWF"
          USE="DPF4CONVERT.convert_native"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </convert_native>
        <convert_office
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="unset_wu_targetdir"
          POSX="400"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4CONVERT.convert_office"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </convert_office>
        <enable_pdfadapt
          COMMENT=""
          NEXT="DPF4CONVERT.pdfpostproc"
          POSX="800"
          POSY="2700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFADAPT="Y"
            />
        </enable_pdfadapt>
        <jobprogress_0
          COMMENT=""
          NEXT="DPF4CONVERT.net_readcustomconfig"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_PLM_SYSTEM_NAME="no_plm_system"
            DPFJOBPROGRESS="0"
            />
        </jobprogress_0>
        <jobprogress_100
          COMMENT=""
          NEXT="END"
          POSX="600"
          POSY="3400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="100"
            />
        </jobprogress_100>
        <jobprogress_50
          COMMENT=""
          NEXT="DPF4CONVERT.pdftarget"
          POSX="600"
          POSY="2000"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="50"
            />
        </jobprogress_50>
        <jobprogress_66
          COMMENT=""
          NEXT="check_activate_pdfadapt"
          POSX="600"
          POSY="2400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="66"
            />
        </jobprogress_66>
        <jobprogress_83
          COMMENT=""
          NEXT="net_put_data"
          POSX="600"
          POSY="3000"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="83"
            />
        </jobprogress_83>
        <net_put_data
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="jobprogress_100"
          POSX="600"
          POSY="3200"
          TYPE="SWF"
          USE="DPF4CONVERT.net_put_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </net_put_data>
        <pdfprocess
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="jobprogress_50"
          POSX="600"
          POSY="1800"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprocess"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdfprocess>
        <set_default_custom_config
          COMMENT=""
          NEXT="jobprogress_0"
          POSX="400"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_CUSTOM_CONFIG="dpf4convert::net_convert,dpf4convert::convert_office,dpf4convert::convert_native,dpf4convert::error_handling"
            />
        </set_default_custom_config>
        <set_wu_targetdir
          COMMENT=""
          NEXT="check_application_type"
          POSX="200"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR="%JOBDIR%"
            DPFJOBPROGRESS="17"
            />
        </set_wu_targetdir>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1100"
          POSY="3400"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <switch_for_input_type
          CASE:native="convert_native"
          CASE:office="convert_office"
          COMMENT=""
          CONNECTION:DEFAULT="DIRECT"
          CONNECTION:native="DIRECT"
          CONNECTION:office="DIRECT"
          DEFAULT="unknown_application"
          POSX="200"
          POSY="1300"
          PROPERTY="DPF4C_APPLICATION_TYPE"
          TYPE="SWITCH"
          />
        <unknown_application
          COMMENT=""
          NEXT="DPF4CONVERT.exit_process1"
          POSX="200"
          POSY="1700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ERR_REASON="The application type &lt;%DPF4C_APPLICATION_TYPE%&gt; is not supported."
            ERR_WFNAME="net_convert"
            ERR_WFPOSITION="err_no_application_for_conversion"
            />
        </unknown_application>
        <unset_wu_targetdir
          COMMENT=""
          NEXT="pdfprocess"
          POSX="600"
          POSY="1600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR=""
            DPFJOBPROGRESS="33"
            />
        </unset_wu_targetdir>
        <version_dpf4c
          COMMENT=""
          ERROR="END"
          NEXT="check_custom_config"
          POSX="200"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c>
      </DPF4CONVERT.net_convert>
    </workflows>
  </config>
</DPF>
