<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.pdfprotect
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfprotect.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Pdfprotect - Set protections on a PDF file"
        LICENSE="CWPROTECT"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-pdfprotect.xml,v 1.5 2017/02/08 12:46:28 thomas Exp $"
        >
        <DEFAULTS
          PDFPROTECT_CHANGE="notallowed"
          PDFPROTECT_COPY="noprotection"
          PDFPROTECT_PRINT="noprotection"
          PDFPROTECT_SET_OPEN="N"
          PDFPROTECT_SET_RIGHTS="N"
          PDFPROTECT_PERMFLAGS_SET=""
          PDFPROTECT_PERMFLAGS_NOT_SET=""
          />
        <INPUT>
          <DPF4C_AUTHORIZE_OWNER_PW
            TYPE="TEXT"
            />
          <DPF4C_AUTHORIZE_USER_PW
            TYPE="TEXT"
            />
          <PDFPROTECT_CHANGE
            DEFAULT="notallowed"
            TYPE="TEXT"
            />
          <PDFPROTECT_COPY
            DEFAULT="noprotection"
            TYPE="TEXT"
            />
          <PDFPROTECT_PERMFLAGS_SET
            TYPE="TEXT"
            />
          <PDFPROTECT_PERMFLAGS_NOT_SET
            TYPE="TEXT"
            />
          <PDFPROTECT_PRINT
            DEFAULT="noprotection"
            TYPE="TEXT"
            />
          <PDFPROTECT_SET_OPEN
            DEFAULT="N"
            TYPE="TEXT"
            />
          <PDFPROTECT_SET_RIGHTS
            DEFAULT="N"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT/>
      </DPF4CONVERT.pdfprotect>
    </workingunits>
  </config>
</DPF>
