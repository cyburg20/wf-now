<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.sap_convert_ini_work
        COMMENT="dpf4convert - SUBPROCESS WEB CONVERT INI WORK
Used by WEB CONVERT INI. Mainly to set the defaults for the user exits."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="WEB Convert INI work"
        OrigIdentifier="WorkflowDPF4CONVERT.sap_convert_ini_work"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-sap_convert_ini_work.xml,v 1.4 2017/01/26 08:20:24 kig Exp $"
        >
        <DEFAULTS
          DPF4CONVERT.ConvIniUserPreCheckout="DPF4CONVERT.ConvIniUserPreCheckout"
          DPF4CONVERT.ConvIniUserPreConvert="DPF4CONVERT.ConvIniUserPreConvert"
          DPF4CONVERT.ConvIniUserPreSendback="DPF4CONVERT.ConvIniUserPreSendback"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="user_pre_checkout"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <conv_ini_checkout
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.conv_ini_checkout"
          NEXT="user_pre_convert"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </conv_ini_checkout>
        <conv_ini_convert
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.conv_ini_convert"
          NEXT="set_progress_for_sendback"
          POSX="200"
          POSY="1100"
          TYPE="WU"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <DEFAULTS
            SubProcess="dpf4convert"
            />
          <MAPPING/>
        </conv_ini_convert>
        <sendback
          COMMENT=""
          ERROR="END"
          NAME="sendback"
          NEXT="END"
          POSX="200"
          POSY="1700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sendback>
        <set_progress_for_checkout
          COMMENT=""
          NEXT="conv_ini_checkout"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="25"
            />
        </set_progress_for_checkout>
        <set_progress_for_convert
          COMMENT=""
          NEXT="conv_ini_convert"
          POSX="200"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="50"
            />
        </set_progress_for_convert>
        <set_progress_for_sendback
          COMMENT=""
          NEXT="user_pre_sendback"
          POSX="200"
          POSY="1300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="75"
            />
        </set_progress_for_sendback>
        <user_pre_checkout
          COMMENT=""
          ERROR="END"
          NEXT="set_progress_for_checkout"
          POSX="200"
          POSY="100"
          TYPE="USER_EXIT"
          USE="%DPF4CONVERT.ConvIniUserPreCheckout%"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </user_pre_checkout>
        <user_pre_convert
          COMMENT=""
          ERROR="END"
          NEXT="set_progress_for_convert"
          POSX="200"
          POSY="700"
          TYPE="USER_EXIT"
          USE="%DPF4CONVERT.ConvIniUserPreConvert%"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </user_pre_convert>
        <user_pre_sendback
          COMMENT=""
          ERROR="END"
          NEXT="sendback"
          POSX="200"
          POSY="1500"
          TYPE="USER_EXIT"
          USE="%DPF4CONVERT.ConvIniUserPreSendback%"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </user_pre_sendback>
      </DPF4CONVERT.sap_convert_ini_work>
    </workflows>
  </config>
</DPF>
