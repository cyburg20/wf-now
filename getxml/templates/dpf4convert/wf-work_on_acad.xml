<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.work_on_acad
        COMMENT="dpf4convert - Summarize processing steps for AutoCAD


This process performs the steps autocad2file, hpgl_convert, PDF-process, create target formats and PDF-postproc on a PDF-file."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Work On Acad - Summarize processing an AutoCAD-file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-work_on_acad.xml,v 1.4 2016/07/22 14:27:50 kig Exp $"
        >
        <CheckFilesets
          COMMENT=""
          ELSE="END"
          OPERATOR="ge"
          POSX="200"
          POSY="1200"
          PROPERTY="DPF4C_NUM_FILESETS"
          THEN="DPF4CONVERT.work_on_filesets1"
          TYPE="IF-THEN-ELSE"
          VALUE="1"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </CheckFilesets>
        <DEFAULTS/>
        <DPF4CONVERT.dpf4c_user_pre_acadconv1
          COMMENT=""
          ERROR="END"
          NEXT="set_wu_targetdir"
          POSX="400"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.dpf4c_user_pre_acadconv"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.dpf4c_user_pre_acadconv1>
        <DPF4CONVERT.work_on_filesets1
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="400"
          POSY="1300"
          TYPE="SWF"
          USE="DPF4CONVERT.work_on_filesets"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.work_on_filesets1>
        <END
          COMMENT=""
          NAME="END"
          POSX="300"
          POSY="1200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="SetCurrfilesSupported"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <SetCurrfilesSupported
          COMMENT=""
          NEXT="check_user_pre_acadconv"
          POSX="200"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            CURRFILES_SUPPORTED="Y"
            />
        </SetCurrfilesSupported>
        <check_user_pre_acadconv
          COMMENT=""
          ELSE="set_wu_targetdir"
          OPERATOR="eq"
          POSX="200"
          POSY="400"
          PROPERTY="DPF4C_SWITCH_USER_PRE_ACADCONV"
          THEN="DPF4CONVERT.dpf4c_user_pre_acadconv1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_user_pre_acadconv>
        <convert_autocad2file
          COMMENT=""
          ERROR="END"
          NEXT="unset_wu_targetdir"
          POSX="200"
          POSY="800"
          TYPE="SWF"
          USE="autocad2file"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </convert_autocad2file>
        <set_wu_targetdir
          COMMENT=""
          NEXT="convert_autocad2file"
          POSX="200"
          POSY="600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR="%JOBDIR%"
            />
        </set_wu_targetdir>
        <unset_wu_targetdir
          COMMENT=""
          NEXT="CheckFilesets"
          POSX="200"
          POSY="1000"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR=""
            />
        </unset_wu_targetdir>
      </DPF4CONVERT.work_on_acad>
    </workflows>
  </config>
</DPF>
