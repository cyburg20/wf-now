<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.SapFixFontbox
        COMMENT="dpf4convert - Process for fixing the fontbox of PDFs."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="SapFixFontbox"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-sapfixfontbox.xml,v 1.2 2017/01/26 08:44:53 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="1700"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="sp-version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <checkout_files_local_copy1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="checkout_files_local_copy"
          NEXT="fixfontbox1"
          POSX="300"
          POSY="900"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </checkout_files_local_copy1>
        <fixfontbox1
          COMMENT=""
          ERROR="END"
          NAME="fixfontbox"
          NEXT="plm_checkin_files_sap1"
          POSX="300"
          POSY="1100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <DPF4C_MODIFY_COMPRESSION
              val="Y"
              />
            <DPF4C_PDFCOMPRESS_EXT
              val=""
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </fixfontbox1>
        <plm_checkin_files_sap1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="plm_checkin_files_sap"
          NEXT="END"
          POSX="300"
          POSY="1300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </plm_checkin_files_sap1>
        <plm_data_mapping1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="plm_data_mapping"
          NEXT="plm_data_mapping_sap1"
          POSX="300"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </plm_data_mapping1>
        <plm_data_mapping_sap1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="plm_data_mapping_sap"
          NEXT="checkout_files_local_copy1"
          POSX="300"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </plm_data_mapping_sap1>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="600"
          POSY="1500"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <sp-read_ini_file
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="dpf4c_read_ini_file"
          NEXT="plm_data_mapping1"
          POSX="300"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-read_ini_file>
        <sp-version_dpf4c
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="sp-read_ini_file"
          POSX="300"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-version_dpf4c>
      </DPF4CONVERT.SapFixFontbox>
