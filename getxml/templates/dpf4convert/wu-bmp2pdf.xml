<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.bmp2pdf
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/bmp2pdf.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="dpf4c - Convert BMP to PDF"
        LICENSE="CIBMP"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-bmp2pdf.xml,v 1.4 2017/02/08 12:46:27 thomas Exp $"
        >
        <DEFAULTS
          DPF4C_BMP2PDF_RESOLUTION_DPI="100"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="bmp"
          DPF4C_BMP2PDF_RESOLUTION_DPI="100"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
      </DPF4CONVERT.bmp2pdf>
    </workingunits>
  </config>
</DPF>
