<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <pdfstamp
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfstamp.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        LOGLEVEL="LOG_INFO"
        LOGNAME="dpf4c-pdfstamp"
        LOGSTDOUT="Y"
        NAME="dpf4c - Stamp pdf"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="5"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-pdfstamp.xml,v 1.13 2012/02/02 14:49:40 dei Exp $"
        >
        <DEFAULTS
          DPF4C_PDFSTAMP_DB_FILE="default.db"
          DPF4C_PDFSTAMP_STP_FILE="default.stp"
          DPF4C_PDFSTAMP_TXT_FILE="%PLSROOT%tools/convcfg/pdfstamp/default.txt"
          DPF4C_STAMP_MODE="normal"
          USE_DEFAULT_TXT_FILE="N"
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          JOBID="*"
          LOGLEVEL=""
          PARFILENAME="*"
          >
          <DPF4C_MODIFY_STAMP_N_TEXT0
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT1
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT2
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT3
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT4
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT5
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT6
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT7
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT8
            TYPE="TEXT"
            />
          <DPF4C_MODIFY_STAMP_N_TEXT9
            TYPE="TEXT"
            />
          <DPF4C_PDFSTAMP_DB_FILE
            DEFAULT="default.db"
            TYPE="TEXT"
            />
          <DPF4C_PDFSTAMP_EXT
            TYPE="TEXT"
            />
          <DPF4C_PDFSTAMP_STP_FILE
            DEFAULT="default.stp"
            TYPE="TEXT"
            />
          <DPF4C_PDFSTAMP_TXT_FILE
            DEFAULT="%PLSROOT%tools/convcfg/pdfstamp/default.txt"
            TYPE="TEXT"
            />
          <DPF4C_STAMP_DB_FILE
            TYPE="TEXT"
            />
          <DPF4C_STAMP_MARK
            TYPE="TEXT"
            />
          <DPF4C_STAMP_MODE
            DEFAULT="normal"
            TYPE="TEXT"
            />
          <DPF4C_STAMP_STP_FILE
            TYPE="TEXT"
            />
          <USE_DEFAULT_TXT_FILE
            DEFAULT="N"
            TYPE="TEXT"
            />
          <WU_ENABLE_DEBUG_LOOP
            DEFAULT="N"
            TYPE="TEXT"
            />
        </INPUT>
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
      </pdfstamp>
    </workingunits>
  </config>
</DPF>
