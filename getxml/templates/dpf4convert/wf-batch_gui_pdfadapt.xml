<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <batch_gui_pdfadapt
        COMMENT="dpf4convert - Pdfadapt process for separate pdfgui with sendback of files."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - batch_gui_pdfadapt"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-batch_gui_pdfadapt.xml,v 1.4 2012/03/21 16:11:38 dh Exp $"
        >
        <CheckCallasSdkDefault
          COMMENT=""
          ELSE="SetDefaultUseCallasSdk"
          OPERATOR="exists"
          POSX="400"
          POSY="700"
          PROPERTY="PDFADAPT_USE_CALLASSDK"
          THEN="DPF4CONVERT.pdfadapt1"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </CheckCallasSdkDefault>
        <DEFAULTS/>
        <DPF4CONVERT.pdfadapt1
          COMMENT=""
          ERROR="check_for_disable_checkin"
          NEXT="check_for_disable_checkin"
          POSX="400"
          POSY="900"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfadapt"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfadapt1>
        <DPF4CONVERT.readcustomconfig1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.readcustomconfig"
          NEXT="CheckCallasSdkDefault"
          POSX="400"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.readcustomconfig1>
        <END
          COMMENT=""
          NAME="END"
          POSX="100"
          POSY="1700"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="sp-version_dpf4c"
          ERROR="checkin"
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="checkin"
          POSX="600"
          POSY="1500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_OUTFILEprot.xml="%PDFADAPT_PROTOCOL%"
            />
        </MODIFY1>
        <SetDefaultUseCallasSdk
          COMMENT=""
          NEXT="DPF4CONVERT.pdfadapt1"
          POSX="600"
          POSY="700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="HORIZONTAL"
            />
          <SET
            PDFADAPT_USE_CALLASSDK="Y"
            />
        </SetDefaultUseCallasSdk>
        <check_for_disable_checkin
          COMMENT=""
          ELSE="check_sendback_pdf_file"
          OPERATOR="eq"
          POSX="400"
          POSY="1100"
          PROPERTY="BATCH_GUI_PDFADAPT_DISABLE_CHECKIN"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_disable_checkin>
        <check_sendback_pdf_file
          COMMENT=""
          ELSE="MODIFY1"
          OPERATOR="eq"
          POSX="400"
          POSY="1300"
          PROPERTY="PDFADAPT_FILE_MODIFIED"
          THEN="set_process_properties_checkin"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="DIRECT"
            />
        </check_sendback_pdf_file>
        <checkin
          COMMENT=""
          ERROR="END"
          NAME="checkin"
          NEXT="sendback"
          POSX="800"
          POSY="1500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </checkin>
        <sendback
          COMMENT=""
          ERROR="checkin"
          NAME="sendback"
          NEXT="END"
          POSX="1000"
          POSY="1500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sendback>
        <set_process_properties
          COMMENT=""
          NEXT="DPF4CONVERT.readcustomconfig1"
          POSX="400"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_DEST_FORMAT="prot.xml"
            DPF4C_CUSTOM_CONFIG="dpf4convert::batch_gui_pdfadapt"
            ENABLE_PDF_ADAPT="Y"
            PDFADAPT_ERROR_IF_REFUSED="N"
            VERSION="1.0.0"
            VERSION_PRODUCER="PDF Longlife Suite - DPF Process"
            />
        </set_process_properties>
        <set_process_properties_checkin
          COMMENT=""
          NEXT="MODIFY1"
          POSX="400"
          POSY="1500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            SWITCH_CHECKIN_CURRFILENAME="N"
            />
        </set_process_properties_checkin>
        <sp-version_dpf4c
          COMMENT=""
          ERROR="checkin"
          NEXT="set_process_properties"
          POSX="400"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-version_dpf4c>
      </batch_gui_pdfadapt>
    </workflows>
  </config>
</DPF>
