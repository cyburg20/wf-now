<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.pdfsetviewpref
        CODEPAGE="ISO-8859-1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfsetviewpref.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="Pdfsetviewpref - Set view preferences in a PDF file"
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-pdfsetviewpref.xml,v 1.4 2012/06/20 07:40:07 dei Exp $"
        >
        <DEFAULTS
          PDFVIEW_OPEN_ACTION=""
          PDFVIEW_PAGE_MODE=""
          PDFVIEW_PREFERENCES=""
          PDFVIEW_FAST_WEBVIEW=""
          />
        <INPUT
          PDFVIEW_OPEN_ACTION="string"
          PDFVIEW_PAGE_MODE="string"
          PDFVIEW_PREFERENCES="string"
          PDFVIEW_FAST_WEBVIEW="string"
          />
        <OUTPUT/>
      </DPF4CONVERT.pdfsetviewpref>
    </workingunits>
  </config>
</DPF>
