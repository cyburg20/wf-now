<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.net_convert_3dpdf
        COMMENT="dpf4convert - PROCESS NET CONVERT TO 3D-PDF.
This process is used to convert a 3D-format to 3D-PDF on the local net."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="NET Convert To 3D-PDF"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-net_convert_3dpdf.xml,v 1.3 2016/11/29 13:20:35 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.convert_3dpdf
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="unset_wu_targetdir"
          POSX="200"
          POSY="1100"
          TYPE="SWF"
          USE="DPF4CONVERT.convert_3dpdf"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.convert_3dpdf>
        <DPF4CONVERT.net_get_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_wu_targetdir"
          POSX="200"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.net_get_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_get_data1>
        <DPF4CONVERT.net_put_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_progress_100"
          POSX="200"
          POSY="1900"
          TYPE="SWF"
          USE="DPF4CONVERT.net_put_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_put_data1>
        <DPF4CONVERT.net_readcustomconfig1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.net_readcustomconfig"
          NEXT="DPF4CONVERT.net_get_data1"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_readcustomconfig1>
        <DPF4CONVERT.pdfpostproc1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_progress_75"
          POSX="400"
          POSY="1700"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfpostproc"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfpostproc1>
        <DPF4CONVERT.pdfprocess1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="DPF4CONVERT.pdfpostproc1"
          POSX="400"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprocess"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfprocess1>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1800"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_currfiletype
          COMMENT=""
          ELSE="set_progress_75"
          OPERATOR="eq"
          POSX="200"
          POSY="1500"
          PROPERTY="CURRFILETYPE"
          THEN="DPF4CONVERT.pdfprocess1"
          TYPE="IF-THEN-ELSE"
          VALUE="pdf"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_currfiletype>
        <set_config
          COMMENT=""
          NEXT="DPF4CONVERT.net_readcustomconfig1"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_PLM_SYSTEM_NAME="no_plm_system"
            DPF4C_CUSTOM_CONFIG="dpf4convert::net_convert,dpf4convert::convert_3dpdf,dpf4convert::error_handling"
            DPFJOBPROGRESS="0"
            />
        </set_config>
        <set_progress_100
          COMMENT=""
          NEXT="END"
          POSX="200"
          POSY="2100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="100"
            />
        </set_progress_100>
        <set_progress_75
          COMMENT=""
          NEXT="DPF4CONVERT.net_put_data1"
          POSX="200"
          POSY="1700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="75"
            />
        </set_progress_75>
        <set_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.convert_3dpdf"
          POSX="200"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR="%JOBDIR%"
            DPFJOBPROGRESS="25"
            />
        </set_wu_targetdir>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="600"
          POSY="2100"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <unset_wu_targetdir
          COMMENT=""
          NEXT="check_currfiletype"
          POSX="200"
          POSY="1300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR=""
            DPFJOBPROGRESS="50"
            />
        </unset_wu_targetdir>
        <version_dpf4c
          COMMENT=""
          ERROR="END"
          NEXT="set_config"
          POSX="200"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c>
      </DPF4CONVERT.net_convert_3dpdf>
    </workflows>
  </config>
</DPF>
