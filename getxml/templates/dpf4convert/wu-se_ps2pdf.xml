<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <DPF4CONVERT.se_ps2pdf
        NAME="dpf4c - Combine PostScript from SolidEdge to PDF"
        VERSION="$Id: wu-se_ps2pdf.xml,v 1.1 2008/06/02 12:15:40 kig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-se_ps2pdf"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        DPFINT-Type="StationDescription"
        THREADS="1"
        IGNOREEXITCODE="N"
        COMMAND="%DPFSRV%/scripts/dpf4convert/se_ps2pdf.pl -parfile %PARFILENAME%"
        >
        <INPUT
          CAD_SE_PS2PDF_PS1="file"
          CAD_SE_PS2PDF_PAGE_ROTATION_FILE="file"
          CAD_SE_PS2PDF_TARGETFILE="file"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
        </DPF4CONVERT.se_ps2pdf>
    </workingunits>
  </config>
</DPF>
