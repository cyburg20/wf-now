<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <autocad2file
        NAME="dpf4c - Convert CAD AutoCAD dwg and dxf to hpgl or dwg. If configured do property update."
        VERSION="$Id: wu-autocad2file.xml,v 1.10 2010/01/21 11:19:29 kig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-autocad2file"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/autocad2file.pl -parfile %PARFILENAME%">
        <DEFAULTS
          AUTOCAD2FILE_ACTIVATE_MULTI_LAYOUT_PLOT="Y"
          AUTOCAD2FILE_FILE_AUTOPLOT_INI="%PLSROOT%server/cad/autoplot/autoplot2010.ini"
          AUTOCAD2FILE_CHECK_EXT_REF="N"
          AUTOCAD2FILE_CHECK_WORKDIRECTOR_RUNNING="N"
          AUTOCAD2FILE_COMMON_DEST_COLOR=""
          AUTOCAD2FILE_CLEAN_TEMP_FILE="Y"
          AUTOCAD2FILE_DEBUG_LEVEL="1"
          AUTOCAD2FILE_DEST_OUTPUT_FORMAT=""
          AUTOCAD2FILE_ENABLE_PROPERTY_LOG="N"
          AUTOCAD2FILE_GENERAL_MULTISHEET="Y"
          AUTOCAD2FILE_GET_CONVERTER_LOG_FILE="N"
          AUTOCAD2FILE_LLC_CONFIG_FILE="%PLSROOT%server/cad/autoplot/layer_control/autocad_llc_mapping.xml"
          AUTOCAD2FILE_LLC_MAPPING_ID=""
          AUTOCAD2FILE_NUMBER_OF_SHEETS="0"
          AUTOCAD2FILE_PLOTSTYLE="USELAYOUTPLOTSTYLE"
          AUTOCAD2FILE_PRINTER="SEAL_BATCH.PC3"
          AUTOCAD2FILE_PROPERTY_UPDATE_PROVIDE_FILE_SAVE_DRAWING_VALUE="2"
          AUTOCAD2FILE_RESTART_WORKDIRECTOR_IF_ERROR="N"
          AUTOCAD2FILE_SAVE_FORMAT=""
          AUTOCAD2FILE_STANDARD_DWG_FILE="%PLSROOT%tools/testfiles/dwg/autocad_dina0_l.dwg"
          AUTOCAD2FILE_STOP_ON_ERROR="N"
          AUTOCAD2FILE_STEP_OVER=""
          DESTINATION_FORMAT_QUALITY=""
          DPF4C_COMMON_DEST_FORMAT=""
          DPF4C_COMMON_TIMEOUT="10"
          DPF4C_MODIFY_ROTATION_HPGL="180"
          DPF4C_PROPERTYUPDATE_ENABLE="N"
          DPF4C_PROPERTYUPDATE_FILE=""
          DPF4C_PROPERTYUPDATE_MAPPINGFILE_DWG="%PLSROOT%server/dpf/conf/dpf4convert/AutoCadToFile_PropertyMappingFile.txt"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="N"
          LAYER_CONTROL_NAMES_FOR_ORDERS=""
          WD_ERROR_CODE_SEVERITY="000=I;001=W;002=E;003=E;004=E;005=E;006=E;007=E;008=W;009=E;010=W;011=W;020=E;050=E;051=E;052=E;053=W;054=E;060=E;061=E;065=E;090=I;091=I;092=I;093=I;094=I;095=I;100=I;101=W;102=W;103=I;104=E;110=E;120=W;121=E;130=E;131=E;132=E;133=E;134=E;135=E;140=E;142=E;143=E;144=W;145=I;146=I;147=I;150=E"
          />
        <INPUT
          AUTOCAD2FILE_ACTIVATE_MULTI_LAYOUT_PLOT="Y|N"
          AUTOCAD2FILE_FILE_AUTOPLOT_INI="file"
          AUTOCAD2FILE_CHECK_EXT_REF="Y|N"
          AUTOCAD2FILE_CHECK_WORKDIRECTOR_RUNNING="N|Y"
          AUTOCAD2FILE_COMMON_DEST_COLOR="|black"
          AUTOCAD2FILE_CLEAN_TEMP_FILE="Y|N"
          AUTOCAD2FILE_DEBUG_LEVEL="1|2|3"
          AUTOCAD2FILE_DEST_OUTPUT_FORMAT="string"
          AUTOCAD2FILE_ENABLE_PROPERTY_LOG="Y|N"
          AUTOCAD2FILE_GENERAL_MULTISHEET="Y|N"
          AUTOCAD2FILE_GET_CONVERTER_LOG_FILE="Y|!N!"
          AUTOCAD2FILE_LLC_CONFIG_FILE="file"
          AUTOCAD2FILE_LLC_MAPPING_ID="string"
          AUTOCAD2FILE_NUMBER_OF_SHEETS="number"
          AUTOCAD2FILE_PLOTSTYLE="string"
          AUTOCAD2FILE_PRINTER="string"
          AUTOCAD2FILE_PROPERTY_UPDATE_PROVIDE_FILE_SAVE_DRAWING_VALUE="0|1|2|3|4"
          AUTOCAD2FILE_RESTART_WORKDIRECTOR_IF_ERROR="N|Y"
          AUTOCAD2FILE_SAVE_FORMAT="|2000|2004|2007|2010"
          AUTOCAD2FILE_STANDARD_DWG_FILE="file"
          AUTOCAD2FILE_STOP_ON_ERROR="Y|N"
          AUTOCAD2FILE_STEP_OVER="string"
          CURRFILENAME="file"
          DESTINATION_FORMAT_DestFormat_ADDITIONAL_PARAMETER_ParamName="string"
          DESTINATION_FORMAT_QUALITY="|pdfa"
          DPF4C_COMMON_DEST_FORMAT="string"
          DPF4C_COMMON_TIMEOUT="number"
          DPF4C_MODIFY_ROTATION_HPGL="number"
          DPF4C_PROPERTYUPDATE_ENABLE="Y|N"
          DPF4C_PROPERTYUPDATE_FILE="file"
          DPF4C_PROPERTYUPDATE_MAPPINGFILE_DWG="file"
          DPF4C_PROPERTYUPDATE_PROVIDE_FILES="Y|N"
          LAYER_CONTROL_NAMES_FOR_ORDERS="string"
          WD_ERROR_CODE_SEVERITY="string"
          />
    	<OUTPUT
          CURRFILENAME="file"
          CURRFILETYPE="pdf|tif|dwg|dxf|*"
          DPF_JOB_ICON="|warn"
          DPF_JOB_MESSAGE="|string"
          DPF4C_COMMON_DEST_COLOR="|black"
          DPF4C_COMMON_OUTFILEn="file"
          DPF4C_PROPERTYUPDATE_OUTFILE1="file"
          DPF4C_SWITCH_CHECKIN_ALL_DESTFORMATS="|N"
          DPF4C_SWITCH_CREATE_DESTFORMATS="|N"
          DPF4C_SWITCH_MODIFY="|N"
          DPF4C_COMMON_MULTIPAGE="|Y"
          ERR_REASON="string"
          />
      </autocad2file>
    </workingunits>
  </config>
</DPF>
