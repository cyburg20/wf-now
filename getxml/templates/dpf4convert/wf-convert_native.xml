<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.convert_native
        COMMENT="dpf4convert - SUBPROCESS CONVERT NATIVE.
This subprocess is used to convert native file formats. It is used from the main processes like SAP_CONVERT_NATIVE and NET_CONVERT_NATIVE inbetween the checkin and checkout subprocesses."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Convert Native - SubProcess Convert Native Formats"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-convert_native.xml,v 1.20 2017/06/19 12:43:01 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.ascii2pdf
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.ascii2pdf"
          NEXT="END"
          POSX="1500"
          POSY="1400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.ascii2pdf>
        <DPF4CONVERT.bmp2pdf2
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.bmp2pdf"
          NEXT="END"
          POSX="1100"
          POSY="1000"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.bmp2pdf2>
        <DPF4CONVERT.cals2pdf1
          COMMENT=""
          ERROR="END"
          NAME="cals2pdf"
          NEXT="END"
          POSX="1500"
          POSY="1000"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.cals2pdf1>
        <DPF4CONVERT.dpf4c_user_pre_nativeconv1
          COMMENT=""
          ERROR="END"
          NEXT="check_enable"
          POSX="200"
          POSY="200"
          TYPE="SWF"
          USE="DPF4CONVERT.dpf4c_user_pre_nativeconv"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.dpf4c_user_pre_nativeconv1>
        <DPF4CONVERT.netconv_check_abort1
          COMMENT=""
          ERROR="END"
          NEXT="reset_z2p_result"
          POSX="300"
          POSY="300"
          TYPE="SWF"
          USE="DPF4CONVERT.netconv_check_abort"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.netconv_check_abort1>
        <DPF4CONVERT.pcx2pdf1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.pcx2pdf"
          NEXT="END"
          POSX="1300"
          POSY="1000"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pcx2pdf1>
        <DPF4CONVERT.tiffpreproc1
          COMMENT=""
          ERROR="END"
          NEXT="tiff2pdf2"
          POSX="900"
          POSY="1000"
          TYPE="SWF"
          USE="DPF4CONVERT.tiffpreproc"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.tiffpreproc1>
        <DPF4CONVERT.txt2pdf
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.txt2pdf"
          NEXT="END"
          POSX="1300"
          POSY="1400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.txt2pdf>
        <DPF4CONVERT.zip2pdf
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1700"
          POSY="1000"
          TYPE="SWF"
          USE="DPF4CONVERT.zip2pdf"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.zip2pdf>
        <END
          COMMENT=""
          NAME="END"
          POSX="1800"
          POSY="1600"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_user_pre_nativeconv"
          NAME=""
          />
        <MAPPING/>
        <cgm2pdfgxc1
          COMMENT=""
          ERROR="END"
          NAME="cgm2pdfgxc"
          NEXT="END"
          POSX="600"
          POSY="1200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </cgm2pdfgxc1>
        <check_application
          COMMENT=""
          ELSE="DPF4CONVERT.txt2pdf"
          OPERATOR="eq"
          POSX="1400"
          POSY="1200"
          PROPERTY="APPLICATION_FOR_CONVERT"
          THEN="DPF4CONVERT.ascii2pdf"
          TYPE="IF-THEN-ELSE"
          VALUE="ascii2pdf"
          >
          <CONNECTIONS
            ELSE="VERTICAL"
            THEN="VERTICAL"
            />
        </check_application>
        <check_enable
          COMMENT=""
          ELSE="check_get_filetype"
          OPERATOR="eq"
          POSX="300"
          POSY="100"
          PROPERTY="ENABLE_NATIVECONV"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE="N"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_enable>
        <check_filetype
          CASE:bmp="DPF4CONVERT.bmp2pdf2"
          CASE:cgm="cgm2pdfgxc1"
          CASE:cls="DPF4CONVERT.cals2pdf1"
          CASE:dcx="DPF4CONVERT.pcx2pdf1"
          CASE:eps="ps2pdfg2"
          CASE:gif="gif2pdf1"
          CASE:hp="hpgl2pdf2"
          CASE:hpgl="hpgl2pdf2"
          CASE:htm="html2pdf1"
          CASE:html="html2pdf1"
          CASE:jpeg="jpg2pdf2"
          CASE:jpg="jpg2pdf2"
          CASE:mhtm="html2pdf1"
          CASE:mhtml="html2pdf1"
          CASE:otf="otf2pdf1"
          CASE:pcx="DPF4CONVERT.pcx2pdf1"
          CASE:pdf="set_outfile_pdf"
          CASE:png="png2pdf1"
          CASE:ps="ps2pdfg2"
          CASE:tif="DPF4CONVERT.tiffpreproc1"
          CASE:tiff="DPF4CONVERT.tiffpreproc1"
          CASE:txt="check_application"
          CASE:xps="gxps2pdf"
          CASE:zip="DPF4CONVERT.zip2pdf"
          COMMENT=""
          CONNECTION:DEFAULT="VERTICAL"
          CONNECTION:bmp="VERTICAL"
          CONNECTION:cgm="VERTICAL"
          CONNECTION:cls="VERTICAL"
          CONNECTION:dcx="VERTICAL"
          CONNECTION:eps="VERTICAL"
          CONNECTION:gif="VERTICAL"
          CONNECTION:hp="VERTICAL"
          CONNECTION:hpgl="VERTICAL"
          CONNECTION:htm="VERTICAL"
          CONNECTION:html="VERTICAL"
          CONNECTION:jpeg="VERTICAL"
          CONNECTION:jpg="VERTICAL"
          CONNECTION:mhtm="VERTICAL"
          CONNECTION:mhtml="VERTICAL"
          CONNECTION:otf="VERTICAL"
          CONNECTION:pcx="VERTICAL"
          CONNECTION:pdf="DIRECT"
          CONNECTION:png="VERTICAL"
          CONNECTION:ps="VERTICAL"
          CONNECTION:tif="VERTICAL"
          CONNECTION:tiff="VERTICAL"
          CONNECTION:txt="VERTICAL"
          CONNECTION:xps="VERTICAL"
          CONNECTION:zip="VERTICAL"
          DEFAULT="unsupported_currfiletype"
          POSX="300"
          POSY="700"
          PROPERTY="CURRFILETYPE"
          TYPE="SWITCH"
          />
        <check_get_filetype
          COMMENT=""
          ELSE="DPF4CONVERT.netconv_check_abort1"
          OPERATOR="contains"
          POSX="500"
          POSY="100"
          PROPERTY="DPF4C_SWITCH_GET_FILETYPE"
          THEN="getfiletype_dpf4c1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="HORIZONTAL"
            THEN="DIRECT"
            />
        </check_get_filetype>
        <check_user_pre_nativeconv
          COMMENT=""
          ELSE="check_enable"
          OPERATOR="eq"
          POSX="100"
          POSY="100"
          PROPERTY="DPF4C_SWITCH_USER_PRE_NATIVECONV"
          THEN="DPF4CONVERT.dpf4c_user_pre_nativeconv1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_user_pre_nativeconv>
        <exit_process1
          COMMENT=""
          ERROR="END"
          NAME="exit_process"
          NEXT="END"
          POSX="100"
          POSY="1200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </exit_process1>
        <getfiletype_dpf4c1
          COMMENT=""
          ERROR="END"
          NEXT="DPF4CONVERT.netconv_check_abort1"
          POSX="600"
          POSY="200"
          TYPE="SWF"
          USE="getfiletype_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="HORIZONTAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </getfiletype_dpf4c1>
        <gif2pdf1
          COMMENT=""
          ERROR="END"
          NAME="gif2pdf"
          NEXT="END"
          POSX="400"
          POSY="1200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </gif2pdf1>
        <gxps2pdf
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1600"
          POSY="1200"
          TYPE="SWF"
          USE="gxps2pdf"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </gxps2pdf>
        <hpgl2pdf2
          COMMENT=""
          ERROR="END"
          NAME="hpgl2pdf"
          NEXT="END"
          POSX="700"
          POSY="1000"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </hpgl2pdf2>
        <html2pdf1
          COMMENT=""
          ERROR="END"
          NAME="html2pdf"
          NEXT="END"
          POSX="1000"
          POSY="1200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </html2pdf1>
        <jpg2pdf2
          COMMENT=""
          ERROR="END"
          NAME="jpg2pdf"
          NEXT="END"
          POSX="500"
          POSY="1000"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </jpg2pdf2>
        <otf2pdf1
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1800"
          POSY="1200"
          TYPE="SWF"
          USE="otf2pdf"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </otf2pdf1>
        <png2pdf1
          COMMENT=""
          ERROR="END"
          NAME="png2pdf"
          NEXT="END"
          POSX="800"
          POSY="1200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </png2pdf1>
        <ps2pdfg2
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1200"
          POSY="1200"
          TYPE="SWF"
          USE="ps2pdfg"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ps2pdfg2>
        <reset_z2p_result
          COMMENT=""
          NEXT="check_filetype"
          POSX="300"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            PDFFILE_STATUS="converted"
            Z2P_RESULT="unk"
            />
        </reset_z2p_result>
        <set_outfile_pdf
          COMMENT=""
          NEXT="END"
          POSX="300"
          POSY="1400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_OUTFILEpdf="%CURRFILENAME%"
            PDFFILE_STATUS="input"
            />
        </set_outfile_pdf>
        <tiff2pdf2
          COMMENT=""
          ERROR="END"
          NAME="tiff2pdf"
          NEXT="END"
          POSX="900"
          POSY="1400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </tiff2pdf2>
        <unsupported_currfiletype
          COMMENT=""
          NEXT="exit_process1"
          POSX="100"
          POSY="1000"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ERR_REASON="Error: This CURRFILETYPE -%CURRFILETYPE%- is not supported!"
            ERR_WFNAME="convert_native"
            ERR_WFPOSITION="unsupported_currfiletype"
            />
        </unsupported_currfiletype>
      </DPF4CONVERT.convert_native>
    </workflows>
  </config>
</DPF>
