          PARFILENAME="*"
          CURRFILENAME="*"
          JOBID="*"
          LOGLEVEL=""
          CURRFILETYPE="gksm|otf"
          />
	<OUTPUT
          CURRHEDFILENAME="*"
          CURRMETFILENAME="*"
          CURRXMLFILENAME=""
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
      </gksm2pdf>
    </workingunits>
  </config>
</DPF>
