<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <slddrw2pdf
        COMMENT="dpf4convert - Convert process CAD SolidWorks DRW to PDF and export formats"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - CAD SolidWorks DRW to PDF and export formats"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-slddrw2pdf.xml,v 1.15 2012/01/30 14:29:49 dei Exp $"
        >
        <DEFAULTS
          DPF4C_SLDDRW2PS_ALWAYS_USE_PDF_EXPORT="Y"
          />
        <END
          COMMENT=""
          NAME="END"
          POSX="900"
          POSY="200"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="property_update_office1"
          OPERATOR="eq"
          POSX="100"
          POSY="200"
          PROPERTY="DPF4C_SLDDRW2PS_ALWAYS_USE_PDF_EXPORT"
          THEN="sowoexport1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <MAIN
          BEGIN="IF-THEN-ELSE1"
          ERROR="END"
          />
        <MAPPING/>
        <check_execute_pdfconvert
          COMMENT=""
          ELSE="END"
          OPERATOR="contains"
          POSX="700"
          POSY="200"
          PROPERTY="EXECUTE_PDFCONVERT_IN_SLDDRW2PDF_PROCESS"
          THEN="pdf_convert"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_execute_pdfconvert>
        <pdf_convert
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="900"
          POSY="200"
          TYPE="SWF"
          USE="pdf_convert2pdfa"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </pdf_convert>
        <property_update_office1
          COMMENT=""
          ERROR="END"
          NAME="property_update_office"
          NEXT="slddrw2ps"
          POSX="300"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <DPF4C_PROPERTYUPDATE_DELETE_EMPTY_VALUES
              val="N"
              />
            <DPF4C_PROPERTYUPDATE_ENABLE
              val="N"
              />
            <DPF4C_PROPERTYUPDATE_PROVIDE_FILES
              val="N"
              />
            <REGSVRTOOL
              val="regsvr32.exe"
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </property_update_office1>
        <slddrw2ps
          COMMENT=""
          ERROR="END"
          NAME="slddrw2ps"
          NEXT="check_execute_pdfconvert"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </slddrw2ps>
        <sowoexport1
          COMMENT=""
          ERROR="END"
          NAME="sowoexport"
          NEXT="check_execute_pdfconvert"
          POSX="400"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <DPF4C_COMMON_TIMEOUT
              val="20"
              />
            <DPF4C_PROPERTYUPDATE_ENABLE
              val="N"
              />
            <DPF4C_PROPERTYUPDATE_PROVIDE_FILES
              val="N"
              />
            <DPF4C_SLDDRW2PS_ALWAYS_USE_PDF_EXPORT
              val="Y"
              />
            <DPF4C_SLDDRW2PS_AUTOMISSINGCONFIG
              val="N"
              />
            <DPF4C_SLDDRW2PS_CONFIG_FILE
              val="sowo.cfg"
              />
            <DPF4C_SLDDRW2PS_EXT
              val=""
              />
            <DPF4C_SLDDRW2PS_NOCONV2QUITSOWO
              val="10"
              />
            <DPF4C_SLDDRW2PS_PRINTER
              val="SEAL4CAD_Solid_Works"
              />
            <DPF4C_SLDDRW2PS_PROG_ID
              val=""
              />
            <DPF4C_SLDDRW2PS_RAPIDDRAFT
              val="N"
              />
            <DPF4C_SLDDRW2PS_REBUILD
              val="Y"
              />
            <DPF4C_SLDDRW2PS_USE_CUSTOM_SIZE
              val="N"
              />
            <DPF4C_SLDDRW2PS_VISIBLE
              val="Y"
              />
            <TARGETFILE
              val=""
              />
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING/>
        </sowoexport1>
      </slddrw2pdf>
    </workflows>
  </config>
</DPF>
