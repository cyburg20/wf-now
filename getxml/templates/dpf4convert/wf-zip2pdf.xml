<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.zip2pdf
        COMMENT="dpf4convert - Convert process ZIP to PDF"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - ZIP to PDF"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-zip2pdf.xml,v 1.2 2017/01/26 08:56:02 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.pdfmerge
          COMMENT=""
          ERROR="END"
          NAME="pdfmerge"
          NEXT="END"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING
            DPF4C_CREATE_SIGNATURE_PAGE="N"
            />
        </DPF4CONVERT.pdfmerge>
        <DPF4CONVERT.zip2pdf
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.zip2pdf"
          NEXT="check_need_merge"
          POSX="100"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.zip2pdf>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="DPF4CONVERT.zip2pdf"
          NAME=""
          />
        <MAPPING/>
        <check_need_merge
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="300"
          POSY="100"
          PROPERTY="Z2P_RESULT"
          THEN="DPF4CONVERT.pdfmerge"
          TYPE="IF-THEN-ELSE"
          VALUE="merge"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_need_merge>
      </DPF4CONVERT.zip2pdf>
    </workflows>
  </config>
</DPF>
