<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.net_convert_native
        COMMENT="dpf4convert - PROCESS NET CONVERT NATIVE.


This process is used to convert native formats on the local net."
        DPFINT-Customer="0"
        DPFINT-Interactive="1"
        DPFINT-Type="WorkflowDescription"
        NAME="NET Convert Native"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-net_convert_native.xml,v 1.3 2011/03/08 16:30:04 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.convert_native
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="unset_wu_targetdir"
          POSX="200"
          POSY="1100"
          TYPE="SWF"
          USE="DPF4CONVERT.convert_native"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.convert_native>
        <DPF4CONVERT.net_get_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="set_wu_targetdir"
          POSX="200"
          POSY="700"
          TYPE="SWF"
          USE="DPF4CONVERT.net_get_data"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_get_data1>
        <DPF4CONVERT.net_put_data1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY4"
          POSX="200"
          POSY="3100"
          TYPE="SWF"
          USE="DPF4CONVERT.net_put_data"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_put_data1>
        <DPF4CONVERT.net_readcustomconfig1
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NAME="DPF4CONVERT.net_readcustomconfig"
          NEXT="DPF4CONVERT.net_get_data1"
          POSX="200"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.net_readcustomconfig1>
        <DPF4CONVERT.pdfpostproc
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY3"
          POSX="200"
          POSY="2700"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfpostproc"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfpostproc>
        <DPF4CONVERT.pdfprocess
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY1"
          POSX="200"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprocess"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfprocess>
        <DPF4CONVERT.pdftarget
          COMMENT=""
          ERROR="sp-dpf4c_error"
          NEXT="MODIFY2"
          POSX="200"
          POSY="1900"
          TYPE="SWF"
          USE="DPF4CONVERT.pdftarget"
          >
          <CONNECTIONS
            ERROR="VERTICAL"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdftarget>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1800"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="DPF4CONVERT.pdftarget"
          POSX="200"
          POSY="1700"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="50"
            />
        </MODIFY1>
        <MODIFY2
          COMMENT=""
          NEXT="check_activate_pdfadapt"
          POSX="200"
          POSY="2100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="66"
            />
        </MODIFY2>
        <MODIFY3
          COMMENT=""
          NEXT="DPF4CONVERT.net_put_data1"
          POSX="200"
          POSY="2900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="83"
            />
        </MODIFY3>
        <MODIFY4
          COMMENT=""
          NEXT="END"
          POSX="200"
          POSY="3300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="100"
            />
        </MODIFY4>
        <check_activate_pdfadapt
          COMMENT=""
          ELSE="DPF4CONVERT.pdfpostproc"
          OPERATOR="contains"
          POSX="200"
          POSY="2300"
          PROPERTY="DESTINATION_FORMAT_QUALITY"
          THEN="enable_pdfadapt"
          TYPE="IF-THEN-ELSE"
          VALUE="pdfa"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_activate_pdfadapt>
        <enable_pdfadapt
          COMMENT=""
          NEXT="DPF4CONVERT.pdfpostproc"
          POSX="200"
          POSY="2500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFADAPT="Y"
            />
        </enable_pdfadapt>
        <set_config
          COMMENT=""
          NEXT="DPF4CONVERT.net_readcustomconfig1"
          POSX="200"
          POSY="300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_COMMON_PLM_SYSTEM_NAME="no_plm_system"
            DPF4C_CUSTOM_CONFIG="dpf4convert::net_convert,dpf4convert::convert_native,dpf4convert::error_handling"
            DPFJOBPROGRESS="0"
            />
        </set_config>
        <set_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.convert_native"
          POSX="200"
          POSY="900"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR="%JOBDIR%"
            DPFJOBPROGRESS="17"
            />
        </set_wu_targetdir>
        <sp-dpf4c_error
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="600"
          POSY="3100"
          TYPE="SWF"
          USE="dpf4c_error"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-dpf4c_error>
        <unset_wu_targetdir
          COMMENT=""
          NEXT="DPF4CONVERT.pdfprocess"
          POSX="200"
          POSY="1300"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_WU_TARGETDIR=""
            DPFJOBPROGRESS="33"
            />
        </unset_wu_targetdir>
        <version_dpf4c
          COMMENT=""
          ERROR="END"
          NEXT="set_config"
          POSX="200"
          POSY="100"
          TYPE="SWF"
          USE="version_dpf4c"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </version_dpf4c>
      </DPF4CONVERT.net_convert_native>
    </workflows>
  </config>
</DPF>
