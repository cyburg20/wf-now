<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <pub2ps
        COMMAND="%DPFSRV%/scripts/dpf4convert/pub2ps.pl -parfile %PARFILENAME%"
        DEFAULTS=""
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        IGNOREEXITCODE="N"
        LOGLEVEL="LOG_INFO"
        LOGSTDOUT="Y"
        NAME="dpf4c - Convert Microsoft Publisher to PostScript"
        PROJECT="DPF4CONVERT"
        Status="Running"
        THREADS="1"
        TYPE="WUSystemCall"
        VERSION="$Id: wu-pub2ps.xml,v 1.1 2008/06/02 14:43:47 dei Exp $"
        >
        <DEFAULTS
          DPF4C_COMMON_TIMEOUT="10"
          PUBCONV_PRINTER="SEAL Systems PS Office"
          />
        <INPUT>
          <DPF4C_COMMON_TIMEOUT
            DEFAULT="10"
            TYPE="TEXT"
            />
          <PUBCONV_PRINTER
            DEFAULT="SEAL Systems PS Office"
            TYPE="TEXT"
            />
        </INPUT>
      </pub2ps>
    </workingunits>
  </config>
</DPF>
