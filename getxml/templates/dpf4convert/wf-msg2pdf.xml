<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <msg2pdf
        COMMENT="dpf4convert - Convert process Microsoft Outlook MSG to PDF"
        DPFINT-Type="WorkflowDescription"
        LICENSE="CIMSO"
        NAME="DPF 4 Convert - Microsoft Outlook MSG to PDF"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-msg2pdf.xml,v 1.7 2017/06/13 11:28:06 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.msgstructureconvert
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="500"
          POSY="500"
          TYPE="SWF"
          USE="DPF4CONVERT.msgstructureconvert"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.msgstructureconvert>
        <END
          COMMENT=""
          NAME="END"
          POSX="1000"
          POSY="400"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_filetype_eml"
          ERROR="END"
          />
        <MAPPING/>
        <check_filetype_eml
          COMMENT=""
          ELSE="check_for_extract_message"
          OPERATOR="eq"
          POSX="100"
          POSY="100"
          PROPERTY="CURRFILETYPE"
          THEN="check_use_eml2files"
          TYPE="IF-THEN-ELSE"
          VALUE="eml"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_filetype_eml>
        <check_for_extract_message
          COMMENT=""
          ELSE="msg2ps"
          OPERATOR="is true"
          POSX="100"
          POSY="300"
          PROPERTY="MSG2PDF_EXTRACT_MSG_ATTACH"
          THEN="DPF4CONVERT.msgstructureconvert"
          TYPE="IF-THEN-ELSE"
          VALUE=""
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="HORIZONTAL"
            />
        </check_for_extract_message>
        <check_ps
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="700"
          POSY="300"
          PROPERTY="CURRFILETYPE"
          THEN="ps2pdfg"
          TYPE="IF-THEN-ELSE"
          VALUE="ps"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_ps>
        <check_use_eml2files
          COMMENT=""
          ELSE="msg2ps"
          OPERATOR="eq"
          POSX="300"
          POSY="100"
          PROPERTY="DPF4C_USE_EML2FILES"
          THEN="eml2files"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="HORIZONTAL"
            THEN="DIRECT"
            />
        </check_use_eml2files>
        <eml2files
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.eml2pdf"
          NEXT="END"
          POSX="500"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </eml2files>
        <msg2ps
          COMMENT=""
          ERROR="END"
          NAME="msg2ps"
          NEXT="check_ps"
          POSX="500"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </msg2ps>
        <ps2pdfg
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="900"
          POSY="300"
          TYPE="SWF"
          USE="ps2pdfg"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ps2pdfg>
      </msg2pdf>
    </workflows>
  </config>
</DPF>
