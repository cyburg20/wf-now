<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.Convertdocumentssubscale
        COMMENT=""
        DPFINT-Type="WorkflowDescription"
        NAME="Convertdocumentssubscale"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-convertdocumentssubscale.xml,v 1.6 2012/08/28 09:24:34 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.checkforscale
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.checkforscale"
          NEXT="check_for_scale"
          POSX="300"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.checkforscale>
        <DPF4CONVERT.prnconvSCALE
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.prnconv"
          NEXT="MODIFY1"
          POSX="900"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            AUTOROTATE="Y"
            PRINTERDIR="%DPFSRV%/conf/prnconv"
            SCALEFACTOR="1.0"
            SCALETYPE="original"
            />
          <MAPPING/>
        </DPF4CONVERT.prnconvSCALE>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="DPF4CONVERT.checkforscale"
          NAME=""
          />
        <MAPPING/>
        <MODIFY1
          COMMENT=""
          NEXT="restore_outputdevice"
          POSX="1100"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DEVICETYPE=""
            OUTPUTCONFIG=""
            OUTPUTDEVICE=""
            />
        </MODIFY1>
        <check_for_scale
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="300"
          POSY="500"
          PROPERTY="DO_PRNCONV"
          THEN="save_output_device"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_for_scale>
        <restore_outputdevice
          COMMENT=""
          NEXT="END"
          POSX="1300"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DEVICETYPE="%ORI_DEVICETYPE%"
            OUTPUTCONFIG="%ORI_OUTPUTCONFIG%"
            OUTPUTDEVICE="%ORI_OUTPUTDEVICE%"
            />
        </restore_outputdevice>
        <save_output_device
          COMMENT=""
          NEXT="set_pdf_outputdevice"
          POSX="500"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ORI_DEVICETYPE="%DEVICETYPE%"
            ORI_OUTPUTCONFIG="%OUTPUTCONFIG%"
            ORI_OUTPUTDEVICE="%OUTPUTDEVICE%"
            />
        </save_output_device>
        <set_pdf_outputdevice
          COMMENT=""
          NEXT="DPF4CONVERT.prnconvSCALE"
          POSX="700"
          POSY="500"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DEVICETYPE="pdf"
            OUTPUTCONFIG=""
            OUTPUTDEVICE=""
            />
        </set_pdf_outputdevice>
      </DPF4CONVERT.Convertdocumentssubscale>
    </workflows>
  </config>
</DPF>
