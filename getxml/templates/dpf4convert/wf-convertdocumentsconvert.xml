            ConvertDocumentsConvertRlicFilter2="Currfiletype"
            ConvertDocumentsConvertRlicFilter="CurrFileName"
            SubProcess="DPF4Convert_Convert2PDF"
            >
            <RlicCurrFileName
              val="CurrFileName"
              />
            <RlicJobName
              val="JobName"
              />
            <SubProcess
              val="Rli.processLinesHelper"
              />
          </DEFAULTS>
          <MAPPING
            FilterPrefix="ConvertDocumentsConvert"
            SubProcess="DPF4CONVERT.Convertdocumentssubconvert"
            />
        </convertFilesToPDF>
      </DPF4CONVERT.ConvertDocumentsConvert>
    </workflows>
  </config>
