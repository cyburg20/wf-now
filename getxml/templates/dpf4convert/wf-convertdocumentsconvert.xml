          NEXT="convertFilesToPDF"
          POSX="500"
          POSY="300"
          TYPE="SWF"
          USE="DPF4CONVERT.Convertdocuments_user_pre_convert"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.Convertdocuments_user_pre_convert>
        <DPF4CONVERT.arrangeduplex
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.arrangeduplex"
          NEXT="END"
          POSX="500"
          POSY="700"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.arrangeduplex>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
