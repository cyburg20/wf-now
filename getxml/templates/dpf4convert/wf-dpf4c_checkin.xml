<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <dpf4c_checkin
        COMMENT="dpf4convert - PROCESS FOR CHECKIN OF FILES DEPENDING ON PLM SYSTEM"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - SubProcess dpf4c_checkin"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-dpf4c_checkin.xml,v 1.18 2009/01/29 08:30:50 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="1100"
          POSY="200"
          TYPE="STOP"
          />
        <IF-THEN-ELSE1
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="1500"
          POSY="300"
          PROPERTY="DPF4C_SWITCH_DO_USER_POST_DPF4C_CHECKIN"
          THEN="dpf4c_user_post_checkin1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </IF-THEN-ELSE1>
        <MAIN
          BEGIN="check_switch_do_user_pre_dpf4c_checkin"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_dpf4c_checkin_plm_system
          CASE:gxnetplot="checkin_pfsclient"
          CASE:no_plm_system="checkin"
          CASE:sap="plm_checkin_files_sap"
          CASE:sap_direct="plm_checkin_files_sap_direct"
          COMMENT=""
          CONNECTION:DEFAULT="HORIZONTAL"
          CONNECTION:gxnetplot="HORIZONTAL"
          CONNECTION:no_plm_system="HORIZONTAL"
          CONNECTION:sap="HORIZONTAL"
          CONNECTION:sap_direct="DIRECT"
          DEFAULT="error_not_supported_plmsystemname"
          POSX="500"
          POSY="200"
          PROPERTY="DPF4C_COMMON_PLM_SYSTEM_NAME"
          TYPE="SWITCH"
          />
        <check_gate_copy
          COMMENT=""
          ELSE="sendback"
          OPERATOR="contains"
          POSX="900"
          POSY="800"
          PROPERTY="DPF4C_GATE_ASSIGNMENT"
          THEN="IF-THEN-ELSE1"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="HORIZONTAL"
            />
        </check_gate_copy>
        <check_switch_checkin
          COMMENT=""
          ELSE="IF-THEN-ELSE1"
          OPERATOR="contains"
          POSX="300"
          POSY="200"
          PROPERTY="DPF4C_SWITCH_CHECKIN"
          THEN="check_dpf4c_checkin_plm_system"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="HORIZONTAL"
            THEN="DIRECT"
            />
        </check_switch_checkin>
        <check_switch_do_user_pre_dpf4c_checkin
          COMMENT=""
          ELSE="check_switch_checkin"
          OPERATOR="contains"
          POSX="100"
          POSY="200"
          PROPERTY="DPF4C_SWITCH_DO_USER_PRE_DPF4C_CHECKIN"
          THEN="sp-user_pre_dpf4c_checkin"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="HORIZONTAL"
            />
        </check_switch_do_user_pre_dpf4c_checkin>
        <checkin
          COMMENT=""
          ERROR="END"
          NAME="checkin"
          NEXT="check_gate_copy"
          POSX="700"
          POSY="800"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </checkin>
        <checkin_pfsclient
          COMMENT=""
          ERROR="END"
          NAME="checkin_pfsclient"
          NEXT="IF-THEN-ELSE1"
          POSX="700"
          POSY="600"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </checkin_pfsclient>
        <dpf4c_user_post_checkin1
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="1700"
          POSY="300"
          TYPE="SWF"
          USE="dpf4c_user_post_checkin"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </dpf4c_user_post_checkin1>
        <error_not_supported_plmsystemname
          COMMENT=""
          NEXT="exit_process"
          POSX="700"
          POSY="1000"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ERR_REASON="Error: This dpf parameter value for DPF4C_COMMON_PLM_SYSTEM_NAME -%DPF4C_COMMON_PLM_SYSTEM_NAME%- is not supported for checkin!"
            ERR_WFNAME="DPF4C_CHECKIN"
            ERR_WFPOSITION="check_dpf4c_checkin_plm_system"
            />
        </error_not_supported_plmsystemname>
        <exit_process
          COMMENT=""
          ERROR="END"
          NAME="exit_process"
          NEXT="END"
          POSX="900"
          POSY="1000"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </exit_process>
        <plm_checkin_files_sap
          COMMENT=""
          ERROR="END"
          NAME="plm_checkin_files_sap"
          NEXT="IF-THEN-ELSE1"
          POSX="700"
          POSY="400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </plm_checkin_files_sap>
        <plm_checkin_files_sap_direct
          COMMENT=""
          ERROR="END"
          NAME="plm_checkin_files_sap_direct"
          NEXT="IF-THEN-ELSE1"
          POSX="700"
          POSY="200"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="HORIZONTAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </plm_checkin_files_sap_direct>
        <sendback
          COMMENT=""
          ERROR="END"
          NAME="sendback"
          NEXT="set_back_currfilename"
          POSX="1100"
          POSY="800"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sendback>
        <set_back_currfilename
          COMMENT=""
          NEXT="IF-THEN-ELSE1"
          POSX="1300"
          POSY="800"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="VERTICAL"
            />
          <SET
            CURRFILENAME="%SOURCE%"
            />
        </set_back_currfilename>
        <sp-user_pre_dpf4c_checkin
          COMMENT=""
          ERROR="END"
          NEXT="check_switch_checkin"
          POSX="200"
          POSY="400"
          TYPE="SWF"
          USE="dpf4c_user_pre_checkin"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="VERTICAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </sp-user_pre_dpf4c_checkin>
      </dpf4c_checkin>
    </workflows>
  </config>
</DPF>
