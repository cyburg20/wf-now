<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.MailService
        COMMENT=""
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Mail Service"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-mailservice.xml,v 1.5 2017/06/28 09:23:06 kig Exp $"
        >
        <DEFAULTS/>
        <DPF4CONVERT.convert_native1
          COMMENT=""
          ERROR="END"
          NEXT="preproc_already_executed"
          POSX="500"
          POSY="100"
          TYPE="SWF"
          USE="dpf4c_appkonv"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.convert_native1>
        <DPF4CONVERT.getfiletypefromname1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.getfiletypefromname"
          NEXT="MODIFY2"
          POSX="100"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.getfiletypefromname1>
        <DPF4CONVERT.pdfprocess1
          COMMENT=""
          ERROR="END"
          NEXT="Mail.sendMail1"
          POSX="900"
          POSY="100"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprocess"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfprocess1>
        <END
          COMMENT=""
          NAME="END"
          POSX="200"
          POSY="100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="DPF4CONVERT.getfiletypefromname1"
          NAME=""
          />
        <MAPPING/>
        <MODIFY2
          COMMENT=""
          NEXT="DPF4CONVERT.convert_native1"
          POSX="300"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPF4C_SWITCH_APPKONV="Y"
            />
        </MODIFY2>
        <Mail.sendMail1
          COMMENT=""
          ERROR="END"
          NAME="Mail.sendMail"
          NEXT="END"
          POSX="1200"
          POSY="100"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS
            MAIL_FROM="%DpfMailService_MessageReceivedByAddr%"
            MAIL_SMTP="%DpfMailService_MailSendHost%"
            MAIL_SUBJECT="Re: %DpfMailService_MessageSubject%"
            MAIL_TEXT="%DpfMailService_MessageBody%"
            MAIL_TO="%DpfMailService_MessageFromAddr%"
            >
            <WU_ENABLE_DEBUG_LOOP
              val="N"
              />
          </DEFAULTS>
          <MAPPING
            MAIL_ATTACH_PROPERTIES="CURRFILENAME"
            />
        </Mail.sendMail1>
        <preproc_already_executed
          COMMENT=""
          NEXT="DPF4CONVERT.pdfprocess1"
          POSX="700"
          POSY="100"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ENABLE_PDFPREPROC="N"
            />
        </preproc_already_executed>
      </DPF4CONVERT.MailService>
    </workflows>
  </config>
</DPF>
