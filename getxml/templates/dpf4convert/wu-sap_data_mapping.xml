<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <sap_data_mapping
        CODEPAGE="UTF8"
        COMMAND="%DPFSRV%/scripts/dpf4convert/sap_data_mapping.pl -parfile %PARFILENAME%"
        DPFINT-Icon="doc://:/wu.gif"
        DPFINT-Type="StationDescription"
        LOGLEVEL="LOG_INFO"
        LOGNAME="sapconvert-sap_data_mapping"
        LOGSTDOUT="Y"
        NAME="SAP Convert- SAP data mapping."
        NODENAMES=""
        PROJECT="DPF4CONVERT"
        THREADS="1"
        TIMEOUT=""
        TYPE="WUSystemCall"
        VERSION="$Id: wu-sap_data_mapping.xml,v 1.3 2012/11/15 12:24:17 kig Exp $"
        >
        <DEFAULTS
          SAP_MESSAGE="default notification from DPF"
          SAP_NOTIFY_LEVEL="I"
          SWITCH_COPY_CLASS_DATA_TO_USER_DATA_PROPERTIES="N"
          SWITCH_MAP_CLASSDATATXT_TO_DPF_PARAMETERS="N"
          SWITCH_MAP_DOCUMENTDATATXT_TO_DPF_PARAMETERS="N"
          SWITCH_MAP_ECMDATATXT_TO_DPF_PARAMETERS="N"
          SWITCH_MAP_EXPORTDATATXT_TO_DPF_PARAMETERS="N"
          SWITCH_MAP_LONGTEXTSTXT_TO_DPF_PARAMETERS="N"
          SWITCH_MAP_OBJECTLINKSTXT_TO_DPF_PARAMETERS="N"
          USERDATA_SECTIONS_TO_SKIP=""
          />
        <INPUT
          PLM_DATA_MAPPING_FORMAT_STRING_OBJECTLINKS="string"
          SWITCH_COPY_CLASS_DATA_TO_USER_DATA_PROPERTIES="Y|!N!"
          SWITCH_MAP_CLASSDATATXT_TO_DPF_PARAMETERS="Y|!N!"
          SWITCH_MAP_DOCUMENTDATATXT_TO_DPF_PARAMETERS="Y|!N!"
          SWITCH_MAP_ECMDATATXT_TO_DPF_PARAMETERS="Y|!N!"
          SWITCH_MAP_EXPORTDATATXT_TO_DPF_PARAMETERS="Y|!N!"
          SWITCH_MAP_LONGTEXTSTXT_TO_DPF_PARAMETERS="Y|!N!"
          SWITCH_MAP_OBJECTLINKSTXT_TO_DPF_PARAMETERS="Y|!N!"
          USERDATA_SECTIONS_TO_SKIP="string"
          />
        <OUTPUT
          CURRFILENAME="*"
          />
      </sap_data_mapping>
    </workingunits>
  </config>
</DPF>
