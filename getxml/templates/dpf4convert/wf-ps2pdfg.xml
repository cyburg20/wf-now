<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <ps2pdfg
        COMMENT="dpf4convert - Convert process PostScript to PDF"
        DPFINT-Type="WorkflowDescription"
        NAME="DPF 4 Convert - PostScript to PDF optional with PDF/A options."
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-ps2pdfg.xml,v 1.7 2012/10/19 15:24:14 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="200"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_quality"
          ERROR="END"
          />
        <MAPPING/>
        <check_quality
          COMMENT=""
          ELSE="ps2pdfg"
          OPERATOR="contains"
          POSX="300"
          POSY="100"
          PROPERTY="DESTINATION_FORMAT_QUALITY"
          THEN="set_options_for_pdfa"
          TYPE="IF-THEN-ELSE"
          VALUE="pdfa"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_quality>
        <ps2pdfg
          COMMENT=""
          ERROR="END"
          NAME="ps2pdfg"
          NEXT="END"
          POSX="300"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </ps2pdfg>
        <set_options_for_pdfa
          COMMENT=""
          NEXT="ps2pdfg"
          POSX="500"
          POSY="200"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            PS2PDF_PDF_SETTING="%DESTINATION_FORMAT_QUALITY%"
            />
        </set_options_for_pdfa>
      </ps2pdfg>
    </workflows>
  </config>
</DPF>
