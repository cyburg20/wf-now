<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.tiffpreproc
        COMMENT="The DPF-process TIFFPREPROC performs a pre-processing of a TIFF-file."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="Tiffpreproc - Preprocess a TIFF file"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-tiffpreproc.xml,v 1.2 2015/02/06 10:33:32 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="900"
          POSY="1600"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="check_enable_preproc"
          NAME=""
          />
        <MAPPING/>
        <check_enable_preproc
          COMMENT=""
          ELSE="END"
          OPERATOR="eq"
          POSX="200"
          POSY="100"
          PROPERTY="ENABLE_TIFFPREPROC"
          THEN="check_user_preproc"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_enable_preproc>
        <check_user_preproc
          COMMENT=""
          ELSE="tiff_preproc"
          OPERATOR="eq"
          POSX="300"
          POSY="300"
          PROPERTY="DPF4C_SWITCH_USER_PRE_TIFFPREPROC"
          THEN="dpf4c_user_pre_tiffpreproc"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_user_preproc>
        <dpf4c_user_pre_tiffpreproc
          COMMENT=""
          ERROR="END"
          NEXT="tiff_preproc"
          POSX="500"
          POSY="400"
          TYPE="SWF"
          USE="DPF4CONVERT.dpf4c_user_pre_tiffpreproc"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </dpf4c_user_pre_tiffpreproc>
        <tiff_preproc
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.tiff_preproc"
          NEXT="END"
          POSX="300"
          POSY="500"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </tiff_preproc>
      </DPF4CONVERT.tiffpreproc>
    </workflows>
  </config>
</DPF>
