<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.pdf_to_file_work
        COMMENT="dpf4convert - SUBPROCESS WORK ON PDF FILE
This subprocess is used by process pdf_to_file"
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="PDF to File Convert - Subprocess to convert PDF to output format"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-pdf_to_file_work.xml,v 1.1 2014/01/09 13:43:32 kig Exp $"
        >
        <DEFAULTS
          PDF_TO_FILE_USER_PRE_CONVERT="DPF4CONVERT.pdf_to_file_user_pre_convert"
          PDF_TO_FILE_USER_PRE_SENDBACK="DPF4CONVERT.pdf_to_file_user_pre_sendback"
          />
        <DPF4CONVERT.exit_process1
          COMMENT=""
          ERROR="END"
          NAME="exit_process"
          NEXT="END"
          POSX="200"
          POSY="1600"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.exit_process1>
        <DPF4CONVERT.pdf2jpg1
          COMMENT=""
          ERROR="END"
          NAME="pdf2jpg"
          NEXT="progress_75"
          POSX="400"
          POSY="1400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdf2jpg1>
        <DPF4CONVERT.pdf_to_file_finish1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.pdf_to_file_finish"
          NEXT="DPF4CONVERT.pdf_to_file_user_pre_sendback1"
          POSX="400"
          POSY="1800"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdf_to_file_finish1>
        <DPF4CONVERT.pdf_to_file_user_pre_convert1
          COMMENT=""
          ERROR="END"
          NEXT="check_destformat"
          POSX="300"
          POSY="1000"
          TYPE="USER_EXIT"
          USE="%PDF_TO_FILE_USER_PRE_CONVERT%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdf_to_file_user_pre_convert1>
        <DPF4CONVERT.pdf_to_file_user_pre_sendback1
          COMMENT=""
          ERROR="END"
          NEXT="check_use_sendback"
          POSX="400"
          POSY="2000"
          TYPE="USER_EXIT"
          USE="%PDF_TO_FILE_USER_PRE_SENDBACK%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdf_to_file_user_pre_sendback1>
        <DPF4CONVERT.pdfpostproc
          COMMENT=""
          ERROR="END"
          NEXT="progress_75"
          POSX="600"
          POSY="1500"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfpostproc"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="HORIZONTAL"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfpostproc>
        <DPF4CONVERT.pdfpreproc1
          COMMENT=""
          ERROR="END"
          NEXT="progress_25"
          POSX="300"
          POSY="200"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfpreproc"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfpreproc1>
        <DPF4CONVERT.pdfprocess1
          COMMENT=""
          ERROR="END"
          NEXT="progress_50"
          POSX="300"
          POSY="600"
          TYPE="SWF"
          USE="DPF4CONVERT.pdfprocess"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </DPF4CONVERT.pdfprocess1>
        <END
          COMMENT=""
          NAME="END"
          POSX="900"
          POSY="2400"
          TYPE="STOP"
          />
        <FileTransfer.sendback1
          COMMENT=""
          ERROR="END"
          NAME="sendback"
          NEXT="END"
          POSX="400"
          POSY="2400"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS>
            <IOR
              val="%ORIGFILESERVERIOR%"
              />
            <OVERWRITE
              val="Y"
              />
            <SOURCE
              val="%CURRFILENAME%"
              />
            <TARGETFILE
              val="%TARGETFILENAME%"
              />
          </DEFAULTS>
          <MAPPING/>
        </FileTransfer.sendback1>
        <MAIN
          BEGIN="DPF4CONVERT.pdfpreproc1"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <check_destformat
          CASE:jpg="DPF4CONVERT.pdf2jpg1"
          CASE:pdf="DPF4CONVERT.pdfpostproc"
          COMMENT=""
          CONNECTION:DEFAULT="VERTICAL"
          CONNECTION:jpg="VERTICAL"
          CONNECTION:pdf="VERTICAL"
          DEFAULT="unsupported_destformat"
          POSX="300"
          POSY="1200"
          PROPERTY="DPF4C_COMMON_DEST_FORMAT"
          TYPE="SWITCH"
          />
        <check_use_sendback
          COMMENT=""
          ELSE="FileTransfer.sendback1"
          OPERATOR="eq"
          POSX="400"
          POSY="2200"
          PROPERTY="PDF_TO_FILE_SKIP_SENDBACK"
          THEN="END"
          TYPE="IF-THEN-ELSE"
          VALUE="Y"
          >
          <CONNECTIONS
            ELSE="DIRECT"
            THEN="DIRECT"
            />
        </check_use_sendback>
        <progress_25
          COMMENT=""
          NEXT="DPF4CONVERT.pdfprocess1"
          POSX="300"
          POSY="400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="25"
            />
        </progress_25>
        <progress_50
          COMMENT=""
          NEXT="DPF4CONVERT.pdf_to_file_user_pre_convert1"
          POSX="300"
          POSY="800"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="50"
            />
        </progress_50>
        <progress_75
          COMMENT=""
          NEXT="DPF4CONVERT.pdf_to_file_finish1"
          POSX="400"
          POSY="1600"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            DPFJOBPROGRESS="75"
            />
        </progress_75>
        <unsupported_destformat
          COMMENT=""
          NEXT="DPF4CONVERT.exit_process1"
          POSX="200"
          POSY="1400"
          TYPE="MODIFY"
          >
          <CONNECTIONS
            NEXT="DIRECT"
            />
          <SET
            ERR_REASON="ERROR: This DPF4C_COMMON_DEST_FORMAT -%DPF4C_COMMON_DEST_FORMAT%- is not supported!"
            ERR_WFNAME="pdf_to_file"
            ERR_WFPOSITION="unsupported_destformat"
            />
        </unsupported_destformat>
      </DPF4CONVERT.pdf_to_file_work>
    </workflows>
  </config>
</DPF>
