<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workingunits>
      <pdfcheckmerge
        NAME="dpf4c - Check PDF file with pdfmerge"
        VERSION="$Id: wu-pdfcheckmerge.xml,v 1.1 2005/10/12 12:00:05 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        TYPE="WUSystemCall"
        LOGNAME="dpf4c-pdfcheckmerge"
        LOGSTDOUT="Y"
        LOGLEVEL="LOG_INFO"
        THREADS="1"
        COMMAND="%DPFSRV%/scripts/dpf4convert/pdfcheckmerge.pl -parfile %PARFILENAME%">
        <DEFAULTS
          WU_ENABLE_DEBUG_LOOP="N"
          />
        <INPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
        <OUTPUT
          CURRFILENAME="*"
          CURRFILETYPE="pdf"
          />
      </pdfcheckmerge>
    </workingunits>
  </config>
</DPF>
