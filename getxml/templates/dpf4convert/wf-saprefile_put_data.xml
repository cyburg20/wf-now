<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <DPF4CONVERT.saprefile_put_data
        COMMENT="SAP Refile - SUBPROCESS SAP REFILE PUT DATA

This subprocess is used from the main SAP processes for refiling to send back files to SAP."
        DPFINT-Customer="0"
        DPFINT-Interactive="0"
        DPFINT-Type="WorkflowDescription"
        NAME="SAP Refile - SubProcess SAPRefile Put Data"
        PROJECT="DPF4CONVERT"
        VERSION="$Id: wf-saprefile_put_data.xml,v 1.1 2012/05/25 14:31:55 kig Exp $"
        >
        <DEFAULTS/>
        <END
          COMMENT=""
          NAME="END"
          POSX="700"
          POSY="1100"
          TYPE="STOP"
          />
        <MAIN
          BEGIN="dpf4c_user_pre_saprefile_put_files1"
          ERROR="END"
          LOGLEVEL="I"
          />
        <MAPPING/>
        <dpf4c_user_pre_saprefile_put_files1
          COMMENT=""
          ERROR="END"
          NEXT="saprefile_put_files1"
          POSX="200"
          POSY="100"
          TYPE="SWF"
          USE="%SAPREFILE_USER_PRE_PUT_FILES%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </dpf4c_user_pre_saprefile_put_files1>
        <saprefile_put_files1
          COMMENT=""
          ERROR="END"
          NAME="DPF4CONVERT.saprefile_put_files"
          NEXT="saprefile_user_post_put_files"
          POSX="200"
          POSY="300"
          TYPE="WU"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </saprefile_put_files1>
        <saprefile_user_post_put_files
          COMMENT=""
          ERROR="END"
          NEXT="END"
          POSX="200"
          POSY="500"
          TYPE="SWF"
          USE="%SAPREFILE_USER_POST_PUT_FILES%"
          >
          <CONNECTIONS
            ERROR="DIRECT"
            NEXT="DIRECT"
            />
          <DEFAULTS/>
          <MAPPING/>
        </saprefile_user_post_put_files>
      </DPF4CONVERT.saprefile_put_data>
    </workflows>
  </config>
</DPF>
