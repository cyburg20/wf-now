<?xml version="1.0" encoding="ISO-8859-1"?>
<DPF>
  <config>
    <workflows>
      <!-- MAIN PROCESS DPF4CONVERT -->  
      <dpf4convert
        NAME="DPF 4 Convert - Main Process dpf4convert"
        VERSION="$Id: wf-dpf4convert.xml,v 1.11 2006/05/12 14:06:02 ludwig Exp $"
        PROJECT="DPF4CONVERT"
        COMMENT="dpf4convert - MAIN PROCESS FOR DPF4CONVERT"
        DPFINT-Type="WorkflowDescription"
        DPFINT-Changes="0"
        DPFINT-Interactive="1"
        >
        <UserInterface
          ORDER="DPF4C_COMMON_DEST_FORMAT DPF4C_MODIFY_ROTATION DPF4C_MODIFY_COMPRESSION DPF4C_SCALE_FACTOR DPF4C_MODIFY_RESOLUTION_DPI DPF4C_MODIFY_STAMP_N_TEXT0 DPF4C_MODIFY_STAMP_N_TEXT1 DPF4C_MODIFY_STAMP_N_TEXT2 DPF4C_MODIFY_STAMP_N_TEXT3 DPF4C_MODIFY_STAMP_N_TEXT4 DPF4C_MODIFY_STAMP_N_TEXT5 DPF4C_MODIFY_STAMP_N_TEXT6 DPF4C_MODIFY_STAMP_N_TEXT7 DPF4C_MODIFY_STAMP_N_TEXT8 DPF4C_MODIFY_STAMP_N_TEXT9"
          >
          <DPF4C_COMMON_DEST_FORMAT
            RANGE="PDF|PDF,TIFF|PDF,CLS|PDF,TIFF,CLS|TIFF|TIFF,CLS|CLS|"
            TYPE="Combo"
            DEFAULT="PDF"
            />
          <DPF4C_MODIFY_ROTATION
            RANGE="0|90|180|270|"
            TYPE="Opt"
            DEFAULT="0"
            />
          <DPF4C_MODIFY_COMPRESSION
            RANGE="Y|N|"
            TYPE="Check"
            DEFAULT="Y"
            />
          <DPF4C_SCALE_FACTOR
            RANGE="10"
            TYPE="Edit"
            DEFAULT="1.0"
            />
          <DPF4C_MODIFY_RESOLUTION_DPI
            RANGE="5"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT0
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT1
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT2
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT3
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT4
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT5
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT6
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT7
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT8
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_MODIFY_STAMP_N_TEXT9
            RANGE="200"
            TYPE="Edit"
            DEFAULT=""
            />
          <DPF4C_SWITCH_PLM_DATA_MAPPING
            RANGE="59"
            TYPE="Hidden"
            DEFAULT="N"
            />
          <DPF4C_SWITCH_CHECKOUT_FILES
            RANGE="59"
            TYPE="Hidden"
            DEFAULT="N"
            />
          <DPF4C_SWITCH_APPKONV
            RANGE="59"
            TYPE="Hidden"
            DEFAULT="Y"
            />
          <DPF4C_SWITCH_MODIFY
            RANGE="59"
            TYPE="Hidden"
            DEFAULT="Y"
            />
          <DPF4C_SWITCH_CREATE_DESTFORMATS
            RANGE="59"
            TYPE="Hidden"
            DEFAULT="Y"
            />
          <DPF4C_SWITCH_CHECKIN 
            RANGE="59"
            TYPE="Hidden"
            DEFAULT="Y"
            />
          <DPF4C_COMMON_PLM_SYSTEM_NAME
            RANGE="59"
            TYPE="Hidden"
            DEFAULT="no_plm_system"
            />
        </UserInterface>
        <MAIN
          BEGIN="sp-version_dpf4c"
          ERROR="sp-dpf4c_error"
          LOGLEVEL="I"     
          />
        <sp-version_dpf4c
          USE="version_dpf4c"
          NEXT="sp-read_ini_file"
          />
        <sp-read_ini_file
          NAME="dpf4c_read_ini_file"
          NEXT="sp-plm_data_mapping"
          />
        <sp-plm_data_mapping
          USE="dpf4c_plm_data_mapping"
          NEXT="sp-checkout_files"
          />
        <sp-checkout_files
          USE="dpf4c_checkout_files"
          NEXT="sp-appkonv"
          />
        <sp-appkonv
          USE="dpf4c_appkonv"
          NEXT="sp-modify"
          />
        <sp-modify
          USE="dpf4c_modify"
          NEXT="sp-create_destformats"
          />
        <sp-create_destformats
          USE="dpf4c_create_destformats"
          NEXT="sp-checkin"
          />
        <sp-checkin
          USE="dpf4c_checkin"
          NEXT="END"
          />
        <sp-dpf4c_error
          USE="dpf4c_error"
          NEXT="END"
          />
      </dpf4convert>
    </workflows>
  </config>
</DPF>
